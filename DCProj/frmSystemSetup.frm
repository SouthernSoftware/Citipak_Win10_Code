Attribute VB_Name = "frmSystemSetup"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Dim Over As clsTextBoxOverRider
Private Temp_Class As Resize_Class

Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    If MsgBox("Are You Sure You Wish To Close Program?", vbYesNo, "Close?") = vbNo Then
      Cancel = True
    Else
      ClearInUse PWcnt
    End If
  End If
End Sub
Private Sub cmdSave_Click()
  Dim goahead As Integer
  goahead = 0
  If Len(fptxtTownName) <> 0 Then
    goahead = 1
  End If
  If goahead > 0 Then
      SaveCodes
      MsgBox "Saved.", vbOKOnly, "Procedure Complete"
      frmDCSetupMenu.Show
      Unload Me
  End If
End Sub


Private Sub Form_Resize()
  If Me.WindowState <> vbMinimized Then
    'Me.Visible = False
    Temp_Class.ResizeControls Me
    Me.Visible = True
    Me.SetFocus
  End If
End Sub
Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyUp:
      SendKeys "+{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%X"
      KeyCode = 0
    Case vbKeyF10:
      SendKeys "%S"
      KeyCode = 0
    Case Else:
  End Select
End Sub

Private Sub Form_Load()
  Dim cnt As Integer, DCSetuplen As Integer
  Set Temp_Class = New Resize_Class
  Temp_Class.InitResizeClass Me
  Set Over = New clsTextBoxOverRider
  Over.OverRide Me
  StatusBar1.Panels.Item(1).Text = TOWNNAME$
  Me.HelpContextID = hlpSystemDefault
  fpcboGLInterface.AddItem "Yes"
  fpcboGLInterface.AddItem "No"
  fpcboTaxes.AddItem "Yes"
  fpcboTaxes.AddItem "No"
  fpcboApps.InsertRow = "1" & Chr$(9) & "Letter Application - Laser"
  fpcboApps.InsertRow = "2" & Chr$(9) & "Letter Application - Text"
  fpcboLookUp.InsertRow = "1" & Chr$(9) & "Account Number"
  fpcboLookUp.InsertRow = "2" & Chr$(9) & "Search Name"
  fpcboLookUp.InsertRow = "3" & Chr$(9) & "SocSec Number"
  If Exist(DCPath$ + "DCSetup.DAT") Then
    ReDim DCSetup(1) As DCSetupType
    'DCSetuplen = Len(DCSetup(1))
    LoadDCSetUpFile DCSetup(), DCSetuplen
    fptxtTownName = QPTrim(DCSetup(1).DCTNNAME)
    If QPTrim(DCSetup(1).GLInterface) = "Y" Then
      fpcboGLInterface.ListIndex = 0
    Else
      fpcboGLInterface.ListIndex = 1
    End If
    If DCSetup(1).AppType = 2 Then
      fpcboApps.ListIndex = 1
    Else
      fpcboApps.ListIndex = 0
    End If
    If QPTrim(DCSetup(1).Taxbalchk) = "Y" Then
      fpcboTaxes.ListIndex = 0
    Else
      fpcboTaxes.ListIndex = 1
    End If
    fpcboLookUp.ListIndex = Val(DCSetup(1).DefLook) - 1
    LabelVers.Caption = "Version - " + DCSetup(1).DCVers
  End If
  
End Sub
Private Sub cmdExit_Click()
  Load frmDCSetupMenu
  DoEvents
  frmDCSetupMenu.Show
  Unload Me
End Sub

Private Sub SaveCodes()
  Dim DCSetup As DCSetupType
  Dim DCSetupRecLen As Integer, Handle As Integer, cnt As Integer
  DCSetupRecLen = Len(DCSetup)
  Handle = FreeFile
  Open DCPath$ + "DCSetup.DAT" For Random Shared As Handle Len = DCSetupRecLen
   Get #Handle, 1, DCSetup
   DCSetup.DCTNNAME = QPTrim$(fptxtTownName.Text)
   If fpcboGLInterface.ListIndex = 0 Then
     DCSetup.GLInterface = "Y"
   Else
     DCSetup.GLInterface = "N"
   End If
   DCSetup.AppType = fpcboApps.ListIndex + 1
   DCSetup.DCVers = "205"
   If fpcboTaxes.ListIndex = 0 Then
     DCSetup.Taxbalchk = "Y"
   Else
     DCSetup.Taxbalchk = "N"
   End If
   fpcboLookUp.col = 0
   LSet DCSetup.DefLook = QPTrim$(fpcboLookUp.ColText)

   DCSetup.Filler = ""
   Put #Handle, 1, DCSetup
  Close
'  'mainlog updates log file if used save command or saved on exit
  DCLog "Setup Saved."
'  Close AcctFileNum
End Sub
Private Sub fptxtTownName_ChangeMode(EditMode As Integer)
  EditMode = True
End Sub
Private Sub fptxtTownName_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeyDown Or KeyCode = vbKeyReturn Then
    fpcboGLInterface.SetFocus
    KeyCode = 0
  Else
    If KeyCode = vbKeyUp Then
      cmdExit.SetFocus
    End If
  End If
End Sub

Private Sub fpcboGLInterface_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeySpace Then
    fpcboGLInterface.ListDown = True
  End If
  If fpcboGLInterface.ListDown <> True Then
    If KeyCode = vbKeyDown Or KeyCode = vbKeyReturn Then
      fpcboApps.SetFocus
      KeyCode = 0
    Else
      If KeyCode = vbKeyUp Then
        fptxtTownName.SetFocus
      End If
    End If
  End If
End Sub

Private Sub fpcboApps_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeySpace Then
    fpcboApps.ListDown = True
  End If
  If fpcboApps.ListDown <> True Then
    If KeyCode = vbKeyDown Or KeyCode = vbKeyReturn Then
      fpcboTaxes.SetFocus
      KeyCode = 0
    Else
      If KeyCode = vbKeyUp Then
        fpcboGLInterface.SetFocus
        KeyCode = 0
      End If
    End If
  End If
End Sub


Private Sub fpcboTaxes_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeySpace Then
    fpcboTaxes.ListDown = True
  End If
  If fpcboTaxes.ListDown <> True Then
    If KeyCode = vbKeyDown Or KeyCode = vbKeyReturn Then
      fpcboLookUp.SetFocus
      KeyCode = 0
    Else
      If KeyCode = vbKeyUp Then
        fpcboApps.SetFocus
        KeyCode = 0
      End If
    End If
  End If
   
End Sub

Private Sub fpcboLookUp_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeySpace Then
    fpcboLookUp.ListDown = True
  End If
  If fpcboLookUp.ListDown <> True Then
    If KeyCode = vbKeyDown Or KeyCode = vbKeyReturn Then
      cmdSave.SetFocus
      KeyCode = 0
    Else
      If KeyCode = vbKeyUp Then
        fpcboTaxes.SetFocus
        KeyCode = 0
      End If
    End If
  End If
   
End Sub

