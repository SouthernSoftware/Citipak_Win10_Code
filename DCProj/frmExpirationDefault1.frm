Private Temp_Class As Resize_Class
Dim Over As clsTextBoxOverRider
Private Sub cmdExit_Click()
  frmDCPurchaseMenu.Show
  Unload Me
End Sub
Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    If cmdExit.Enabled = False Then
      Cancel = True
    Else
      If MsgBox("Are You Sure You Wish To Close The Program?", vbYesNo, "Close?") = vbNo Then
        Cancel = True
      Else
        DCLog "Closed via defaultexpiration  by " + PWUser$
        CitiTerminate
      End If
    End If
  End If
End Sub

Private Sub mnuExit_Click()
  cmdExit_Click
End Sub

Private Sub mnuPrnScn_Click()
  PrintForm
End Sub

Private Sub txtExpDate_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeyReturn Then
    cmdSave.SetFocus
  End If
End Sub

Private Function ValidDate()
  If CheckValDate(txtExpDate) = False Then
    MsgBox "Date Is Not Valid. Please Correct.", vbOKOnly, "Invalid Date"
    ValidDate = False
  Else
    ValidDate = True
  End If
End Function

Private Sub cmdSave_Click()
  Dim DCFile As Integer
  Dim DCDate(1) As DCExpireDate
  If ValidDate Then
    DCFile = FreeFile
    Open "DCEXPIRE.DAT" For Random Access Read Write Shared As DCFile Len = 10
    DCDate(1).ExpireDate = txtExpDate.Text
    Put DCFile, 1, DCDate(1)
    Close
    DCLog PWUser + " Saved ExpirationDefault- " + txtExpDate.Text
    MsgBox "Expiration Default Date Saved", vbOKOnly, "Saved"
    cmdExit_Click
  End If
End Sub
Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
'    Case vbKeyDown, vbKeyReturn:
'      SendKeys "{Tab}"
'      KeyCode = 0
'    Case vbKeyUp:
'      SendKeys "+{Tab}"
'      KeyCode = 0
    Case vbKeyEscape:
      cmdExit_Click
      KeyCode = 0
    Case vbKeyF10:
      cmdSave_Click
      KeyCode = 0
    Case Else:
  End Select
End Sub

Private Sub Form_Load()
  Set Temp_Class = New Resize_Class
  Temp_Class.InitResizeClass Me
  Set Over = New clsTextBoxOverRider
  Over.OverRide Me
  StatusBar1.Panels.Item(1).Text = TOWNNAME$
  Me.HelpContextID = hlpSetDefault
  Dim DCFile As Integer
  Dim DCDate(1) As DCExpireDate
  DCFile = FreeFile
  Open "DCEXPIRE.DAT" For Random Access Read Write Shared As DCFile Len = 10
  Get #DCFile, 1, DCDate(1)
  txtExpDate.Text = DCDate(1).ExpireDate
  Close
End Sub

