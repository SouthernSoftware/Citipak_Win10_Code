Private Sub SaveLetter()
Dim LaLe As Integer, lenlate As Integer, cnt As Integer
ReDim APPLet(1) As ApplicationDefaultsType
LaLe = FreeFile
lenlate = Len(APPLet(1))
Open DCPath$ + "DCApplet.dat" For Random Shared As LaLe Len = lenlate
  APPLet(1).Head1 = RTrim(fpHead1.Text)
  APPLet(1).Head2 = RTrim(fpHead2.Text)
  APPLet(1).Head3 = RTrim(fpHead3.Text)
  APPLet(1).Head4 = RTrim(fpHead4.Text)
  APPLet(1).Head5 = RTrim(fpHead5.Text)
  For cnt = 0 To 19
    APPLet(1).Body(cnt + 1) = RTrim(fptxtBody(cnt))
  Next
  If fpcboLogo.ListIndex = 1 Then
    APPLet(1).DoLogo = 1
  Else
    APPLet(1).DoLogo = 0
  End If
Put LaLe, 1, APPLet(1)
Close
  MsgBox "Application Letter has been saved.", vbOKOnly, "Save Complete"
  DCLog PWUser$ + "Application Letter Saved"
End Sub
Private Sub loadLetter()
  Dim LaLe As Integer, lenlate As Integer, cnt As Integer
  If Exist(DCPath$ + "DCApplet.dat") Then
    ReDim APPLet(1) As ApplicationDefaultsType
    LaLe = FreeFile
    lenlate = Len(APPLet(1))
    Open DCPath$ + "DCApplet.dat" For Random Shared As LaLe Len = lenlate
    Get LaLe, 1, APPLet(1)
    fpHead1.Text = RTrim(APPLet(1).Head1)
    fpHead2.Text = RTrim(APPLet(1).Head2)
    fpHead3.Text = RTrim(APPLet(1).Head3)
    fpHead4.Text = RTrim(APPLet(1).Head4)
    fpHead5.Text = RTrim(APPLet(1).Head5)
    For cnt = 0 To 19
      fptxtBody(cnt) = RTrim(APPLet(1).Body(cnt + 1))
    Next
    If APPLet(1).DoLogo = 1 Then
      fpcboLogo.ListIndex = 1
    Else
      fpcboLogo.ListIndex = 0
    End If
    Close
  Else
    fpHead1 = TOWNNAME$
    fpHead2 = ""
    fpHead3 = ""
    fpHead4 = ""
    fpHead5 = ""
  End If
End Sub
