'  If GetOKorNot(MsgText()) Then
'    UBLog "USER WANTS TO CONTINUE!"
'  Else
'    UBLog "USER ABORTED."
'    Exit Sub
'  End If
  If MsgBox("Would you like to save changes before you exit?", vbYesNo, "Save?") = vbYes Then
    cmdSave_Click
  End If
End If
  Load frmDCSetupMenu
  DoEvents
  frmDCSetupMenu.Show
  Unload Me
End Sub

