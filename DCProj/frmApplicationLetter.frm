Private Sub cmdExit_Click()
'Dim FntSize As Integer
Dim LaLe As Integer, lenlate As Integer, cnt As Integer, Chg As Integer
ReDim APPLet(1) As ApplicationDefaultsType
LaLe = FreeFile
Chg = 0
lenlate = Len(APPLet(1))
If Exist(DCPath$ + "DCApplet.dat") Then
  Open DCPath$ + "DCApplet.dat" For Random Shared As LaLe Len = lenlate
  Get LaLe, 1, APPLet(1)
  If Not RTrim(APPLet(1).Head1) = RTrim(fpHead1.Text) Then Chg = Chg + 1
  If Not RTrim(APPLet(1).Head2) = RTrim(fpHead2.Text) Then Chg = Chg + 1
  If Not RTrim(APPLet(1).Head3) = RTrim(fpHead3.Text) Then Chg = Chg + 1
  If Not RTrim(APPLet(1).Head4) = RTrim(fpHead4.Text) Then Chg = Chg + 1
  If Not RTrim(APPLet(1).Head5) = RTrim(fpHead5.Text) Then Chg = Chg + 1
  For cnt = 0 To 19
    If Not RTrim(APPLet(1).Body(cnt + 1)) = RTrim(fptxtBody(cnt)) Then Chg = Chg + 1
  Next
Close
Else
  If Not RTrim(fpHead1.Text) = "" Then Chg = Chg + 1
  If Not RTrim(fpHead2.Text) = "" Then Chg = Chg + 1
  If Not RTrim(fpHead3.Text) = "" Then Chg = Chg + 1
  If Not RTrim(fpHead4.Text) = "" Then Chg = Chg + 1
  If Not RTrim(fpHead5.Text) = "" Then Chg = Chg + 1
  For cnt = 0 To 19
    If Not RTrim(fptxtBody(cnt)) = "" Then Chg = Chg + 1
  Next
End If
If Chg > 0 Then
'  ReDim MsgText(0 To 5) As String
'  FntSize = frmMsgDialog.Label(1).FontSize
