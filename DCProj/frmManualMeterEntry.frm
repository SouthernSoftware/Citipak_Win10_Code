        UBCustRec(1).LocMeters(MtrCnt).UseCnt = 0
        Average# = 0
        AvgCnt! = 0
      End If
      fpAvgUse(MtrCnt - 1) = Using("##,###,###", Average#)
    Else
      fpMtrType1(MtrCnt - 1).Enabled = False
      fpMtrNum(MtrCnt - 1).Enabled = False
