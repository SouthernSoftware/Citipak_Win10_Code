  For MtrCnt = 1 To 7           'find last active meter
'
'    'inputting = False
    TempRev$ = QPTrim$(UBCustRec(1).LocMeters(MtrCnt).MtrType)
'
    If Len(TempRev$) <> 0 Then
      Rem set information as known
      ' = Right$(Str$(BookNumber), 2)

     ' GoSub GetMeterType
      fpMtrType1(MtrCnt - 1).Enabled = True
      fpMtrNum(MtrCnt - 1).Enabled = True
      txtDate(MtrCnt - 1).Enabled = True
      fpEstimated(MtrCnt - 1).Enabled = True
      fpCurrent(MtrCnt - 1).Enabled = True
      fpPrevious(MtrCnt - 1).Enabled = True
      fpAvgUse(MtrCnt - 1).Enabled = True

      fpMtrType1(MtrCnt - 1).Caption = UBCustRec(1).LocMeters(MtrCnt).MtrType 'MeterType$
      fpMtrNum(MtrCnt - 1).Caption = UBCustRec(1).LocMeters(MtrCnt).MtrNum
