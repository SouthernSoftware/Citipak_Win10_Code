Private Sub DoCalcUsage(Mtr As Integer)
  Dim NewRead As Double, PastRead As Double, Calcusage As Double
  Dim J As Double, Max As Double, AvgCnt As Single
  ' do the calculation here
  If fpCurrent(Mtr).Enabled = True Then
  NewRead# = Val(fpCurrent(Mtr))
  PastRead# = Val(fpPrevious(Mtr))
  If NewRead# >= PastRead# Then
    Calcusage# = NewRead# - PastRead#
  Else
    J# = Len(Str$(PastRead#)) - 1: Max# = 10 ^ J#
    Calcusage# = Max# - PastRead# + NewRead#
  End If
  fpUsage(Mtr) = Str$(Calcusage#)

  fpAvgUse(Mtr) = Using("##,###,###", Average#)
  If Calcusage# <> Average# Then
     fpEstimated(Mtr) = "NO"
  End If
  If Average# > 0 Then 'AvgCnt! > 0 And
    LabelMsg(Mtr).Visible = False
    If Calcusage# > (Average# * (HighVar / 100)) Then
      'HighRead = True
      LabelMsg(Mtr).Visible = True
      LabelMsg(Mtr).Caption = "<-HIGH"
      Beep
      'fpCurrent(mtr).SetFocus 'nono this won't allow if valid
    End If
    If Calcusage# < (Average# * (LowVar / 100)) Then
      LabelMsg(Mtr).Visible = True
      LabelMsg(Mtr).Caption = "<-LOW"
      'LowRead = True
      Beep
      'fpCurrent(mtr).SetFocus 'nono won't ever let off field unless change
    End If
  End If
  End If
  'if UBCustRec(1).EstFlag = "E"
  End Sub

