        txtDate(MtrCnt - 1) = Num2Date(UBCustRec(1).LocMeters(MtrCnt).CurDate)
        fpCurrent(MtrCnt - 1) = QPTrim$(Str$(UBCustRec(1).LocMeters(MtrCnt).CurRead))
        fpPrevious(MtrCnt - 1) = QPTrim$(Str$(UBCustRec(1).LocMeters(MtrCnt).PrevRead))
        If UBCustRec(1).EstFlag = "E" Then
          fpEstimated(MtrCnt - 1) = "YES"
        End If
      Else
        fpPrevious(MtrCnt - 1) = Str$(UBCustRec(1).LocMeters(MtrCnt).CurRead)
      End If

      If MeterType$ = "Demand Electric" Then
        fpPrevious(MtrCnt - 1) = "0"    ' Demand Always = 0 for Previous Read
      End If


      Average# = UBCustRec(1).LocMeters(MtrCnt).AvgUse
      AvgCnt! = UBCustRec(1).LocMeters(MtrCnt).UseCnt
      ' RESET AVERAGE IS OUT OF WHACK
