  ReDim UBCustRec(1) As NewUBCustRecType
  CurrChg = False
  UBCustRecLen = Len(UBCustRec(1))
  C2Handle = FreeFile
  Open UBPath$ + "UBCUST.DAT" For Random Shared As C2Handle Len = UBCustRecLen
  Get #C2Handle, RecNo&, UBCustRec(1)
  Close C2Handle
   For Xidx = 1 To 7
    If UBCustRec(1).LocMeters(Xidx).ReadFlag = "Y" Then
      If Val(fpCurrent(Xidx - 1)) <> UBCustRec(1).LocMeters(Xidx).CurRead Then
        CurrChg = True
        Exit For
      End If
    Else
      If Val(fpCurrent(Xidx - 1)) <> 0 Then
        CurrChg = True
        Exit For
      End If
    End If
  Next
End Function

Private Sub fpCmdEst_Click(Index As Integer)
  Dim OPrev As Double
  If fpCurrent(Index).Enabled = True Then
    OPrev# = Val(fpPrevious(Index))
    fpCurrent(Index) = QPTrim$(Str$(OPrev# + Average#))
    fpEstimated(Index) = "YES"
    DoCalcUsage Index
  End If

End Sub
Private Sub fpCmdEstAll_Click()
  Dim OPrev As Double, lcnt As Integer
  For lcnt = 0 To 6
  If fpCurrent(lcnt).Enabled = True Then
    OPrev# = Val(fpPrevious(lcnt))
    fpCurrent(lcnt) = QPTrim$(Str$(OPrev# + Average#))
    fpEstimated(lcnt) = "YES"
    DoCalcUsage lcnt
  End If
  Next
End Sub

