Private Sub ExitCustDelete()
  UBLog "OUT: DELETE Account."
  DoEvents
  Recno = 0
  BeenDone = False
  TransRec = 0
  If codeopt = 1 Then
    ActivateControls frmCustEditLookUP
  ElseIf codeopt = 2 Then
    ActivateControls frmDisplayList
  End If
  Unload frmCustDelete
End Sub

