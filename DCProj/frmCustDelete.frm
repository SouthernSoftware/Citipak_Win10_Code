  PrintForm
End Sub

Private Sub fpCmdDelete_Click()
  Dim LookFor As String
  If MsgBox("Are You Sure You Wish to Delete This Account?", vbYesNo, "Delete Account") = vbYes Then
    DeActivateControls Me
    DeleteCustomer
    ReIndexSystem False
  '  ******************************
    MsgBox "Account Deletion Complete", vbOKOnly, "Deleted"
    UBLog "OUT: DELETE Account."
    DoEvents
    Recno = 0
    BeenDone = False
    TransRec = 0
    ActivateControls Me
    If codeopt = 1 Then
      ActivateControls frmCustEditLookUP
    ElseIf codeopt = 2 Then
    'Have to do this to repopulate list without deleted acct
      Unload frmDisplayList
      LookFor$ = QPTrim$(frmCustEditLookUP.fpSearchText)
      Call frmCustEditLookUP.LookUpList(LookFor$, frmCustEditLookUP.DefLookUp, False, False, frmCustEditLookUP)
    End If
    Unload frmCustDelete
  End If
End Sub

Private Sub fpCmdExit_Click()
  ExitCustDelete
End Sub
Private Sub mnuExit_Click()
  ExitCustDelete
End Sub

