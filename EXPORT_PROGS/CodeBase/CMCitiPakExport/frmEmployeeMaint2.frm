Private Sub LoadEmpMnt2File()

   Dim EmpData2FileHandle As Integer
   Dim EmpData2FileRec As EmpData2Type
   Dim EmpRecLen As Long
   Dim DedCodeFileRec As DedCodeRecType
   Dim DedCodeFileHandle As Integer
   Dim DecRecLen As Integer, x As Integer, dedCnt As Integer
   Dim ThisState$
   Dim UnitHandle As Integer, UnitRec As UnitFileRecType
   
   OpenUnitFile UnitHandle
   Get UnitHandle, 1, UnitRec
   Close UnitHandle
   ThisState$ = UnitRec.UFSTATE
   
   OpenDedCodeFile DedCodeFileHandle
   DecRecLen = LOF(DedCodeFileHandle) / Len(DedCodeFileRec)
'   These fields in the miscellaneous spreadsheet will always
'   be loaded regardless if this is a new employee or an
'   existing one
   For x = 1 To DecRecLen
      Get DedCodeFileHandle, x, DedCodeFileRec
      vaSpreadMisc.Col = 1
      vaSpreadMisc.Row = x
      vaSpreadMisc.Text = QPTrim$(DedCodeFileRec.DCDESC1)
      vaSpreadMisc.Col = 3
      vaSpreadMisc.Row = x
      vaSpreadMisc.Text = QPTrim$(DedCodeFileRec.DCFWT1)
   Next x
   Close DedCodeFileHandle
   
   If frmEmployeeMaintMenu.Selection = neoOn Then
      newEmpFlag = True
   Else
       thisRecordNum = RecNum
       newEmpFlag = False
   End If
   
   fpcomboFedX.AddItem ("Y")
   fpcomboFedX.AddItem ("N")
   fpcomboFedAmtPct.AddItem ("A")
   fpcomboFedAmtPct.AddItem ("P")
   fpcomboFedStatus.AddItem ("SINGLE")
   fpcomboFedStatus.AddItem ("MARRIED")
   fpcomboStateX.AddItem ("Y")
   fpcomboStateX.AddItem ("N")
   fpcomboStateAmtPct.AddItem ("A")
   fpcomboStateAmtPct.AddItem ("P")
   
   Select Case ThisState$
   Case "GA":
     fpcomboStateStatus.AddItem ("GASingle")
     fpcomboStateStatus.AddItem ("GAMarried")
     fpcomboStateStatus.AddItem ("GAHead of Household")
   Case "SC":
     fpcomboStateStatus.AddItem ("SC Tax Table")
   Case "OK":
     fpcomboStateStatus.AddItem ("Single")
     fpcomboStateStatus.AddItem ("Married, Head of Household")
     fpcomboStateStatus.AddItem ("Dual Income Married")
   Case "AR":
     fpcomboStateStatus.AddItem ("Single")
     fpcomboStateStatus.AddItem ("Married (1 Exempt'n)")
     fpcomboStateStatus.AddItem ("Married/Head Fam(2 Exempt'ns")
   Case Else:
     fpcomboStateStatus.AddItem ("Single")
     fpcomboStateStatus.AddItem ("Married")
     fpcomboStateStatus.AddItem ("Head of Household")
   End Select
   
   fpcomboSocX.AddItem ("Y")
   fpcomboSocX.AddItem ("N")
   fpcomboMedX.AddItem ("Y")
   fpcomboMedX.AddItem ("N")
   fpcomboEIC.AddItem ("0 - No Certificate")
   fpcomboEIC.AddItem ("1 - Employee Only")
   fpcomboEIC.AddItem ("2 - Employee & Spouse")
   
   If newEmpFlag = False Then
      OpenEmpData2File EmpData2FileHandle
      Get EmpData2FileHandle, thisRecordNum, EmpData2FileRec
      Close EmpData2FileHandle
      fpcomboFedX.Text = QPTrim$(EmpData2FileRec.EMPFEDX)
      fpcomboFedAmtPct.Text = QPTrim$(EmpData2FileRec.EMPFEDO2)
      fptxtFedFig.Text = EmpData2FileRec.EMPFEDO1
      If EmpData2FileRec.EMPFEDS = "Head of Household" Then
         MsgBox "Head of Household is no longer a status type"
         EmpData2FileRec.EMPFEDS = ""
      End If
      fpcomboFedStatus.Text = EmpData2FileRec.EMPFEDS
      fptxtAllowNumFed.Text = EmpData2FileRec.EMPFEDA       'num of allowance
      fptxtAddWHFed.Text = EmpData2FileRec.EMPFEDAA
      fpcomboStateX.Text = QPTrim$(EmpData2FileRec.EMPSTAX)
      fpcomboStateAmtPct.Text = QPTrim$(EmpData2FileRec.EMPSTAO2)
      fptxtStateFig.Text = EmpData2FileRec.EMPSTAO1
      fpcomboStateStatus.Text = EmpData2FileRec.EMPSTAS
      fptxtAllowNumState.Text = EmpData2FileRec.EMPSTAA
      fptxtAddWHState.Text = EmpData2FileRec.EMPSTAAA
      fpcomboSocX.Text = EmpData2FileRec.EMPSOCX
      fpcomboMedX.Text = EmpData2FileRec.EMPMEDX
      fpcomboEIC.Text = EmpData2FileRec.EMPEIC
      For x = 1 To 12
          vaSpreadMisc.Col = 2
          vaSpreadMisc.Row = x
          vaSpreadMisc.Text = EmpData2FileRec.EmpDed(x).DPct
          vaSpreadMisc.Col = 3
          vaSpreadMisc.Row = x
          vaSpreadMisc.Text = EmpData2FileRec.EmpDed(x).DAmt
          vaSpreadMisc.Col = 4
          vaSpreadMisc.Row = x
          vaSpreadMisc.Text = EmpData2FileRec.EmpDed(x).DOTI
      Next x
   End If

End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%X"
      KeyCode = 0
    Case vbKeyF10:
      SendKeys "%S"
      KeyCode = 0
    Case Else:
  End Select

End Sub

Private Sub Form_Load()
  Dim cnt As Integer
  Dim ScrWidth As Long
  ScrWidth = Screen.Width / Screen.TwipsPerPixelX
  Set Over = New clsTextBoxOverRider
  Over.OverRide Me
  Set Temp_Class = New Resize_Class
  Temp_Class.InitResizeClass Me
  Select Case ScrWidth
  Case 1280
    For cnt = 1 To 6
      vaSpreadMisc.ColWidth(cnt) = vaSpreadMisc.ColWidth(cnt) + 5.5
    Next
  Case 1024
    For cnt = 1 To 6
      vaSpreadMisc.ColWidth(cnt) = vaSpreadMisc.ColWidth(cnt) - 0.65
    Next
  Case 800
    For cnt = 1 To 6
      vaSpreadMisc.ColWidth(cnt) = vaSpreadMisc.ColWidth(cnt) - 1.2
    Next
  Case Else
  End Select
  
  LoadEmpMnt2File
End Sub


Private Sub Form_Resize()
  If Me.WindowState <> vbMinimized Then
    Me.Visible = False
    Temp_Class.ResizeControls Me
    Me.Visible = True
    Me.SetFocus
  End If

End Sub

