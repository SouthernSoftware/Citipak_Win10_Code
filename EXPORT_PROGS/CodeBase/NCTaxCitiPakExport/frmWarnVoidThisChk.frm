Attribute VB_Name = "frmWarnVoidThisChk"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Dim FlashFlag As Boolean
Public Enum VoidYN
  vynInvalidOption = 0
  vynNo
  vynYes
End Enum

Private m_vynOption As VoidYN

Property Get Selection() As VoidYN
  Selection = m_vynOption
End Property


Private Sub cmdNO_Click()
  FlashFlag = True
  m_vynOption = vynNo
  Unload frmWarnVoidThisChk
  MainLog ("Void check warning issued...NO chosen.")
End Sub

Private Sub cmdYes_Click()
  If fpcmbWhichDate.Text = "" Then
    MsgBox "Please select which date to post."
    fpcmbWhichDate.SetFocus
    Exit Sub
  End If
  If InStr(fpcmbWhichDate.Text, "TODAY'S") > 0 Then
    frmVoidListOfChecks.WhichDate = Date2Num(Date)
  Else
    frmVoidListOfChecks.WhichDate = Date2Num(fptxtDate.Text)
  End If
  m_vynOption = vynYes
  Unload frmWarnVoidThisChk
  MainLog ("Void check warning issued...YES chosen.")
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
  Case vbKeyF10:
    SendKeys "%O"
    KeyCode = 0
  Case Else:
  End Select
End Sub

Private Sub Form_Load()
  fptxtEmpName.Text = Mid(frmVoidListOfChecks.fptxtEName, 10)
  frmVoidListOfChecks.fpList.Col = 0
  fptxtTransNum.Text = frmVoidListOfChecks.fpList.ColText
  frmVoidListOfChecks.fpList.Col = 1
  fptxtDate.Text = frmVoidListOfChecks.fpList.ColText
  frmVoidListOfChecks.fpList.Col = 2
  fptxtNetPay.Text = frmVoidListOfChecks.fpList.ColText
'  fpcmbWhichDate.Text = "POST WITH CHECK DATE"
  fpcmbWhichDate.AddItem "POST WITH CHECK DATE"
  fpcmbWhichDate.AddItem "POST WITH TODAY'S DATE"
End Sub

Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    MainLog ("Payroll.exe terminated via menu bar on frmWarnVoidThisChk.")
    Call Terminate
    End
  End If
End Sub

Private Sub Timer1_Timer()
   If Label10.Caption <> CStr(Time) Then
      Label10.Caption = CStr(Time)
      Label2.Caption = "WARNING! WARNING! WARNING!"
   Else
      Label2.Caption = ""
   End If
End Sub


