Private Sub cmdExit_Click()
  fptxtChoice.Text = "exit"
  Me.Hide
End Sub

Private Sub cmdInitialize_Click()
  fptxtChoice = "continue"
  Me.Hide
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyF10:
      Call cmdInitialize_Click
      KeyCode = 0
    Case vbKeyEscape:
      Call cmdExit_Click
      KeyCode = 0
    Case Else:
  End Select

End Sub

