Private Sub txtNumber_LostFocus()
  Dim EmpData2FileHandle As Integer
  Dim EmpData2FileRec As EmpData2Type
  Dim EmpRecNo As Long, x As Long
  If QPTrim$(txtNumber.Text) = "" Then GoTo NoChange
  If newEmpFlag = True Then
     OpenEmpData2File EmpData2FileHandle
     EmpRecNo = LOF(EmpData2FileHandle) / Len(EmpData2FileRec)
     For x = 1 To EmpRecNo
        Get EmpData2FileHandle, x, EmpData2FileRec
        If QPTrim$(EmpData2FileRec.EmpNo) = "" Then GoTo EmptyString
        If QPTrim$(EmpData2FileRec.EmpNo) = QPTrim$(txtNumber.Text) Then
           MsgBox "You have entered an employee number that is already in use."
           txtNumber.SetFocus
           Exit For
        End If
EmptyString:
     Next x
     Close EmpData2FileHandle
  End If
  If newEmpFlag = False Then
     If Val(QPTrim$(txtNumber.Text)) = tempEmpNum Then
        GoTo NoChange
     Else
        OpenEmpData2File EmpData2FileHandle
        EmpRecNo = LOF(EmpData2FileHandle) / Len(EmpData2FileRec)
        For x = 1 To EmpRecNo
        Get EmpData2FileHandle, x, EmpData2FileRec
        If QPTrim$(EmpData2FileRec.EmpNo) = QPTrim$(txtNumber.Text) Then
           MsgBox "You have entered an employee number that is already in use."
           txtNumber.SetFocus
           Exit For
        End If
        Next x
     End If
     Close EmpData2FileHandle
   End If
     
  
NoChange:
End Sub

