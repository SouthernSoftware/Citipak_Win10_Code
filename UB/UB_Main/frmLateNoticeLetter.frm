Private Sub cmdSave_Click()
'do save stuff
Dim LaLe As Integer, lenlate As Integer, cnt As Integer
ReDim latelet(1) As UBLateLetterType
LaLe = FreeFile
lenlate = Len(latelet(1))
Open UBPath$ + "UBLatLet.dat" For Random Shared As LaLe Len = lenlate
  latelet(1).Head1 = RTrim(fpText1.Text)
  latelet(1).Head2 = RTrim(fpText2.Text)
  latelet(1).Head3 = RTrim(fpText3.Text)
  latelet(1).Head4 = RTrim(fpText4.Text)
  latelet(1).Head5 = RTrim(fpText5.Text)
  For cnt = 0 To 19
    latelet(1).Body(cnt + 1) = RTrim(fptxtBody(cnt))
  Next
Put LaLe, 1, latelet(1)
Close
  MsgBox "Late Notice Letter has been saved.", vbOKOnly, "Save Complete"
cmdPrint.Enabled = True
End Sub

Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    If cmdExit.Enabled = False Then
      Cancel = True
    Else
      If MsgBox("Are You Sure You Wish To Close The Program?", vbYesNo, "Close?") = vbNo Then
        Cancel = True
      Else
        UBLog "Closed via LateNoticeLetter by " + PWUser$
        CitiTerminate
      End If
    End If
  End If
End Sub

