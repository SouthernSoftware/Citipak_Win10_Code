  Dim UBSetupreclen As Integer
  ReDim UBSetUp(1) As UBSetupRecType
  UBSetupreclen = Len(UBSetUp(1))
  LoadUBSetUpFile UBSetUp(), UBSetupreclen

  If Exist(UBPath$ + "UBLatLet.dat") Then
    ReDim latelet(1) As UBLateLetterType
    LaLe = FreeFile
    lenlate = Len(latelet(1))
    Open UBPath$ + "UBLatLet.dat" For Random Shared As LaLe Len = lenlate
    Get LaLe, 1, latelet(1)
    fpText1.Text = RTrim(latelet(1).Head1)
    fpText2.Text = RTrim(latelet(1).Head2)
    fpText3.Text = RTrim(latelet(1).Head3)
    fpText4.Text = RTrim(latelet(1).Head4)
    fpText5.Text = RTrim(latelet(1).Head5)
    For cnt = 0 To 19
      fptxtBody(cnt) = RTrim(latelet(1).Body(cnt + 1))
    Next
    Close
  Else
    fpText1 = TOWNNAME$
    fpText2 = ""
    fpText3 = ""
    fpText4 = QPTrim(UBSetUp(1).DEFCITY) & ", " & QPTrim(UBSetUp(1).DEFSTATE) & " " & QPTrim(UBSetUp(1).ZIPCODE)
    fpText5 = ""
  End If
End Sub
