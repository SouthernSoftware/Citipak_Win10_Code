Private Sub UpdateMeter()
  Dim UBCustRecLen As Integer, C2Handle As Integer, TempRev As String
  Dim AcctStatus As String, AvgCnt!
  ReDim UBCustRec(1) As NewUBCustRecType
  UBCustRecLen = Len(UBCustRec(1))
  ClearScn
  C2Handle = FreeFile
  Open UBPath$ + "UBCUST.DAT" For Random Shared As C2Handle Len = UBCustRecLen
  Get #C2Handle, RecNo&, UBCustRec(1)
  Close C2Handle
  
      If cnt = 1 Then
        LabelRec.Caption = "First Record Displayed."
        LabelRec.Visible = True
      ElseIf cnt = txtTotRec Then
        LabelRec.Caption = "Last Record Displayed."
        LabelRec.Visible = True
      Else
        LabelRec.Visible = False
      End If
      GoSub GetAcctStatus
  'Help$ = "Record #" + Str$(cnt) + " of " + Str$(IdxNumOfRecs)
      fpCustName.Caption = UBCustRec(1).CustName
      fpBook.Caption = UBCustRec(1).Book
      fpSeqNumb.Caption = UBCustRec(1).SEQNUMB
      LabelAcctNo.Caption = Str$(RecNo&)
      fpServAddr.Caption = UBCustRec(1).ServAddr
      fpAcctStatus.Caption = AcctStatus$
      
  For MtrCnt = 1 To 7           'find last active meter

'    'inputting = False
    TempRev$ = QPTrim$(UBCustRec(1).LocMeters(MtrCnt).MTRType)
'
    If Len(TempRev$) <> 0 Then
      Rem set information as known
      ' = Right$(Str$(BookNumber), 2)

     ' GoSub GetMeterType
      fpMtrType1(MtrCnt - 1).Enabled = True
      fpMtrNum(MtrCnt - 1).Enabled = True
      txtDate(MtrCnt - 1).Enabled = True
      If UBCustRec(1).LocMeters(MtrCnt).ReadFlag = "Y" Then
        txtDate(MtrCnt - 1) = Num2Date(UBCustRec(1).LocMeters(MtrCnt).CurDate)
      End If
      fpCurrent(MtrCnt - 1).Enabled = True
      fpPrevious(MtrCnt - 1).Enabled = True
      fpAvgUse(MtrCnt - 1).Enabled = True

      fpMtrType1(MtrCnt - 1).Caption = UBCustRec(1).LocMeters(MtrCnt).MTRType 'MeterType$
      fpMtrNum(MtrCnt - 1).Caption = UBCustRec(1).LocMeters(MtrCnt).MtrNum
      
'      If UBCustRec(1).LocMeters(MtrCnt).CurRead > 0 Then
'        fpCurrent(MtrCnt - 1) = QPTrim$(Str$(UBCustRec(1).LocMeters(MtrCnt).CurRead))
'      Else
'        fpCurrent(MtrCnt - 1) = Str$(0)
'      End If
'      If UBCustRec(1).LocMeters(MtrCnt).PrevRead > 0 Then
'        fpPrevious(MtrCnt - 1) = QPTrim$(Str$(UBCustRec(1).LocMeters(MtrCnt).PrevRead))
'      Else
'        fpPrevious(MtrCnt - 1) = Str$(0)
'      End If
      If UBCustRec(1).LocMeters(MtrCnt).ReadFlag = "Y" Then
        fpCurrent(MtrCnt - 1) = QPTrim$(Str$(UBCustRec(1).LocMeters(MtrCnt).CurRead))
        fpPrevious(MtrCnt - 1) = QPTrim$(Str$(UBCustRec(1).LocMeters(MtrCnt).PrevRead))
      Else
        fpPrevious(MtrCnt - 1) = Str$(UBCustRec(1).LocMeters(MtrCnt).CurRead)
      End If

      If MeterType$ = "Demand Electric" Then
        fpPrevious(MtrCnt - 1) = "0"    ' Demand Always = 0 for Previous Read
      End If


      Average# = UBCustRec(1).LocMeters(MtrCnt).AvgUse
      AvgCnt! = UBCustRec(1).LocMeters(MtrCnt).UseCnt
      ' RESET AVERAGE IS OUT OF WHACK
      If Average# > 99999999 Or Average# <= 0 Then
        UBCustRec(1).LocMeters(MtrCnt).AvgUse = 0
        UBCustRec(1).LocMeters(MtrCnt).UseCnt = 0
        Average# = 0
        AvgCnt! = 0
      End If
      fpAvgUse(MtrCnt - 1) = Using("##,###,###", Average#)
    Else
      fpMtrType1(MtrCnt - 1).Enabled = False
      fpMtrNum(MtrCnt - 1).Enabled = False
      txtDate(MtrCnt - 1).Enabled = False
      fpCurrent(MtrCnt - 1).Enabled = False
      fpPrevious(MtrCnt - 1).Enabled = False
      fpAvgUse(MtrCnt - 1).Enabled = False
    End If
  Next MtrCnt
 
  Exit Sub

GetAcctStatus:
  Select Case UBCustRec(1).Status
  Case "A"
    AcctStatus$ = "Active"
  Case "F"
    AcctStatus$ = "Final"
  Case "I"
    AcctStatus$ = "Vacant"
  Case Else
    AcctStatus$ = "Undef."
  End Select
  Return
