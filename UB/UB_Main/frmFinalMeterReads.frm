Private Sub fpCurrent_KeyDown(Index As Integer, KeyCode As Integer, Shift As Integer)
  Dim x As Integer
  x = Index
  If KeyCode = vbKeyReturn Or KeyCode = vbKeyRight Or KeyCode = vbKeyDown Then
'   If CurrChg = True Then
'    DoCalcUsage x
'   End If
    If Index < 6 Then
     For x = Index To 5
      If fpCurrent(x + 1).Enabled Then
        fpCurrent(x + 1).SetFocus
        Exit For
      End If
     Next
    End If
    If x = 6 Then
      If fpCurrent(6).Enabled = False Then
       fpCmdSave.SetFocus
      End If
    End If
  ElseIf KeyCode = vbKeyLeft Or KeyCode = vbKeyUp Then
'   If CurrChg = True Then
'    DoCalcUsage x
'   End If
    If Index > 0 Then
     For x = Index To 5
      If fpCurrent(x - 1).Enabled Then
        fpCurrent(x - 1).SetFocus
        Exit For
      End If
     Next
    End If
    If x = 0 Then
      If fpCurrent(0).Enabled = False Then
       fpCmdSave.SetFocus
      End If
    End If

  End If
