Private Sub DoCalcUsage(Mtr As Integer)
  Dim NewRead As Double, PastRead As Double, Calcusage As Double
  Dim J As Double, Max As Double, AvgCnt As Single
  ' do the calculation here
  If fpCurrent(Mtr).Enabled = True Then
  NewRead# = Val(fpCurrent(Mtr))
  PastRead# = Val(fpPrevious(Mtr))
  If NewRead# >= PastRead# Then
    Calcusage# = NewRead# - PastRead#
  Else
    J# = Len(Str$(PastRead#)) - 1: Max# = 10 ^ J#
    Calcusage# = Max# - PastRead# + NewRead#
  End If
  fpUsage(Mtr) = Str$(Calcusage#)

  fpAvgUse(Mtr) = Using("##,###,###", Average#)
  If Calcusage# <> Average# Then
     'fpEstimated(Mtr) = "NO"
  End If
  If Average# > 0 Then 'AvgCnt! > 0 And
    LabelMsg(Mtr).Visible = False
    If Calcusage# > (Average# * (HighVar / 100)) Then
      'HighRead = True
      LabelMsg(Mtr).Visible = True
      LabelMsg(Mtr).Caption = "<-HIGH"
      Beep
      'fpCurrent(mtr).SetFocus 'nono this won't allow if valid
    End If
    If Calcusage# < (Average# * (LowVar / 100)) Then
      LabelMsg(Mtr).Visible = True
      LabelMsg(Mtr).Caption = "<-LOW"
      'LowRead = True
      Beep
      'fpCurrent(mtr).SetFocus 'nono won't ever let off field unless change
    End If
  End If
  End If
  'if UBCustRec(1).EstFlag = "E"
  End Sub

Private Function CurrChg()
  Dim Xidx As Integer
  Dim C2Handle As Integer, UBCustRecLen As Integer
  ReDim UBCustRec(1) As NewUBCustRecType
  CurrChg = False
  UBCustRecLen = Len(UBCustRec(1))
  C2Handle = FreeFile
  Open UBPath$ + "UBCUST.DAT" For Random Shared As C2Handle Len = UBCustRecLen
  Get #C2Handle, RecNo&, UBCustRec(1)
  Close C2Handle
   For Xidx = 1 To 7
    If UBCustRec(1).LocMeters(Xidx).ReadFlag = "Y" Then
      If Val(fpCurrent(Xidx - 1)) <> UBCustRec(1).LocMeters(Xidx).CurRead Then
        CurrChg = True
        Exit For
      End If
    Else
      If Val(fpCurrent(Xidx - 1)) <> 0 Then
        CurrChg = True
        Exit For
      End If
    End If
  Next
End Function


Private Sub DoSave()
  Dim UBCustRecLen As Integer, C2Handle As Integer, MCnt As Integer
  Dim FinalReadDate As String, OldRec As Long, NumOfCust As Long
  Dim UBCustRec As NewUBCustRecType
  UBCustRecLen = Len(UBCustRec)
  C2Handle = FreeFile
  
  NumOfCust& = FileSize&(UBPath$ + "UBCUST.DAT") \ UBCustRecLen
  Open UBPath$ + "UBCUST.DAT" For Random Shared As C2Handle Len = UBCustRecLen
  Get #C2Handle, RecNo&, UBCustRec
'
  For MCnt = 1 To 7

    UBCustRec.LocMeters(MCnt).PrevRead = Val(fpPrevious(MCnt - 1))
    'UBCustRec.LocMeters(MCnt).PrevRead = UBCustRec.LocMeters(MCnt).CurRead
    UBCustRec.LocMeters(MCnt).CurRead = Val(fpCurrent(MCnt - 1))
    'UBCustRec(1).LocMeters(MCnt).PrevRead = Value(Form$((5 * MCnt) + 5, 0),Ecode)
    If UBCustRec.LocMeters(MCnt).ReadFlag <> "Y" Then
      UBCustRec.LocMeters(MCnt).PastDate = UBCustRec.LocMeters(MCnt).CurDate
      UBCustRec.LocMeters(MCnt).CurDate = Date2Num(txtDate(MCnt - 1))
    Else
      UBCustRec.LocMeters(MCnt).CurDate = Date2Num(txtDate(MCnt - 1))
    End If
    FinalReadDate = txtDate(MCnt - 1)
    UBCustRec.LocMeters(MCnt).ReadFlag = "Y"
  Next
  OldRec& = UBCustRec.OldRec  'which is really the new one
  Put #C2Handle, RecNo&, UBCustRec
    If OldRec& > 0 And OldRec& <= NumOfCust& Then
      Get #C2Handle, OldRec&, UBCustRec
      If UBCustRec.Status <> "I" Or QPTrim$(UBCustRec.SEARCH) <> "" Then
      'you dont want to change the new account
      Else 'status is I and no name
        For MCnt = 1 To 7
        'change the prev and curr to same on 8/4/04 Per John Roscoe
          UBCustRec.LocMeters(MCnt).PrevRead = Val(fpCurrent(MCnt - 1)) 'Val(fpPrevious(MCnt - 1))
          'UBCustRec.LocMeters(MCnt).PrevRead = UBCustRec.LocMeters(MCnt).CurRead
          UBCustRec.LocMeters(MCnt).CurRead = Val(fpCurrent(MCnt - 1))
          'UBCustRec(1).LocMeters(MCnt).PrevRead = Value(Form$((5 * MCnt) + 5, 0),Ecode)
          'Change to N so will be ready for new cust to move in 2/2/05
          UBCustRec.LocMeters(MCnt).ReadFlag = "N"
          UBCustRec.LocMeters(MCnt).PastDate = UBCustRec.LocMeters(MCnt).CurDate
          UBCustRec.LocMeters(MCnt).CurDate = Date2Num(txtDate(MCnt - 1))
          FinalReadDate = txtDate(MCnt - 1)
        Next
        Put #C2Handle, OldRec&, UBCustRec
      End If
    End If
  Close C2Handle
        
        
  UBLog "UPDATED READINGS:" + Str$(RecNo&) + "," + Str$(OldRec&) + " P: " + Num2Date$(UBCustRec.LocMeters(1).PastDate) + " C: " + FinalReadDate

End Sub

Private Sub txtDate_KeyDown(Index As Integer, KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeyReturn Or KeyCode = vbKeyDown Then
    fpCurrent(Index).SetFocus
  ElseIf KeyCode = vbKeyUp Then
    If Index <> 0 Then
      fpCurrent(Index - 1).SetFocus
    Else
      fpCurrent(6).SetFocus
    End If
  End If
End Sub
