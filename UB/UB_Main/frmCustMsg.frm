Attribute VB_Name = "frmCustMsgEdit"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Dim CustMsgRec As UBMessRecType, BeenDone As Boolean
Dim RecNo As Long, MessageRec As Long, zz As Integer

Private Sub Form_Activate()
  If Not BeenDone Then
    BeenDone = True
    Call LoadCustMsg
  End If
End Sub

Private Sub Form_Load()
  DoEvents
'  Call LoadCustMsg
'  RecNo = Val(frmCustMsgEdit.CustRec)
End Sub

Private Sub fpCmdClear_Click()
  ReDim MsgText(0 To 5) As String
  Dim zz As Integer
  MsgText(0) = "Erase Messages?"
  MsgText(1) = "WARNING!  WARNING!  WARNING!  WARNING!"
  MsgText(2) = "THIS PROCEDURE CAN NOT BE UNDONE!"
  MsgText(3) = "CLEAR THIS ACCOUNTS MESSAGE INFORMATION?"
  MsgText(4) = ""
  MsgText(5) = "ARE YOU SURE YOU WANT TO DO THIS?"
  
  If GetOKorNot(MsgText()) Then
    For zz = 0 To 14
      frmCustMsgEdit.fpCMsg(zz) = ""
    Next
  End If
  Savemsg
End Sub

Private Sub fpCmdExit_Click()
  If Chk4Change = True Then
    frmChangedWarning.Show vbModal, Me
    Select Case SaveFlag
    Case False
      BeenDone = False
      Unload frmCustMsgEdit
      DoEvents
    Case True
      Savemsg
      BeenDone = False
      Unload frmCustMsgEdit
      DoEvents
    Case 1
      'just stay here
    End Select
  Else
    BeenDone = False
    Unload frmCustMsgEdit
    DoEvents
  End If
End Sub
Private Function Chk4Change()
  Dim UBCustRecLen As Integer
  Dim UBMessRecLen As Integer, changcnt As Integer
  Dim UBFile As Integer
  Dim Txt As String
  Chk4Change = False
  ReDim UBCustRec(1) As NewUBCustRecType
  UBCustRecLen = Len(UBCustRec(1))

  ReDim UBMessRec(1) As UBMessRecType
  UBMessRecLen = Len(UBMessRec(1))

  UBFile = FreeFile
  Open UBCustFile For Random Shared As UBFile Len = UBCustRecLen
  Get UBFile, RecNo, UBCustRec(1)
  Close UBFile
  MessageRec = UBCustRec(1).MessageRec
  If MessageRec > 0 Then
    UBFile = FreeFile
    Open UBPath + "UBMESAGE.DAT" For Random Shared As UBFile Len = UBMessRecLen
    Get UBFile, MessageRec, UBMessRec(1)
    Close UBFile
    For zz = 1 To 15
      If QPTrim(UBMessRec(1).MessLine(zz).Msg) <> QPTrim(frmCustMsgEdit.fpCMsg(zz - 1).Text) Then
       changcnt = changcnt + 1
      End If
    Next
  Else
    For zz = 1 To 15
      If Len(frmCustMsgEdit.fpCMsg(zz - 1).Text) <> 0 Then
        changcnt = changcnt + 1
      End If
    Next
  End If
  If changcnt > 0 Then
    Chk4Change = True
  End If
End Function
Private Sub LoadCustMsg()
  Dim UBCustRecLen As Integer
  Dim UBMessRecLen As Integer
  Dim UBFile As Integer
  Dim Txt As String
  
  RecNo = Val(frmCustMsgEdit.CustRec)
  ReDim UBCustRec(1) As NewUBCustRecType
  UBCustRecLen = Len(UBCustRec(1))

  ReDim UBMessRec(1) As UBMessRecType
  UBMessRecLen = Len(UBMessRec(1))
  
  UBFile = FreeFile
  Open UBCustFile For Random Shared As UBFile Len = UBCustRecLen
  Get UBFile, RecNo, UBCustRec(1)
  Close UBFile
  Label1.Caption = "Acct #: " + Str(RecNo)
  Label2.Caption = "Name : " + UBCustRec(1).CustName
  Label3.Caption = "Status: " + UBCustRec(1).Status
  MessageRec = UBCustRec(1).MessageRec
  If MessageRec > 0 Then
    UBFile = FreeFile
    Open UBPath + "UBMESAGE.DAT" For Random Shared As UBFile Len = UBMessRecLen
    Get UBFile, MessageRec, UBMessRec(1)
    Close UBFile
    For zz = 1 To 15
      Txt = UBMessRec(1).MessLine(zz).Msg
      Txt = RTrim(RemNulls(Txt))
      frmCustMsgEdit.fpCMsg(zz - 1).Text = Txt
    Next
  End If
End Sub

Private Sub fpcmdPrint_Click()
    frmReportOpt.Show 1
    If rptopt = 1 Then
      'do the graphics
      PrintMsg True
    ElseIf rptopt = 2 Then
      'do the text
      PrintMsg False
    End If

End Sub
Private Sub fpCmdSave_Click()
  Savemsg
  MsgBox "Message Saved", vbOKOnly, "Saved"
End Sub
Private Sub Savemsg()
  Dim UBCustRecLen As Integer
  Dim UBMessRecLen As Integer
  Dim UBFile As Integer
  Dim Txt As String
  
  RecNo = Val(frmCustMsgEdit.CustRec)
  ReDim UBCustRec(1) As NewUBCustRecType
  UBCustRecLen = Len(UBCustRec(1))

  ReDim UBMessRec(1) As UBMessRecType
  UBMessRecLen = Len(UBMessRec(1))
  
  For zz = 1 To 15
    UBMessRec(1).MessLine(zz).Msg = frmCustMsgEdit.fpCMsg(zz - 1).Text
  Next
  UBMessRec(1).CustRec = Val(frmCustMsgEdit.CustRec)
  
  UBFile = FreeFile
  Open UBPath + "UBMESAGE.DAT" For Random Shared As UBFile Len = UBMessRecLen
  If MessageRec = 0 Then
    MessageRec = (LOF(UBFile) / UBMessRecLen) + 1
  End If
  Put UBFile, MessageRec, UBMessRec(1)
  Close UBFile

  UBFile = FreeFile
  Open UBCustFile For Random Shared As UBFile Len = UBCustRecLen
  Get UBFile, RecNo&, UBCustRec(1)
  UBCustRec(1).MessageRec = MessageRec
  Put UBFile, RecNo&, UBCustRec(1)
  Close UBFile

End Sub
Private Sub fpCMsg_KeyDown(Index As Integer, KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
  Case vbKeyReturn
    KeyCode = 0
    DoEvents
    SendKeys "{Tab}", True
    DoEvents
  Case vbKeyF3
    KeyCode = 0
    DoEvents
    Call fpCmdClear_Click
  Case vbKeyF8
    KeyCode = 0
    DoEvents
    Call fpcmdPrint_Click
  Case vbKeyF10
    KeyCode = 0
    DoEvents
    Call fpCmdSave_Click
  Case vbKeyEscape
    KeyCode = 0
    DoEvents
    Call fpCmdExit_Click
  End Select
End Sub
Private Sub PrintMsg(Grph As Boolean)
  Dim ReportFile As String
  Dim cnt As Integer
  Dim UBRpt As Integer
  If Grph Then
    MaxLines = 48
  Else
    MaxLines = 53
  End If

  
  ReportFile$ = UBPath$ + "UBCustMsg.RPT"
  UBRpt = FreeFile
  Open ReportFile$ For Output As UBRpt
  
  Print #UBRpt, " "
  Print #UBRpt, " "
  Print #UBRpt, " "
  Print #UBRpt, Tab(5); "Customer Message"
  Print #UBRpt, " "
  Print #UBRpt, " "

  Print #UBRpt, Tab(5); Label2.Caption
  Print #UBRpt, Tab(5); Label1.Caption
  Print #UBRpt, Tab(5); Label3.Caption
  Print #UBRpt, " "
  Print #UBRpt, " "
  Print #UBRpt, " "
    For zz = 1 To 15
      Print #UBRpt, Tab(5); RTrim(frmCustMsgEdit.fpCMsg(zz - 1).Text)
    Next
  Print #UBRpt, " "
  If Grph Then
    Print #UBRpt, " "
  Else
    Print #UBRpt, FF$
  End If
  Close
  If Grph Then
