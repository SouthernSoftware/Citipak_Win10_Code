Private Sub loadLetter()
  Dim LaLe As Integer, lenlate As Integer, cnt As Integer
  Dim UBSetupreclen As Integer
  ReDim UBSetUp(1) As UBSetupRecType
  UBSetupreclen = Len(UBSetUp(1))
  LoadUBSetUpFile UBSetUp(), UBSetupreclen

  If Exist(UBPath$ + "UBLatN21.dat") Then
    ReDim latelet(1) As UBLateNotice21LineType
    LaLe = FreeFile
    lenlate = Len(latelet(1))
    Open UBPath$ + "UBLatN21.dat" For Random Shared As LaLe Len = lenlate
    Get LaLe, 1, latelet(1)
    fpTextH1.Text = RTrim(latelet(1).TxtLineH1)
    fpTextH2.Text = RTrim(latelet(1).TxtLineH2)
    fpText1.Text = RTrim(latelet(1).TxtLine1)
    fpText2.Text = RTrim(latelet(1).TxtLine2)
    fpText3.Text = RTrim(latelet(1).TxtLine3)
    fpText4.Text = RTrim(latelet(1).TxtLine4)
    fpText5.Text = RTrim(latelet(1).TxtLine5)
    fpText6.Text = RTrim(latelet(1).TxtLine6)
    fptext7.Text = RTrim(latelet(1).TxtLine7)
    fptext8.Text = RTrim(latelet(1).TxtLine8)
    fptext9.Text = RTrim(latelet(1).TxtLine9)
    fpText10.Text = RTrim(latelet(1).TxtLine10)
    fpText11.Text = RTrim(latelet(1).TxtLine11)
    fptextB1.Text = RTrim(latelet(1).TxtBLine1)
    fptextB2.Text = RTrim(latelet(1).TxtBLine2)
    Close
  Else
    fpText1 = TOWNNAME$
    fpText3 = QPTrim(UBSetUp(1).DEFCITY) & ", " & QPTrim(UBSetUp(1).DEFSTATE) & " " & QPTrim(UBSetUp(1).ZIPCODE)
    
  End If
End Sub
Private Sub LateLetterSample()
  Dim fmt1 As String, fmt2 As String, ReportFile As String
  Dim LaLe As Integer, lenlate As Integer, cnt As Integer
  Dim UBRpt As Integer, DeDate As String
  If Exist(UBPath$ + "UBLatN21.dat") Then
    ReDim latelet(1) As UBLateNotice21LineType
    LaLe = FreeFile
    lenlate = Len(latelet(1))
    Open UBPath$ + "UBLatN21.dat" For Random Shared As LaLe Len = lenlate
    Get LaLe, 1, latelet(1)
    Close
  ReportFile$ = UBPath$ + "UBLatAln.RPT"
  UBRpt = FreeFile
  Open ReportFile$ For Output As UBRpt
  
  DeDate$ = FormatDateTime(Date$, vbLongDate)
     Print #UBRpt, "Notice Date: "; DeDate$;
     Print #UBRpt, Tab(50); RTrim(latelet(1).TxtLine1)
     Print #UBRpt, "Pay By Date: "; DeDate$;
     Print #UBRpt, Tab(50); RTrim(latelet(1).TxtLine2)
     Print #UBRpt, RTrim(latelet(1).TxtLineH1);
     Print #UBRpt, Tab(50); RTrim(latelet(1).TxtLine3)
     Print #UBRpt, RTrim(latelet(1).TxtLineH2);
     Print #UBRpt, Tab(50); RTrim(latelet(1).TxtLine4)
     Print #UBRpt, "Rev1name"; Tab(32); Using$("#####,#.##", 75#);
     Print #UBRpt, Tab(50); RTrim(latelet(1).TxtLine5)
     Print #UBRpt, "Rev2name"; Tab(32); Using("#####,#.##", 25);
     Print #UBRpt, Tab(50); RTrim(latelet(1).TxtLine6)
     Print #UBRpt, "Rev3name"; Tab(32); Using("#####,#.##", 50#);
     Print #UBRpt, Tab(50); RTrim(latelet(1).TxtLine7)
     Print #UBRpt, "Rev4name"; Tab(32); Using("#####,#.##", 5#);
     Print #UBRpt, Tab(50); RTrim(latelet(1).TxtLine8)
     Print #UBRpt, "Rev5name"; Tab(32); Using("#####,#.##", 4.5);
     Print #UBRpt, Tab(50); RTrim(latelet(1).TxtLine9)
     Print #UBRpt, "Past Balance Due: "; Tab(32); Using("#####,#.##", 159.5);
     Print #UBRpt, Tab(50); RTrim(latelet(1).TxtLine10)
     Print #UBRpt, Tab(50); RTrim(latelet(1).TxtLine11)
     Print #UBRpt,
     Print #UBRpt,
     Print #UBRpt, "1121";
     Print #UBRpt, Tab(10); "CUSTOMER NAME"
     Print #UBRpt, Tab(10); "ADDRESS LINE 1"
     Print #UBRpt, Tab(10); "ADDRESS LINE 2"
     Print #UBRpt, Tab(10); "ANYWHERE          "; " "; "NC"; " "; "11111"
     Print #UBRpt,
     Rem condense here lines 19 & 20
     Print #UBRpt, Chr$(15);
     Print #UBRpt, RTrim$(latelet(1).TxtBLine1) '"All bills due upon presentation. A penalty of 10% will be added 30 days after billing date. Services will be discontinued if not"
     Print #UBRpt, RTrim$(latelet(1).TxtBLine2) '"paid within 35 days from billing date.  Receipt will not be returned unless requested.  A $10.00 charge on all returned checks."
     Print #UBRpt, Chr$(18);
     Rem uncondense here
     Print #UBRpt, "~" '    ToPrint$ = ToPrint$ + "~" + " ~ ~ "
  Close
    ViewPrint ReportFile$, "Late Notice"
  
End If
