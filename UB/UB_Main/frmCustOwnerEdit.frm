Attribute VB_Name = "frmCustOwnerEdit"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Dim OwnerBeenDone As Boolean
Dim OwnerRecNo As Long
'

Private Sub Form_Activate()
  If Val(frmCustOwnerEdit.RecNo) > 0 And Not OwnerBeenDone Then
    OwnerRecNo = Val(frmCustOwnerEdit.RecNo)
    frmCustOwnerEdit.RecNo = 0
    OwnerBeenDone = True
    LoadOwnerInfo2Form
    DoEvents
  End If
End Sub

Private Sub cmdCancel_Click()
  frmCustOwnerEdit.ActionFlag = Str$(False)
  Call ExitOwnerEdit
End Sub

Private Sub cmdOk_Click()
  frmCustOwnerEdit.ActionFlag = Str$(True)
  Call ExitOwnerEdit
End Sub

Private Sub ExitOwnerEdit()
  OwnerBeenDone = False
  OwnerRecNo = 0
  DoEvents
  frmCustOwnerEdit.Hide
End Sub

Private Sub LoadOwnerInfo2Form()
  Dim UBOwnerRec As UBOwnerRecType
  Dim UBFile As Integer, OwnerRecLen As Integer
  OwnerRecLen = Len(UBOwnerRec)
  UBFile = FreeFile
  Open UBOwnerFile For Random As UBFile Len = OwnerRecLen
  Get UBFile, OwnerRecNo, UBOwnerRec
  Close UBFile
  With frmCustOwnerEdit
    fpLastName.Text = QPTrim$(UBOwnerRec.OwnLName)
    fpFirstName.Text = QPTrim$(UBOwnerRec.OwnFName)
    fpAddr1.Text = QPTrim$(UBOwnerRec.ADDR1)
    fpAddr2.Text = QPTrim$(UBOwnerRec.ADDR2)
    fpCity.Text = QPTrim$(UBOwnerRec.CITY)
    fpState.Text = QPTrim$(UBOwnerRec.STATE)
    fpZip.Text = QPTrim$(UBOwnerRec.ZIPCODE)
    fpHPhone.Text = QPTrim$(UBOwnerRec.HPHONE)
    fpWPhone.Text = QPTrim$(UBOwnerRec.WPHONE)
  End With
End Sub

Private Sub Form_KeyUp(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
  Case vbKeyReturn
    KeyCode = 0
    DoEvents
    SendKeys "{Tab}", True
    DoEvents
  Case vbKeyF10
    KeyCode = 0
    DoEvents
    Call cmdOk_Click
  Case vbKeyEscape
    KeyCode = 0
    DoEvents
    Call cmdCancel_Click
  End Select
End Sub
