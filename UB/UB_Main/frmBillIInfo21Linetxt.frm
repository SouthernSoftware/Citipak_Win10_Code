Private Sub cmdTest_Click()
  Dim UBBillSetuplen As Integer
  ReDim UBBillSetup(1) As UBBillSetupType
  UBBillSetuplen = Len(UBBillSetup(1))
  LoadUBBillSetUpFile UBBillSetup(), UBBillSetuplen

  If UBBillSetup(1).Bill = 21 Then
    Savebill
    DeActivateControls Me
    Test21LinePrint
    ActivateControls Me
  Else
    MsgBox "Test Print only for Letter Bill #21.", vbOKOnly, "Invalid Bill Type"
  End If
End Sub

Private Sub fpcboMorD_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeySpace Then
    fpcboMorD.ListDown = True
  End If
  If fpcboMorD.ListDown <> True Then
    If KeyCode = vbKeyDown Or KeyCode = vbKeyReturn Then
      fptextB1.SetFocus
      KeyCode = 0
    Else
      If KeyCode = vbKeyUp Then
        fpText11.SetFocus
        KeyCode = 0
      End If
    End If
  End If
End Sub


Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    If cmdExit.Enabled = False Then
      Cancel = True
    Else
      If MsgBox("Are You Sure You Wish To Close The Program?", vbYesNo, "Close?") = vbNo Then
        Cancel = True
      Else
        UBLog "Closed via Bill21Linfo setup by " + PWUser$
        CitiTerminate
      End If
    End If
  End If
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
'    Case vbKeyDown, vbKeyReturn:
'      SendKeys "{Tab}"
'      KeyCode = 0
'    Case vbKeyUp:
'      SendKeys "+{Tab}"
'      KeyCode = 0
    Case vbKeyEscape:
      KeyCode = 0
      DoEvents
      Call cmdExit_Click
    Case vbKeyF5:
      KeyCode = 0
      DoEvents
      Call cmdTest_Click
    Case vbKeyF10:
      KeyCode = 0
      DoEvents
      Call cmdSave_Click
    Case Else:
  End Select
End Sub

Private Sub Form_Load()
  Dim UBBill21Linelen As Integer
  Dim UBBillSetuplen As Integer, cnt As Integer
  ReDim UBBillSetup(1) As UBBillSetupType
  UBBillSetuplen = Len(UBBillSetup(1))
  Set Temp_Class = New Resize_Class
  Temp_Class.InitResizeClass Me
  Set Over = New clsTextBoxOverRider
  Over.OverRide Me
  StatusBar1.Panels.Item(1).Text = TOWNNAME$
  LoadUBBillSetUpFile UBBillSetup(), UBBillSetuplen
  fpcboMorD.AddItem "Message Line 1"
  fpcboMorD.AddItem "Dates of Service"
  If Exist(UBPath$ + "UBBil21L.DAT") Then
    ReDim UBBill(1) As UBBill21LineType
    UBBill21Linelen = Len(UBBill(1))
    LoadUBBill21LineFile UBBill(), UBBill21Linelen
    If QPTrim$(UBBill(1).MsgORdates) = "M" Then
      fpcboMorD.ListIndex = 0
    Else
      fpcboMorD.ListIndex = 1
    End If
    fpText1 = UBBill(1).TxtLine1
    fpText2 = UBBill(1).TxtLine2
    fpText3 = UBBill(1).TxtLine3
    fpText4 = UBBill(1).TxtLine4
    fpText5 = UBBill(1).TxtLine5
    fpText6 = UBBill(1).TxtLine6
    fptext7 = UBBill(1).TxtLine7
    fptext8 = UBBill(1).TxtLine8
    fptext9 = UBBill(1).TxtLine9
    
    fpText11 = UBBill(1).TxtLine11
    fptextB1 = UBBill(1).TxtBLine1
    fptextB2 = UBBill(1).TxtBLine2
  End If
End Sub

Private Sub Form_Resize()
  If Me.Visible Then
    Temp_Class.ResizeControls Me
    DoEvents
  End If
End Sub

Private Sub Savebill()
  Dim UBBill As UBBill21LineType
  Dim Handle As Integer, UBBill21Linelen As Integer
  UBBill21Linelen = Len(UBBill)
  Handle = FreeFile
  Open UBPath$ + "UBBil21L.DAT" For Random Shared As Handle Len = UBBill21Linelen
    If fpcboMorD.ListIndex = 0 Then
      UBBill.MsgORdates = "M"
    Else
      UBBill.MsgORdates = "D"
    End If
    UBBill.TxtLine1 = fpText1
    UBBill.TxtLine2 = fpText2
    UBBill.TxtLine3 = fpText3
    UBBill.TxtLine4 = fpText4
    UBBill.TxtLine5 = fpText5
    UBBill.TxtLine6 = fpText6
    UBBill.TxtLine7 = fptext7
    UBBill.TxtLine8 = fptext8
    UBBill.TxtLine9 = fptext9
    
    UBBill.TxtLine11 = fpText11
    UBBill.TxtBLine1 = fptextB1
    UBBill.TxtBLine2 = fptextB2
  Put #Handle, 1, UBBill
  Close Handle
  MsgBox "Save Complete", vbOKOnly, "Saved"
End Sub


