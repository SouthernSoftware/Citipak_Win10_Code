Dim changeFlag As Integer

Private Sub cmdExit_Click()
   changeFlag = 0
   Dim DoWhatFlag As SaveChangeOptions1, x As Integer
   Dim RetFileHandle As Integer, FileLen As Integer
   Dim RetFileRec As RetireRecType
   
   OpenRetFile RetFileHandle
   For x = 1 To 6
      Get RetFileHandle, x, RetFileRec
      If QPTrim$(RetFileRec.TYPEDES1) <> QPTrim$(txtDescrip(x).Text) Then
      'if it has changed then set the changeFlag to 1 and reset focus to
      'where the change was made
        changeFlag = 1
        txtDescrip(x).SetFocus
        Exit For
      End If
      If RetFileRec.TYPEWH1 <> Val(fptxtWH(x).Text) Then
        changeFlag = 1
        fptxtWH(x).SetFocus
        Exit For
      End If
      If RetFileRec.TYPEM1 <> Val(fptxtMatch(x).Text) Then
        changeFlag = 1
        fptxtMatch(x).SetFocus
        Exit For
      End If
      If QPTrim$(RetFileRec.TYPEOT1) <> QPTrim$(fpComboOT(x).Text) Then
        changeFlag = 1
        fpComboOT(x).SetFocus
        Exit For
      End If
      If QPTrim$(RetFileRec.TYPETD1) <> QPTrim$(fpComboTax(x).Text) Then
        changeFlag = 1
        fpComboTax(x).SetFocus
        Exit For
      End If
   Next x
   Close RetFileHandle
   'check each textbox to see if a change has been made
   
   'if no changes were made then move back to control menu
   If changeFlag = 0 Then 'no changes detected
     frmControlFileMaint.Show
     Unload frmRetireFileMaint
     GoTo endClick
   'if a change was made then bring up a warning window that forces
   'the user to decide whether to save, review or abandon changes
   Else
     DoWhatFlag = PromptSaveChanges(Me)
     Select Case DoWhatFlag
     Case SaveChangeOptions1.scoSaveChanges 'save changes
       Call cmdSave_Click
     Case SaveChangeOptions1.scoReviewChanges 'review is just bringing back the current form
     Case SaveChangeOptions1.scoAbandonChanges 'abandon
       frmControlFileMaint.Show
       Unload frmRetireFileMaint
     Case Else:
      'Do nothing because we don't know about any options except
      'save, review or abandon...used as a placeholder for adding
      'other options at a later date
     End Select
      
   End If
endClick:

End Sub

Private Sub cmdPrintForm_Click()
  PrintForm
End Sub

Private Sub cmdSave_Click()
   Dim RetHandle As Integer, x As Integer
   Dim RetFileRec(1 To 6) As RetireRecType
   Dim TempRet As RetireRecType, BlankRetRec As RetireRecType
   Dim ThisRec As Integer
   changeFlag = 0
   
   For x = 1 To 6
     RetFileRec(x).TYPEDES1 = QPTrim$(txtDescrip(x).Text)
       If Len(QPTrim$(RetFileRec(x).TYPEDES1)) <> 0 Then 'this row has a description
         If Val(fptxtWH(x).Text) <= 0 Then 'if both withholding and Match are empty
         'then alert user to enter a value for one or both
           If Val(fptxtMatch(x).Text) <= 0 Then
             MsgBox "Please enter a value for Withholding Rate or a value for Matching Rate."
             fptxtWH(x).SetFocus
             Exit Sub
           End If
         ElseIf QPTrim$(fpComboOT(x).Text) = "" Then 'this column must have a value
           MsgBox "Please enter a Y or N for Include Overtime."
           fpComboOT(x).SetFocus
           Exit Sub
         ElseIf QPTrim$(fpComboTax(x).Text) = "" Then 'this column must have a value
           MsgBox "Please enter a Y or N for Tax Deferred."
           fpComboTax(x).SetFocus
           Exit Sub
         End If
       ElseIf Len(QPTrim$(RetFileRec(x).TYPEDES1)) = 0 Then 'this row does not have a description
         If Val(fptxtWH(x).Text) > 0 Then 'if there us a value for withholding
         'then there must be a description
           MsgBox "Please enter a value for Retirement Description."
           txtDescrip(x).SetFocus
           Exit Sub
         ElseIf Val(fptxtMatch(x).Text) > 0 Then 'if there is a value for match
         'then there must be a description
           MsgBox "Please enter a value for Retirement Description."
           txtDescrip(x).SetFocus
           Exit Sub
         ElseIf QPTrim$(fpComboOT(x).Text) <> "" Then 'if there is a value
         'here then there must be a description
         'this was changed from = "" on 8/23 because starting then
         'rows with no descriptions were no longer loaded with Y or N
         'to make the screen cleaner
           MsgBox "Please enter a value for Retirement Description."
           txtDescrip(x).SetFocus
           Exit Sub
         ElseIf QPTrim$(fpComboTax(x).Text) <> "" Then 'if there is
         'a value here then there must be a description
         'this was changed from = "" on 8/23 because starting then
         'rows with no descriptions were no longer loaded with Y or N
         'to make the screen cleaner
           MsgBox "Please enter a value for Retirement Description."
           txtDescrip(x).SetFocus
           Exit Sub
         End If
       End If
     RetFileRec(x).TYPEWH1 = Val(fptxtWH(x).Text)
     RetFileRec(x).TYPEM1 = Val(fptxtMatch(x).Text)
     RetFileRec(x).TYPEOT1 = QPTrim$(fpComboOT(x).Text)
     RetFileRec(x).TYPETD1 = QPTrim$(fpComboTax(x).Text)
   Next
      
   'this for loop checks to see if a new earnings value
   'has been saved but saved on a row that is not the
   'next empty row
   For x = 1 To 6
     If Len(QPTrim$(RetFileRec(x).TYPEDES1)) = 0 Then 'found
     'an empty row
       RetFileRec(x) = BlankRetRec 'load array with empty
       'retirement type
       ThisRec = x 'spot ThisRec as this position
       Do
         ThisRec = ThisRec + 1 'go to the next row
         If ThisRec = 7 Then Exit Do 'exit if we're past the last row
         If Len(QPTrim$(RetFileRec(ThisRec).TYPEDES1)) Then 'oops, we
         'found data after an empty row
           LSet TempRet = RetFileRec(ThisRec) 'TempRet gets this
           'new data
           LSet RetFileRec(ThisRec) = RetFileRec(x) 'RetFileRec in
           'the new row gets the empty data from the previous row
           LSet RetFileRec(x) = TempRet 'now the empty row
           'gets the new data
           Exit Do
         End If
       Loop 'keep looping until row 6 has been examined
     End If
   Next
   OpenRetFile RetHandle
   For x = 1 To 6
     Put RetHandle, x, RetFileRec(x)
   Next
   Close RetHandle

   MsgBox "Your Information has been saved.", vbOKOnly
   frmControlFileMaint.Show
   DoEvents
   Unload frmRetireFileMaint
NoRetFileYet:
   MainLog ("Retirement data saved in Retirement File Maintenance.")
End Sub
