Private Sub LoadThisForm()
'   On Error Resume Next
   Dim RetHandle As Integer, x As Integer, Count As Integer
   Dim RetFileRec As RetireRecType
   Dim NumOfRetRecs As Integer
   Dim FileSize As Long
   
   OpenRetFile RetHandle
   NumOfRetRecs = LOF(RetHandle) / Len(RetFileRec)
   If NumOfRetRecs = 0 Then
     For x = 1 To 6
       fpComboOT(x).AddItem "Y"
       fpComboOT(x).AddItem "N"
       fpComboTax(x).AddItem "Y"
       fpComboTax(x).AddItem "N"
     Next
      'file is zero bytes
     Close
     GoTo NoRetFileYet
   End If
   For x = 1 To 6
     Get RetHandle, x, RetFileRec
   'load form info
   '8/23 changed this procedure so that rows without
   'a description no longer get data
     If Len(QPTrim$(RetFileRec.TYPEDES1)) > 0 Then
       txtDescrip(x).Text = QPTrim$(RetFileRec.TYPEDES1)
       fptxtWH(x).Text = Using$("##0.000", RetFileRec.TYPEWH1)
       fptxtMatch(x).Text = Using$("##0.000", RetFileRec.TYPEM1)
       If QPTrim$(RetFileRec.TYPEOT1) = "" Then RetFileRec.TYPEOT1 = "Y"
       fpComboOT(x).Text = QPTrim$(RetFileRec.TYPEOT1)
       If QPTrim$(RetFileRec.TYPETD1) = "" Then RetFileRec.TYPETD1 = "Y"
       fpComboTax(x).Text = QPTrim$(RetFileRec.TYPETD1)
     End If
     fpComboOT(x).AddItem "Y"
     fpComboOT(x).AddItem "N"
     fpComboTax(x).AddItem "Y"
     fpComboTax(x).AddItem "N"
   Next x
   Close RetHandle
      
NoRetFileYet:
   MainLog ("Retirement File Maintenance loaded.")
End Sub

Private Sub fpComboOT_KeyDown(Index As Integer, KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeySpace Then
    fpComboOT(Index).ListDown = True
  End If
  If fpComboOT(Index).ListDown = False Then
    If KeyCode = vbKeyDown Then
      SendKeys "{Tab}"
      KeyCode = 0
    ElseIf KeyCode = vbKeyUp Then
      SendKeys "+{Tab}"
      KeyCode = 0
    End If
  End If
  If KeyCode = vbKeyDelete Then fpComboOT(Index).Text = "" 'added 8/23
End Sub

Private Sub fpComboOT_LostFocus(Index As Integer)
  fpComboOT(Index).Action = ActionClearSearchBuffer
End Sub

Private Sub fpComboTax_KeyDown(Index As Integer, KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeySpace Then
    fpComboTax(Index).ListDown = True
  End If
  If fpComboTax(Index).ListDown = False Then
    If KeyCode = vbKeyDown Then
      If Index = 6 Then
        txtDescrip(1).SetFocus
        KeyCode = 0
      Else
        SendKeys "{Tab}"
        KeyCode = 0
      End If
    ElseIf KeyCode = vbKeyUp Then
      SendKeys "+{Tab}"
      KeyCode = 0
    End If
  End If
  If KeyCode = vbKeyDelete Then fpComboTax(Index).Text = "" 'added 8/23
End Sub

Private Sub fpComboTax_LostFocus(Index As Integer)
