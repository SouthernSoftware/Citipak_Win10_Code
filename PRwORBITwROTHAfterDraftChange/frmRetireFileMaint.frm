End Sub

Private Sub fptxtMatch_LostFocus(Index As Integer)
  If QPTrim$(fptxtMatch(Index).Text) = "" Then Exit Sub
  
  If CheckFor2ManyDecimals(fptxtMatch(Index).Text) = True Then
    MsgBox "Invalid number entered"
    fptxtMatch(Index).SetFocus
    Exit Sub
  End If
  
  fptxtMatch(Index).Text = Using$("##0.000", fptxtMatch(Index).Text)

End Sub

