  fpcmbBMonth.Text = "JAN"
  fpcmbBMonth.AddItem "JAN"
  fpcmbBMonth.AddItem "FEB"
  fpcmbBMonth.AddItem "MAR"
  fpcmbBMonth.AddItem "APR"
