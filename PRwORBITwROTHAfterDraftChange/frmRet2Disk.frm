Option Explicit

Private Sub cmdExit_Click()
  fptxtHide.Text = "Exit"
  Me.Hide
End Sub

Private Sub cmdProcess_Click()
  If Check4ValidDates = False Then Exit Sub
  fptxtHide.Text = fpcmbBMonth.Text + fpcmbEMonth.Text + fptxtYear.Text
  Me.Hide
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyEscape:
      SendKeys "%C"
      Call cmdExit_Click
      KeyCode = 0
    Case vbKeyF10:
      SendKeys "%P"
      Call cmdProcess_Click
      KeyCode = 0
    Case Else:
  End Select

End Sub

