  fptxtHide.Visible = False
End Sub

Private Function Check4ValidDates() As Boolean
  Dim ThisMonth$
  Dim MonthInt As Integer
  Dim BMonth As Integer
  Dim EMonth As Integer
  
  Check4ValidDates = True
  
  ThisMonth = fpcmbBMonth.Text
  GoSub GetMonthInt
  BMonth = MonthInt
  ThisMonth = fpcmbEMonth.Text
  GoSub GetMonthInt
  EMonth = MonthInt
  If BMonth > EMonth Then
    Check4ValidDates = False
    MsgBox "The beginning month comes after the ending month. Please re-enter the month values."
    fpcmbBMonth.SetFocus
    Close
    Exit Function
  End If
  
  If Val(fptxtYear.Text) < 1980 Or Val(fptxtYear.Text) > 2099 Then
    Check4ValidDates = False
    MsgBox "Please enter a year after 1979 and before 2099."
    fptxtYear.SetFocus
    Close
    Exit Function
  End If
  
  Exit Function
  
GetMonthInt:
  Select Case ThisMonth
    Case "JAN"
      MonthInt = 1
    Case "FEB"
      MonthInt = 2
    Case "MAR"
      MonthInt = 3
    Case "APR"
      MonthInt = 4
    Case "MAY"
      MonthInt = 5
    Case "JUN"
      MonthInt = 6
    Case "JUL"
      MonthInt = 7
    Case "AUG"
      MonthInt = 8
    Case "SEP"
      MonthInt = 9
    Case "OCT"
      MonthInt = 10
    Case "NOV"
      MonthInt = 11
    Case "DEC"
      MonthInt = 12
  End Select
  
  Return
End Function

