  fpcmbBMonth.AddItem "JUN"
  fpcmbBMonth.AddItem "JUL"
  fpcmbBMonth.AddItem "AUG"
  fpcmbBMonth.AddItem "SEP"
  fpcmbBMonth.AddItem "OCT"
  fpcmbBMonth.AddItem "NOV"
  fpcmbBMonth.AddItem "DEC"
  fpcmbEMonth.Text = "DEC"
  fpcmbEMonth.AddItem "JAN"
  fpcmbEMonth.AddItem "FEB"
  fpcmbEMonth.AddItem "MAR"
  fpcmbEMonth.AddItem "APR"
  fpcmbEMonth.AddItem "MAY"
