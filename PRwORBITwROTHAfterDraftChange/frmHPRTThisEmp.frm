Private Sub cmdDistOT_Click()
  Dim TotHrs As Double
  Dim Pct As Double
  Dim x As Integer
  Dim TotOTHrs As Double
  Dim DotSpot As Integer
  Dim ThisChr$
  Dim ThisLen As Integer
  Dim y As Integer, z As Integer
  Dim thisNum$
  Dim Num1 As Integer
  Dim Num2 As Integer
  Dim OTTotPct As Double
  Dim Dif As Double
  
  TotHrs = CDbl(fptxtDistTotRegHrs.Text)
  If TotHrs = 0 Then
    MsgBox "There are no regular hours from which to gather distribution percentages. Distribution attempt aborted."
    Exit Sub
  End If
    
  TotOTHrs = CDbl(fptxtOTHrsPaid.Text)
  
  ReDim ThisPct(1 To 8) As Double
  For x = 1 To 8
    Pct = CDbl(fptxtRegHrs(x).Text)
    ThisPct(x) = Pct / TotHrs
  Next x
  
  For x = 1 To 8
    fptxtOTHrs(x).Text = CStr(OldRound(TotOTHrs * ThisPct(x)))
  Next x
  
  For x = 1 To 8
    ThisLen = Len(QPTrim$(fptxtOTHrs(x).Text))
    thisNum = QPTrim$(fptxtOTHrs(x).Text)
    DotSpot = 0
    For y = 1 To ThisLen
      ThisChr = Mid(thisNum, y, 1)
      If ThisChr = "." Then
        DotSpot = y
        ThisLen = DotSpot + 2
        fptxtOTHrs(x).Text = Mid(fptxtOTHrs(x).Text, 1, ThisLen)
        If ThisLen - Len(fptxtOTHrs(x).Text) = 1 Then
          fptxtOTHrs(x).Text = fptxtOTHrs(x).Text + "0"
        End If
        Exit For
      End If
    Next y
    If DotSpot = 0 Then
      fptxtOTHrs(x).Text = fptxtOTHrs(x).Text + ".00"
    End If
  Next x
  
  For x = 1 To 8
    OTTotPct = OTTotPct + CDbl(fptxtOTHrs(x).Text)
  Next x
  
  Dif = OldRound(TotOTHrs - OTTotPct)
  Dif = CDbl(fptxtOTHrs(1).Text) + Dif
 
  fptxtOTHrs(1).Text = CStr(Dif)
   For x = 1 To 8
    ThisLen = Len(QPTrim$(fptxtOTHrs(x).Text))
    thisNum = QPTrim$(fptxtOTHrs(x).Text)
    DotSpot = 0
    For y = 1 To ThisLen
      ThisChr = Mid(thisNum, y, 1)
      If ThisChr = "." Then
        DotSpot = y
        ThisLen = DotSpot + 2
        fptxtOTHrs(x).Text = Mid(fptxtOTHrs(x).Text, 1, ThisLen)
        If ThisLen - Len(fptxtOTHrs(x).Text) = 1 Then
          fptxtOTHrs(x).Text = fptxtOTHrs(x).Text + "0"
        End If
        Exit For
      End If
    Next y
