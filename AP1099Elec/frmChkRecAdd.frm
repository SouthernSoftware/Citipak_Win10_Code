Dim CDActive As String, CashAcct As String, CDCash As String, CDDue As String
Dim NogoMsg As String
Private Sub cmdExit_Click()
  If Not Exist("APCHKINF.DAT") Then
    If InEdit = True Then
      If MsgBox("Abandon Changes?", vbYesNo, "Abandon?") = vbNo Then
        Exit Sub
        fpcboBanks.SetFocus
      End If
    End If
    frmBankReconMenu.Show
    frmBankReconMenu.SortCheckFile
    KillFileD "crchek.opn"
    Unload frmChkRecAdd
  Else
    MsgBox "UnPosted Check File Exists - Changes Abandoned.", vbOKOnly, "Canceled"
    frmBankReconMenu.Show
    KillFileD "crchek.opn"
    Unload frmChkRecAdd
  End If
End Sub
Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    If MsgBox("Are You Sure You Wish To Close Program?", vbYesNo, "Close?") = vbNo Then
      Cancel = True
    Else
      If Not Exist("APCHKINF.DAT") Then
        If InEdit = True Then
          If MsgBox("Abandon Changes and Close Program?", vbYesNo, "Abandon?") = vbNo Then
            Cancel = True
          End If
        End If
      Else
        MsgBox "UnPosted Check File Exists.", vbOKOnly, "Changes Abandoned"
      End If
      KillFileD "crchek.opn"
      ClearInUse PWcnt
    End If
  End If
End Sub

Private Sub fpcboSource_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeySpace Then
    fpcboSource.ListDown = True
  End If
  If KeyCode = vbKeyDelete Then
    fpcboSource.ListIndex = -1
    fpcboSource.Action = ActionClearSearchBuffer
  End If
  If fpcboSource.ListDown <> True Then
    If KeyCode = vbKeyDown Then
      SendKeys "{Tab}"
      KeyCode = 0
    Else
      If KeyCode = vbKeyUp Then
        SendKeys "+{Tab}"
        KeyCode = 0
      End If
    End If
  End If
End Sub

Private Sub fpcboBanks_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeySpace Then
    fpcboBanks.ListDown = True
  End If
  If KeyCode = vbKeyDelete Then
    fpcboBanks.ListIndex = -1
    fpcboBanks.Action = ActionClearSearchBuffer
  End If
  If fpcboBanks.ListDown <> True Then
    If KeyCode = vbKeyDown Then
      SendKeys "{Tab}"
      KeyCode = 0
    Else
      If KeyCode = vbKeyUp Then
        SendKeys "+{Tab}"
        KeyCode = 0
      End If
    End If
  End If
End Sub

Private Sub fpDate_LostFocus()
  If CheckValDate(fpDate) = False Then
    MsgBox "Invalid Date, Please Correct.", vbOKOnly, "Invalid Date"
    fpDate.SetFocus
  End If
End Sub

Private Sub cmdSave_Click()
  If Not Exist("APCHKINF.DAT") Then
    If InEdit = True Then
      AddChecks2File
      MsgBox "Check saved.", vbOKOnly, "Saved"
      ClrScreen
    Else
      MsgBox NogoMsg, vbOKOnly, "Incomplete Info."
    End If
    fpcboBanks.SetFocus
  Else
    MsgBox "UnPosted Check File Exists - Editing Check Reconciliation NOT Allowed.", vbOKOnly, "Canceled"
    frmBankReconMenu.Show
    KillFileD "crchek.opn"
    Unload frmChkRecAdd
  End If
End Sub
Private Function InEdit()
  InEdit = False
  If fpcboBanks.ListIndex <> -1 Then
    If fptxtChkNum.Text <> "" Then
      If CheckValDate(fpDate) = True Then
        If txtPayee <> "" Then
          If fpchkamt <> 0 Then
            If fpcboSource.ListIndex <> -1 Then
              InEdit = True
            Else
              NogoMsg = "You Must Select AP or Payroll Source"
              fpcboSource.SetFocus
            End If
          Else
            NogoMsg = "Check Amount May Not Be 0"
            fpchkamt.SetFocus
          End If
        Else
          NogoMsg = "Please Enter the Payee"
          txtPayee.SetFocus
        End If
      Else
        NogoMsg = "Date is Not Within Acceptable Range"
        fpDate.SetFocus
      End If
    Else
      NogoMsg = "Please Enter a Check Number"
      fptxtChkNum.SetFocus
    End If
  Else
    NogoMsg = "Bank Code Is Required"
    fpcboBanks.SetFocus
  End If
End Function

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyUp:
      SendKeys "+{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%X"
      KeyCode = 0
    Case vbKeyF10:
      SendKeys "%S"
      KeyCode = 0
    Case Else:
  End Select
End Sub

