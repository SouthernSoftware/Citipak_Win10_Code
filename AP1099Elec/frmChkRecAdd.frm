Private Sub AddChecks2File()
  Dim ccnt As Integer, OSChekFile As Integer, OSChkDate As String
  Dim NumOSChks As Integer, T As String, Tag As String, chkdate As String
  Dim ChkDat As Integer, Src As String, NextChkRecs As Long
  Dim tempstr As String, TempAmt As String, ChkAmt As Double
  Dim OSChek As OSChekRecType
  OpenOSChekFile OSChekFile, NumOSChks
  NextChkRecs = NumOSChks + 1
  fpcboBanks.Col = 0
  OSChek.Bankcode = QPTrim(fpcboBanks.ColText)
  OSChek.ChkNum = QPTrim(fptxtChkNum)
  chkdate$ = fpDate.Text
  ChkDat = DateDiff("d", "12/31/1979", fpDate.Text)
  OSChek.chkdate = ChkDat
  OSChek.Desc = QPTrim$(txtPayee)
  ChkAmt = fpchkamt.DoubleValue
  OSChek.Amt = ChkAmt
  Src$ = Left$(QPTrim$(fpcboSource.Text), 1)

    If Src$ = "A" Then
      OSChek.Src = 0
    Else
      OSChek.Src = 1
    End If
  

  Put OSChekFile, NextChkRecs, OSChek
  Close OSChekFile
  Call MainLog("ChkRecAdd Chk#: " + QPTrim(fptxtChkNum))
End Sub






