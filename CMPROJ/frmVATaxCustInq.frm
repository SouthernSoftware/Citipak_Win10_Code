Private Sub cmdPropDet_Click()
  Dim ThisRec As Long, ThisDesc$
  Dim RealPropRec As VAPropertyRecType
  Dim RHandle As Integer
  Dim NumOfRealRecs As Long
  Dim PersPropRec As VAPersonalRecType
  Dim PHandle As Integer
  Dim NumOfPersRecs As Long
  
  On Local Error GoTo ERRORSTUFF
  
  If fpList.ListIndex <> -1 Then
    fpList.col = 0
    fpList.Row = fpList.ListIndex
    ThisDesc = QPTrim$(fpList.ColText)
    If ThisDesc = "No Real Property Listed" Or ThisDesc = "No Personal Property Listed" Then
      Call VATaxMsg(900, "No details available for selection.")
      Exit Sub
    End If
    fpList.col = 2
    ThisRec = CLng(fpList.ColText)
  Else
    Call VATaxMsg(900, "Please make a property selection.")
    Exit Sub
  End If
  
  If InStr(ThisDesc, "Personal") Then
    OpenVAPersPropFile PHandle, NumOfPersRecs
    Get PHandle, ThisRec, PersPropRec
    Close PHandle
    PersRec = ThisRec
    frmVATaxPersDetail.Show vbModal
  ElseIf InStr(ThisDesc, "Real") Then
    OpenVARealPropFile RHandle, NumOfRealRecs
    Get RHandle, ThisRec, RealPropRec
    Close RHandle
    RealRec = ThisRec
    frmVATaxRealDetail.Show vbModal
  End If
  
  Exit Sub

ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmVATaxCustInq", "cmdPropDet", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close

  
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%E"
      Call cmdExit_Click
      KeyCode = 0
    Case vbKeyF4:
      SendKeys "%H"
      Call cmdHistory_Click
      KeyCode = 0
    Case vbKeyF2:
      SendKeys "%M"
      Call cmdMessage_Click
      KeyCode = 0
    Case vbKeyF3:
      SendKeys "%D"
      Call cmdPropDet_Click
      KeyCode = 0
    Case vbKeyF7:
      SendKeys "%L"
      Call cmdLookup_Click
      KeyCode = 0
    Case Else:
  End Select
End Sub

Private Sub Form_Load()
  Dim One As Integer
  Dim AHandle As Integer
  
  One = 1
  AHandle = FreeFile
  Open "C:\CPWork\custinq.dat" For Output As AHandle
  Print #AHandle, One
  Close AHandle
  
  ScrWidth = Screen.Width / Screen.TwipsPerPixelX
  Set Over = New clsTextBoxOverRider
  Over.OverRide Me
  Set Temp_Class = New Resize_Class
  Temp_Class.InitResizeClass Me
  ScreenW = (Screen.Width / Screen.TwipsPerPixelX)
  If Exist("C:\CPWork\txrealpyment.dat") Or Exist("C:\CPWork\txperspyment.dat") Or Exist("c:\CPWork\prepayrefund.dat") Then
    cmdLookup.Visible = False
    fptxtAcctNum.ControlType = ControlTypeReadOnly
  End If
End Sub

Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    If cmdExit.Enabled = False Then
      Cancel = True
    ElseIf MsgBox("Are You Sure You Wish To Close Program?", vbYesNo, "Close?") = vbNo Then
      Cancel = True
    Else
      KillFile ("C:\CPWork\custinq.dat")
      TXLog ("CitiTaxes.exe terminated via menu bar on frmVATaxCustInq.")
      Call CMTerminate
      End
    End If
  End If

End Sub
