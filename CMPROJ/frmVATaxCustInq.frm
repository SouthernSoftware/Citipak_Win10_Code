Public Sub LoadCust()
  Dim TaxCust As VATaxCustType
  Dim TCHandle As Integer
  Dim NumOfTCRecs As Long
  Dim ThisPhone$
  Dim NewPhone$
  Dim ThisLen As Integer
  Dim x As Integer
  Dim RealPropRec As VAPropertyRecType
  Dim RHandle As Integer
  Dim NumOfRealRecs As Long
  Dim PersPropRec As VAPersonalRecType
  Dim PHandle As Integer
  Dim NumOfPersRecs As Long
  Dim TaxMasterRec As VATaxMasterType
  Dim TMHandle As Integer
  Dim ThisRec As Long
  
  On Local Error GoTo ERRORSTUFF
  
  OpenVATaxSetUpFile TMHandle
  Get TMHandle, 1, TaxMasterRec
  Close TMHandle
  
  Town = QPTrim$(TaxMasterRec.Name)
  
  If GCustNum = 0 Then
    Call VATaxMsg(900, "ERROR: The customer data could not be loaded. Please try again.")
    Close TCHandle
    Exit Sub
  End If
  
  If CustHasMsg(GCustNum) Then
    MsgAlertTimer.Enabled = True
  Else
    MsgAlertTimer.Enabled = False
    cmdMessage.ForeColor = &H80000012
  End If
  
  OpenVATaxCustFile TCHandle, NumOfTCRecs
  Get TCHandle, GCustNum, TaxCust
  Close TCHandle
  fptxtAcctNum.Text = CStr(TaxCust.Acct)
  fptxtName.Text = QPTrim$(TaxCust.CustName)
  fptxtOpenDate.Text = MakeRegDate(TaxCust.OPENDATE)
  fptxtHomePhone.Text = GetPhoneNum(TaxCust.HPHONE)
  fptxtWorkPhone.Text = GetPhoneNum(TaxCust.WPHONE)
  Call InsertSSNDashes(TaxCust.CSSN)
  
  fptxtSocSec.Text = TaxCust.CSSN
  
  fpCurrBalance = VAGetCustBalance(GCustNum, -1)
  fptxtDrvrsLic.Text = TaxCust.DrvrsLic
  fptxtAdd1.Text = QPTrim$(TaxCust.Addr1)
  fptxtAdd2.Text = QPTrim$(TaxCust.Addr2)
  fptxtCity.Text = QPTrim$(TaxCust.City)
  fptxtState.Text = TaxCust.State
  fptxtZip.Text = QPTrim$(TaxCust.Zip)
  fptxtActiveYN.Text = TaxCust.Active
'  fptxtChrgPenYN.Text = TaxCust.Penalty
  fptxtChrgIntYN.Text = TaxCust.Interest
  fptxtLateNotYN.Text = TaxCust.LateNotice
  fptxtTaxExYN.Text = TaxCust.TaxExempt
  
  fpList.Clear
  ThisRec = TaxCust.FirstPropRec
  If ThisRec > 0 Then
    OpenVARealPropFile RHandle, NumOfRealRecs
    Do While ThisRec > 0
      Get RHandle, ThisRec, RealPropRec
      fpList.InsertRow = "Real Property" + Chr(9) + QPTrim$(RealPropRec.RealPin) + Chr(9) + CStr(ThisRec)
      ThisRec = RealPropRec.NextRec
    Loop
  Else
    fpList.InsertRow = "No Real Property Listed"
  End If
  Close RHandle
  
  ThisRec = TaxCust.FirstPersRec
  If ThisRec > 0 Then
    OpenVAPersPropFile PHandle, NumOfPersRecs
    Do While ThisRec > 0
      Get PHandle, ThisRec, PersPropRec
      fpList.InsertRow = "Personal Property" + Chr(9) + QPTrim$(PersPropRec.PropPin) + Chr(9) + CStr(ThisRec)
      ThisRec = PersPropRec.NextRec
    Loop
  Else
    fpList.InsertRow = "No Personal Property Listed"
  End If
  Close PHandle
  TempAcctNum = CLng(fptxtAcctNum.Value)
  
  Exit Sub
  
GetPhoneNum:
  
  ThisPhone$ = ReplaceString(TaxCust.HPHONE, "-", "")
  NewPhone = ""
  ThisLen = Len(ThisPhone)
  If ThisLen = 10 Then
    For x = 1 To 12
      If x = 4 Or x = 8 Then
        NewPhone = NewPhone + "-"
      Else
        ThisPhone = NewPhone + Mid(ThisPhone, x, 1)
      End If
    Next x
  ElseIf ThisLen = 7 Then
    For x = 1 To 12
      If x <= 3 Then
        NewPhone = NewPhone + "0"
      ElseIf x = 4 Or x = 8 Then
        NewPhone = NewPhone + "-"
      ElseIf x <= 7 Then
        NewPhone = NewPhone + Mid(ThisPhone, x - 4, 1)
      Else
        NewPhone = NewPhone + Mid(ThisPhone, x - 5, 1)
      End If
    Next x
  End If
    
  fptxtHomePhone.Text = NewPhone
  
  ThisPhone$ = ReplaceString(TaxCust.WPHONE, "-", "")
  NewPhone = ""
  ThisLen = Len(ThisPhone)
  If ThisLen = 10 Then
    For x = 1 To 12
      If x = 4 Or x = 8 Then
        NewPhone = NewPhone + "-"
      Else
        ThisPhone = NewPhone + Mid(ThisPhone, x, 1)
      End If
    Next x
  ElseIf ThisLen = 7 Then
    For x = 1 To 12
      If x <= 3 Then
        NewPhone = NewPhone + "0"
      ElseIf x = 4 Or x = 8 Then
        NewPhone = NewPhone + "-"
      ElseIf x <= 7 Then
        NewPhone = NewPhone + Mid(ThisPhone, x - 4, 1)
      Else
        NewPhone = NewPhone + Mid(ThisPhone, x - 5, 1)
      End If
    Next x
  End If
  
  fptxtHomePhone.Text = GetPhoneNum(TaxCust.HPHONE)
  fptxtWorkPhone.Text = GetPhoneNum(TaxCust.WPHONE)
  
  Return


ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmVATaxCustInq", "LoadCust", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close

End Sub

Private Sub fptxtAcctNum_LostFocus()
  If CLng(fptxtAcctNum.Text) = 0 Then Exit Sub
'  If TempAcctNum = CLng(fptxtAcctNum.Value) Then Exit Sub
  If VACheck4ValidCustNum(CLng(fptxtAcctNum.Text)) = True Then
    GCustNum = CLng(fptxtAcctNum.Value)
    Call LoadCust
  Else
    TempAcctNum = CLng(fptxtAcctNum.Value)
    Call VATaxMsg(900, "The account number entered is not valid.")
    Call Clearscreen
    fptxtAcctNum.SetFocus
  End If
  
End Sub

Private Function VACheck4ValidCustNum(ThisCust As Long) As Boolean
  Dim TaxRec As VATaxCustType
  Dim CHandle As Integer
  Dim NumOfCRecs As Long
  Dim x As Long
  Dim Number$
  Dim Name$
  Dim Found As Boolean
  
  On Local Error GoTo ERRORSTUFF
  VACheck4ValidCustNum = True
  
  If fptxtAcctNum.Value = 0 Then
    VACheck4ValidCustNum = False
    Exit Function
  End If
  
  OpenVATaxCustFile CHandle, NumOfCRecs
  
  If NumOfCRecs = 0 Then
    frmVATaxMsg.Label1.Caption = "There are no tax customers saved."
    frmVATaxMsg.Label1.Top = 900
    frmVATaxMsg.Show vbModal
    Close CHandle
    Exit Function
  End If
  
  For x = 1 To NumOfCRecs
    Get CHandle, x, TaxRec
    If ThisCust = TaxRec.Acct Then
      If TaxRec.Deleted <> 0 Then
        VACheck4ValidCustNum = False
      End If
      Exit For
    End If
  Next x

  Close CHandle

  If x > NumOfCRecs Then
    Call Clearscreen
    VACheck4ValidCustNum = False
  End If
  Exit Function

ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmVATaxCustInq", "VACheck4ValidCustNum", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close

  
End Function

