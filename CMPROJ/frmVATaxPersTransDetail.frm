Attribute VB_Name = "frmVATaxPersTransDetail"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

Private Sub cmdExit_Click()
  Unload Me
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%C"
      Call cmdExit_Click
      KeyCode = 0
    Case Else:
  End Select
End Sub

Private Sub Form_Load()
  Dim ThisRec As Long
  Dim TaxTRec As VATaxTransactionType
  Dim TaxTRec2 As VATaxTransactionType
  Dim TRHandle As Integer
  Dim TRHandle2 As Integer
  Dim NumOfTaxTRecs As Long
  Dim Opt1Desc As String * 12
  Dim Opt2Desc As String * 12
  Dim Opt3Desc As String * 12
  Dim Personal#
  Dim PersonalPd#
  Dim FarmEquip#
  Dim FarmEquipPd#
  Dim MachTools#
  Dim MachToolsPd#
  Dim MerchCap#
  Dim MerchCapPd#
  Dim MobHomes#
  Dim MobHomesPd#
  Dim Interest#
  Dim InterestPd#
  Dim Penalty#
  Dim PenaltyPd#
  Dim TType$
  Dim TaxYear$
  Dim BillType$
  Dim ThisBillType$
  Dim Post2GL$
  Dim BillNum$
  Dim TaxMasterRec As VATaxMasterType
  Dim MHandle As Integer
  Dim Disc As Double
  Dim Dif As Double
  
  On Local Error GoTo ERRORSTUFF
  
  If frmVATaxCustInfoTHist.BillType = "NA" Then
    Call VATaxMsg(900, "ERROR: There is a problem loading the detail screen. Please try again.")
    Exit Sub
  Else
    ThisBillType = frmVATaxCustInfoTHist.BillType
  End If
  
  If Exist("TAXSETUP.DAT") Then
    OpenVATaxSetUpFile MHandle
    Get MHandle, 1, TaxMasterRec
    Close MHandle
  End If
  
  frmVATaxCustInfoTHist.fpList1Pers.col = 3
  frmVATaxCustInfoTHist.fpList1Pers.Row = frmVATaxCustInfoTHist.fpList1Pers.ListIndex
  ThisRec = CLng(frmVATaxCustInfoTHist.fpList1Pers.ColText)
  
  OpenVATaxTransFile TRHandle, NumOfTaxTRecs
  Get TRHandle, ThisRec, TaxTRec
  fptxtTDate.Text = MakeRegDate(TaxTRec.TransDate)
  
  If TaxTRec.PPTRARmvl > 0 Then
    fpCurrPPTRARmvl.Visible = True
    Label18.Visible = True
    fpCurrPPTRARmvl = TaxTRec.PPTRARmvl
  End If
  
  fpCurrPPTRADisc = TaxTRec.PPTRADisc
  Personal# = 0
  MachTools# = 0
  MerchCap# = 0
  FarmEquip# = 0
  MobHomes# = 0
  Interest# = 0
  Penalty# = 0
  
  PersonalPd# = 0
  MachToolsPd# = 0
  MerchCapPd# = 0
  FarmEquipPd# = 0
  MobHomesPd# = 0
  InterestPd# = 0
  PenaltyPd# = 0
  
  TType$ = QPTrim$(TaxTRec.Description)
  
  If TaxTRec.BelongTo > 0 Then
    OpenVATaxTransFile TRHandle2, NumOfTaxTRecs
    Get TRHandle2, TaxTRec.BelongTo, TaxTRec2
    TaxYear$ = QPTrim$(Str$(TaxTRec2.TaxYear))
    Close TRHandle2
  End If
  
  BillType$ = ""
  TaxYear$ = "N/A"
  Post2GL$ = "N"
  If TaxTRec.Posted2GL = "Y" Then
    Post2GL$ = "Y"
  End If
  
  Select Case TaxTRec.TranType
  Case 1
    Select Case TaxTRec.BillType
    Case "R"
      BillType$ = "Real-Estate Bill"
    Case "P"
      BillType$ = "Personal Property Bill"
    Case "C"
      BillType$ = "Combined Bill"
    Case "M"
      BillType$ = "Manual Bill"
    End Select
    TaxYear$ = QPTrim$(Str$(TaxTRec.TaxYear))
  Case 2
    BillNum$ = ParseBillNum$(TaxTRec.Description)
    If Len(BillNum$) = 0 Then
      If QPTrim$(TaxTRec.Description) = "Prepay" Then
        BillType = "Prepayment"
      Else
        BillType$ = "Payment ??? "
      End If
    Else
      If TaxTRec.Revenue.PrePaidAmt > 0 Then
        BillType = "Pre/Payment on: "
      Else
        BillType$ = "Payment on: "
      End If
    End If
    BillType$ = BillType$ + BillNum$
  Case 3
    BillType$ = "Release"
  Case 4
    BillType$ = "Interest"
    Interest# = TaxTRec.Revenue.Interest#
  Case 5
    BillType$ = "Penalty"
  Case 6
    BillType$ = "Collection/Ad Cost"
  Case 7
    BillType$ = "Adjust Paid Down"
  Case 9
    BillType$ = "Credit Applied at Billing"
  Case 13
    BillType$ = "Adjust Bill Down"
  Case 14
    BillType$ = "Adjust Bill Up"
  Case 21
    BillType$ = "Paid Bill Plus Prepay"
  Case 22
    BillType$ = "Prepayment"
  Case 10
    BillType$ = "Adjust Pay Down Affecting Credit"
  Case 11
    BillType$ = "Adjust Prepay Down"
  Case 12
    BillType$ = "Refund Prepay"
  Case 24
    BillType$ = "Adjust Bill Up Affecting Credit"
  Case 30
    BillType$ = "PPTRA Discount Removed"
  Case Else
    BillType$ = Str$(TaxTRec.TranType) + "??"
    
  End Select
  
  If Len(TType$) = 0 Then
    TType$ = BillType$
  End If
  Disc = TaxTRec.DiscAmt
  Personal# = TaxTRec.Revenue.Principle1
  MachTools# = TaxTRec.Revenue.Principle2
  MerchCap# = TaxTRec.Revenue.Principle3
  FarmEquip# = TaxTRec.Revenue.Principle4
  MobHomes# = TaxTRec.Revenue.Principle5
  Interest# = TaxTRec.Revenue.Interest
  Penalty# = TaxTRec.Revenue.Penalty
  PersonalPd# = OldRound(TaxTRec.Revenue.Principle1Pd + TaxTRec.PPTRADisc - TaxTRec.PPTRARmvl)
  MachToolsPd# = TaxTRec.Revenue.Principle2Pd
  MerchCapPd# = TaxTRec.Revenue.Principle3Pd
  FarmEquipPd# = TaxTRec.Revenue.Principle4Pd
  MobHomesPd# = TaxTRec.Revenue.Principle5Pd
  InterestPd# = TaxTRec.Revenue.InterestPd
  PenaltyPd# = TaxTRec.Revenue.PenaltyPd
  If QPTrim$(TaxTRec.Posted2GL) <> "Y" Then TaxTRec.Posted2GL = "N"
  fptxtPosted2GL.Text = TaxTRec.Posted2GL
  fptxtDesc.Text = QPTrim$(TaxTRec.Description)
  If TaxTRec.TranType <> 9 Then
    fpCurrTAmt = OldRound(TaxTRec.Amount + TaxTRec.PPTRADisc)
  Else
    fpCurrTAmt = TaxTRec.Revenue.PrePaidUsed
  End If
  fptxtBillYear.Text = TaxYear$
  fptxtType.Text = BillType$
  fpCurrPers = Personal#
  fpCurrPersPaid = PersonalPd#
  fpCurrPersBal = OldRound(Personal# - PersonalPd#)
  fpCurrMT = MachTools#
  fpCurrMTPaid = MachToolsPd#
  fpCurrMTBal = OldRound(MachTools# - MachToolsPd#)
  fpCurrMC = MerchCap#
  fpCurrMCPaid = MerchCapPd#
  fpCurrMCBal = OldRound(MerchCap# - MerchCapPd#)
  fpCurrFE = FarmEquip#
  fpCurrFEPaid = FarmEquipPd#
  fpCurrFEBal = OldRound(FarmEquip# - FarmEquipPd#)
  fpCurrMH = MobHomes#
  fpCurrMHPaid = MobHomesPd#
  fpCurrMHBal = OldRound(MobHomes# - MobHomesPd#)
  fpCurrInt = Interest#
  fpCurrIntPaid = InterestPd#
  fpCurrIntBal = OldRound(Interest# - InterestPd#)
  fpCurrDisc = TaxTRec.DiscAmt
  fpCurrPen = Penalty#
  fpCurrPenPaid = PenaltyPd#
  fpCurrPenBal = OldRound(Penalty# - PenaltyPd#)
  Opt1Desc = QPTrim$(TaxMasterRec.POptRev1)
  Opt2Desc = QPTrim$(TaxMasterRec.POptRev2)
  Opt3Desc = QPTrim$(TaxMasterRec.POptRev3)

  lblRevOpt1.Caption = Opt1Desc
  fpCurrOptRev1 = TaxTRec.Revenue.RevOpt1
  fpCurrOptRev1Paid = TaxTRec.Revenue.RevOpt1Pd
  fpCurrOptRev1Bal = OldRound(CDbl(fpCurrOptRev1.Value) - CDbl(fpCurrOptRev1Paid.Value))
  
  lblRevOpt2.Caption = Opt2Desc
  fpCurrOptRev2 = TaxTRec.Revenue.RevOpt2
  fpCurrOptRev2Paid = TaxTRec.Revenue.RevOpt2Pd
  fpCurrOptRev2Bal = OldRound(CDbl(fpCurrOptRev2.Value) - CDbl(fpCurrOptRev2Paid.Value))
  
  lblRevOpt3.Caption = Opt3Desc
  fpCurrOptRev3 = TaxTRec.Revenue.RevOpt3
  fpCurrOptRev3Paid = TaxTRec.Revenue.RevOpt3Pd
  fpCurrOptRev3Bal = OldRound(CDbl(fpCurrOptRev3.Value) - CDbl(fpCurrOptRev3Paid.Value))
  fpCurrPrePay = TaxTRec.Revenue.PrePaidAmt
  Close TRHandle
  
  If Disc > 0 Then
    Dif = fpCurrPers.Value - fpCurrPersBal.Value
    If Dif >= Disc Then
      fpCurrPersPaid.Value = fpCurrPersPaid.Value + Disc
      fpCurrPersBal.Value = fpCurrPers.Value - fpCurrPersPaid.Value
      Disc = 0
      GoTo DiscGone
    ElseIf Dif <> 0 Then
      fpCurrPersPaid.Value = fpCurrPersPaid.Value + Disc
      fpCurrPersBal.Value = fpCurrPers.Value - fpCurrPersPaid.Value
      Disc = Disc - Dif
    End If

    Dif = fpCurrMT.Value - fpCurrMTBal.Value
    If Dif >= Disc Then
      fpCurrMTPaid.Value = fpCurrMTPaid.Value + Disc
      fpCurrMTBal.Value = fpCurrMT.Value - fpCurrMTPaid.Value
      Disc = 0
      GoTo DiscGone
    ElseIf Dif <> 0 Then
      fpCurrMTPaid.Value = fpCurrMTPaid.Value + Disc
      fpCurrMTBal.Value = fpCurrMT.Value - fpCurrMTPaid.Value
      Disc = Disc - Dif
    End If
  
    Dif = fpCurrMC.Value - fpCurrMCBal.Value
    If Dif >= Disc Then
      fpCurrMCPaid.Value = fpCurrMCPaid.Value + Disc
      fpCurrMCBal.Value = fpCurrMC.Value - fpCurrMCPaid.Value
      Disc = 0
      GoTo DiscGone
    ElseIf Dif <> 0 Then
      fpCurrMCPaid.Value = fpCurrMCPaid.Value + Disc
      fpCurrMCBal.Value = fpCurrMC.Value - fpCurrMCPaid.Value
      Disc = Disc - Dif
    End If
  
    Dif = fpCurrFE.Value - fpCurrFEBal.Value
    If Dif >= Disc Then
      fpCurrFEPaid.Value = fpCurrFEPaid.Value + Disc
      fpCurrFEBal.Value = fpCurrFE.Value - fpCurrFEPaid.Value
      Disc = 0
      GoTo DiscGone
    ElseIf Dif <> 0 Then
      fpCurrFEPaid.Value = fpCurrFEPaid.Value + Disc
      fpCurrFEBal.Value = fpCurrFE.Value - fpCurrFEPaid.Value
      Disc = Disc - Dif
    End If
  
    Dif = fpCurrMH.Value - fpCurrMHBal.Value
    If Dif >= Disc Then
      fpCurrMHPaid.Value = fpCurrMHPaid.Value + Disc
      fpCurrMHBal.Value = fpCurrMH.Value - fpCurrMHPaid.Value
      Disc = 0
      GoTo DiscGone
    ElseIf Dif <> 0 Then
      fpCurrMHPaid.Value = fpCurrMHPaid.Value + Disc
      fpCurrMHBal.Value = fpCurrMH.Value - fpCurrMHPaid.Value
      Disc = Disc - Dif
    End If
  
    Dif = fpCurrOptRev1.Value - fpCurrOptRev1Bal.Value
    If Dif >= Disc Then
      fpCurrOptRev1Paid.Value = fpCurrOptRev1Paid.Value + Disc
      fpCurrOptRev1Bal.Value = fpCurrOptRev1.Value - fpCurrOptRev1Paid.Value
      Disc = 0
      GoTo DiscGone
    ElseIf Dif <> 0 Then
      fpCurrOptRev1Paid.Value = fpCurrOptRev1Paid.Value + Disc
      fpCurrOptRev1Bal.Value = fpCurrOptRev1.Value - fpCurrOptRev1Paid.Value
      Disc = Disc - Dif
    End If
  
    Dif = fpCurrOptRev2.Value - fpCurrOptRev2Bal.Value
    If Dif >= Disc Then
      fpCurrOptRev2Paid.Value = fpCurrOptRev2Paid.Value + Disc
      fpCurrOptRev2Bal.Value = fpCurrOptRev2.Value - fpCurrOptRev2Paid.Value
      Disc = 0
      GoTo DiscGone
    ElseIf Dif <> 0 Then
      fpCurrOptRev2Paid.Value = fpCurrOptRev2Paid.Value + Disc
      fpCurrOptRev2Bal.Value = fpCurrOptRev2.Value - fpCurrOptRev2Paid.Value
      Disc = Disc - Dif
    End If
  
    Dif = fpCurrOptRev3.Value - fpCurrOptRev3Bal.Value
    If Dif >= Disc Then
      fpCurrOptRev3Paid.Value = fpCurrOptRev3Paid.Value + Disc
      fpCurrOptRev3Bal.Value = fpCurrOptRev3.Value - fpCurrOptRev3Paid.Value
      Disc = 0
      GoTo DiscGone
    ElseIf Dif <> 0 Then
      fpCurrOptRev3Paid.Value = fpCurrOptRev3Paid.Value + Disc
      fpCurrOptRev3Bal.Value = fpCurrOptRev3.Value - fpCurrOptRev3Paid.Value
      Disc = Disc - Dif
    End If
  End If
  
DiscGone:
  
  Exit Sub
  
ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmVATaxTransDetail", "Form_Load", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close
   ' ClearInUse PWcnt
    'Terminate
  
End Sub


