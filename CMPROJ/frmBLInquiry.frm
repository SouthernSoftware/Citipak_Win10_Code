Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
  Dim ScrWidth As Long
  Dim Over As clsBLTextBoxOverrider
  Private Temp_Class As Resize_Class

Private Sub cmdExit_Click()
  frmBLCustomerLookup.Show
  DoEvents
  Unload frmBLInquiry
End Sub

Private Sub cmdHistory_Click()
  Dim PrintType$
  
  If Not Exist("artrans.dat") Then
    frmBLMessageBoxJr.Label1.Caption = "No transactions records saved."
    frmBLMessageBoxJr.Label1.Top = 900
    frmBLMessageBoxJr.Show vbModal
    Exit Sub
  End If
  
  frmBLReportOpt.Show vbModal 'opens small screen from which the
  'user selects the printing method
  PrintType$ = frmBLReportOpt.fptxtPrintType
  Select Case PrintType$
    Case "Graphical"
      Call PrintGraphics
    Case "Text"
      frmBLMessageBoxJr.Label1.Caption = "Pitch 10 is recommended for this report."
      frmBLMessageBoxJr.Label1.Top = 900
      frmBLMessageBoxJr.Show vbModal
      Call PrintText
    Case "Exit"
  End Select
  Unload frmBLReportOpt

End Sub
Private Sub PrintText()
  Dim RunBalance#
  Dim Page As Integer
  Dim TransRec As ARTransRecType
  Dim ARTransFile As Integer
  Dim MaxLines As Integer
  Dim ReportFile$
  Dim RptHandle As Integer
  Dim TransRecd&
  Dim LineCnt As Integer
  Dim FF$
  Dim CustRec As ARCustRecType
  Dim CHandle As Integer
  
  On Error GoTo ERRORSTUFF
  FF$ = Chr$(12)
  RunBalance# = 0
  Page = 0
  OpenTransFile ARTransFile

  MaxLines = 58
  ReportFile$ = "ARHIST.PRN"
  RptHandle = FreeFile
  Open ReportFile$ For Output As #RptHandle

  OpenCustFile CHandle
  Get CHandle, GCustNum, CustRec
  Close CHandle
  
  GoSub PrintCustInqRptHeader
  
  TransRecd& = CustRec.FirstTrans

  Do While TransRecd& > 0
    Get ARTransFile, TransRecd&, TransRec

    If LineCnt >= MaxLines Then
      Print #RptHandle, FF$
      GoSub PrintCustInqRptHeader
    End If
    Print #RptHandle, MakeRegDate(TransRec.TransDate);
    Print #RptHandle, Tab(12); RTrim$(TransRec.TransDesc);
    Print #RptHandle, Tab(50); "";
    If TransRec.DetailTransType = 0 And TransRec.TransType > 0 Then
      Select Case TransRec.TransType
      Case 1
        Print #RptHandle, "Charge";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", TransRec.TransAmount);
      Case 2
        Print #RptHandle, "Payment";
        RunBalance# = RunBalance# - TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", -TransRec.TransAmount);
      Case 6
        Print #RptHandle, "Penalty";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", TransRec.TransAmount);
      Case 9
        Print #RptHandle, "Beg Bal";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", TransRec.TransAmount);
      Case 100
        Print #RptHandle, "DN Adj.";
        RunBalance# = RunBalance# - TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", -TransRec.TransAmount);
      Case 101
        Print #RptHandle, "UP Adj.";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", TransRec.TransAmount);
      Case Else
        Print #RptHandle, "Unknown";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", 0);
      End Select
    ElseIf TransRec.DetailTransType > 0 Then
      Select Case TransRec.DetailTransType
      Case 110      'lic
        Print #RptHandle, "License Charge";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", TransRec.TransAmount);
      Case 101      'payment
        Print #RptHandle, "Penalty Charge";
        RunBalance# = RunBalance# - TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", -TransRec.TransAmount);
      Case 211
        Print #RptHandle, "Paid License and Penalty";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", TransRec.TransAmount);
      Case 210
        Print #RptHandle, "Paid License";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", TransRec.TransAmount);
      Case 201
        Print #RptHandle, "Paid Penalty";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", TransRec.TransAmount);
      Case 311    'dn adj
        Print #RptHandle, "Adjust Down License and Penalty";
        RunBalance# = RunBalance# - TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", -TransRec.TransAmount);
      Case 310    'dn adj
        Print #RptHandle, "Adjust Down License";
        RunBalance# = RunBalance# - TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", -TransRec.TransAmount);
      Case 301    'dn adj
        Print #RptHandle, "Adjust Down Penalty";
        RunBalance# = RunBalance# - TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", -TransRec.TransAmount);
      Case 411    'up adj
        Print #RptHandle, "Adjust Up License and Penalty";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", TransRec.TransAmount);
      Case 410    'up adj
        Print #RptHandle, "Adjust Up License";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", TransRec.TransAmount);
      Case 401    'up adj
        Print #RptHandle, "Adjust Up Penalty";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", TransRec.TransAmount);
      Case Else
        Print #RptHandle, "Unknown";
        RunBalance# = RunBalance# + TransRec.TransAmount
        Print #RptHandle, Tab(58); Using("###,##0.00", 0);
      End Select
    End If
    Print #RptHandle, Tab(71); Using("###,##0.00", TransRec.BalanceAfterTrans)               'RunBalance#
    LineCnt = LineCnt + 1
    TransRecd& = TransRec.NextTrans

  Loop
  
  Print #RptHandle, Chr$(18);   ' oki 320 10 cpi
  Print #RptHandle, FF$;
  Close         'Close all open files now

  ViewPrint ReportFile$, "Customer Account History", True
  
  KillFile ReportFile$

  Exit Sub

PrintCustInqRptHeader:
  Page = Page + 1
  Print #RptHandle, Tab(18); "Business License : Detailed Customer History"
  Print #RptHandle, "Report Date: "; Date$; Tab(65); "Page #"; Page
  Print #RptHandle, "Customer Name: "; QPTrim$(CustRec.BillName)
  Print #RptHandle, "  Date"; Tab(10); "Description "; Tab(50); "Type"; Tab(60); "  Amount"; Tab(74); "Balance"
  Print #RptHandle, String$(80, "=")
  LineCnt = 3
  Return
  
ERRORSTUFF:
   Unload frmBLShowPctComp
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmBLInquiry", "PrintText", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close
    ClearInUse PWcnt
    Terminate
  
  
End Sub

Private Sub Form_Load()
  ScrWidth = Screen.Width / Screen.TwipsPerPixelX
  Set Over = New clsBLTextBoxOverrider
  Over.OverRide Me
  Set Temp_Class = New Resize_Class
  Temp_Class.InitResizeClass Me
  ScreenW = (Screen.Width / Screen.TwipsPerPixelX)
  Call LoadMe
End Sub

Private Sub Form_Resize()
  If Me.WindowState <> vbMinimized Then
    Me.Visible = False
    Temp_Class.ResizeControls Me
    Me.Visible = True
    Me.SetFocus
  End If
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      Call cmdExit_Click
      SendKeys "%C"
      KeyCode = 0
    Case vbKeyF4:
      Call cmdHistory_Click
      SendKeys "%H"
      KeyCode = 0
    Case Else:
  End Select
End Sub

Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    If cmdExit.Enabled = False Then
      Cancel = True
    ElseIf MsgBox("Are You Sure You Wish To Close Program?", vbYesNo, "Close?") = vbNo Then
      Cancel = True
    Else
      ClearInUse PWcnt
      MainLog ("BusinessLicense.exe terminated via menu bar on frmBLInquiry.")
      Call Terminate
      End
    End If
  End If
End Sub

Private Sub Form_KeyPress(KeyAscii As Integer)
'  this causes all characters to be capitalized
   KeyAscii = Asc(UCase$(Chr$(KeyAscii)))

End Sub

Public Sub LoadMe()
  Dim CHandle As Integer
  Dim CustRec As ARCustRecType
  
  On Error GoTo ERRORSTUFF
  
  If GCustNum = 0 Then
    frmBLMessageBoxJr.Label1.Caption = "Error: There was a problem retrieving data for this customer. Please try again."
    frmBLMessageBoxJr.Label1.Top = 900
    frmBLMessageBoxJr.Show vbModal
    Close
    frmBLCustomerLookup.Show
    DoEvents
    Unload frmBLInquiry
    Exit Sub
  End If
  
  OpenCustFile CHandle
  Get CHandle, GCustNum, CustRec
  Close CHandle
  
  fptxtCustNum.Text = QPTrim$(CustRec.CustNumb)
  fptxtSearchName.Text = QPTrim$(CustRec.SortName)
  fptxtBillingName.Text = QPTrim$(CustRec.BillName)
  fptxtAddress1.Text = QPTrim$(CustRec.ADDRESS1)
  fptxtAddress2.Text = QPTrim$(CustRec.ADDRESS2)
  fptxtCity.Text = QPTrim$(CustRec.City)
  fptxtState.Text = QPTrim$(CustRec.State)
  fptxtZip.Text = QPTrim$(CustRec.ZipCode)
  fptxtBusName.Text = QPTrim$(CustRec.CustName)
  fptxtBillCat.Text = QPTrim$(CustRec.BILLCAT1)
  fpMaskWorkPhone = TrimPhone(CustRec.WPHONE)
  fpcurrFee = CustRec.Fee1 + CustRec.Fee2 + CustRec.Fee3 + CustRec.Fee4 + CustRec.Fee5
  fptxtLicenseNum.Text = QPTrim$(CustRec.LICENSE)
  fpDateValidThru = MakeRegDate(CustRec.VALID)
  If CheckValDate(fpDateValidThru) = False Then
    fpDateValidThru.Text = "NOT SAVED"
  End If
  fpDateOpened.Text = "NOT SAVED"
  If CustRec.IssueLicense = "Y" Or CustRec.IssueLicense = "N" Then
    fptxtPrintThru.Text = QPTrim$(CustRec.IssueLicense)
  Else
    fptxtPrintThru.Text = ""
  End If
  
  If CustRec.AcctBal < 1000000000000# Then
    fpcurrBal = OldRound(CustRec.AcctBal)
  Else
    fpcurrBal = 0
  End If
  
  If UCase(CustRec.Inactive) = "Y" Then
    fptxtInactiveAcct.Text = "Inactive"
  ElseIf UCase(CustRec.Inactive) = "N" Then
    fptxtInactiveAcct.Text = "Active"
  Else
    fptxtInactiveAcct.Text = ""
  End If
  
  Exit Sub
  
ERRORSTUFF:
   Unload frmBLShowPctComp
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmBLInquiry", "LoadMe", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close
    ClearInUse PWcnt
    Terminate
  
  
End Sub

