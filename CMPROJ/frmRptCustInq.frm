Private Sub fpCmdTranHist_Click()
  ReDim MsgText(0 To 5) As String
  Dim FntSize As Integer
  Dim NumOfDCRecs As Long, DCFile As Integer
  ReDim DCCustREc(1) As DCCustRecType
  If fpCustRecNo > 0 Then
    OpenDCCustFile NumOfDCRecs, DCFile
    Get DCFile, fpCustRecNo, DCCustREc(1)
    Close DCFile
    If DCCustREc(1).FirstTrans > 0 Then
      'DeActivateControls Me
      DisplayCustTransList fpCustRecNo
      'ActivateControls Me
    Else
      frmMsgDialog.RetLabel = "-2"
      FntSize = frmMsgDialog.Label(2).FontSize
      frmMsgDialog.Label(2).FontSize = (FntSize + 2)
      MsgText(0) = "ERROR:"
      MsgText(1) = ""
      MsgText(2) = ""
      MsgText(3) = "There are NO transactions to display."
      MsgText(4) = ""
      MsgText(5) = ""
      GetOKorNot MsgText(), True
    End If
  End If
End Sub
Private Sub LoadCustInfo()
  Dim NumOfDCRecs As Long, DCFile As Integer, AccountRecord As Long
  ReDim DCCustREc(1) As DCCustRecType
  OpenDCCustFile NumOfDCRecs, DCFile
  RecNo& = Val(frmRptCustInq.fpCustRecNo)
  Get DCFile, RecNo&, DCCustREc(1)
  Close DCFile
  
  'If DCCustRec(1).Deleted = "Y" Then GoTo CustomerDeleted
  LabelAcctNo = RTrim$(DCCustREc(1).CUSTNUMB)
  fpSearch = RTrim$(DCCustREc(1).SORTNAME)
  fpCustName = DCCustREc(1).BILLNAME
  fpAddr1 = DCCustREc(1).ADDRESS1
  fpAddr2 = DCCustREc(1).ADDRESS2
  fpCity = DCCustREc(1).City
  fpState = DCCustREc(1).State
  fpZip = DCCustREc(1).ZIPCODE
  fpSoSec = DCCustREc(1).SOSEC
  fpDrvLic = DCCustREc(1).DRVLIC
  fpOpenDate = Num2Date(DCCustREc(1).DATEOPED)
  fpHPhone = DCCustREc(1).HPHONE
  fpWPhone = DCCustREc(1).WPHONE
  fpAcctBalance = Using$("$#,###.##", DCCustREc(1).AcctBal)
  Select Case DCCustREc(1).CASHONLY
    Case "N", " "
      fpCashOnly = "No"
    Case Else
      fpCashOnly = "Yes"
  End Select
  Select Case DCCustREc(1).resident
    Case "N", " "
      fpResidential = "No"
    Case Else
      fpResidential = "Yes"
  End Select
  Select Case DCCustREc(1).Owner
    Case "N", " "
      fpOwner = "No"
    Case Else
      fpOwner = "Yes"
  End Select
  If DCCustREc(1).FirstCar > 0 Then
    fpVehOne = DCCustREc(1).FirstCar
    Gettheveh
  Else
    fpVehOne = 0
  End If
  If fpVehOne > 0 Then
    fpVehonfile = "Yes"
    fpCmdVehicles.Enabled = True
  Else
    fpVehonfile = "No"
    fpCmdVehicles.Enabled = False
  End If
End Sub
Private Sub Gettheveh()
  Dim NumOfVRecs As Long, DCvFile As Integer, DCVehReclen As Integer
  ReDim DCVRec(1) As DCVehType
startloop:
  If fpVehOne > 0 Then
    DCVehReclen = Len(DCVRec(1))
    DCvFile = FreeFile
    Open "DCVEH.DAT" For Random Access Read Write Shared As DCvFile Len = DCVehReclen
    NumOfVRecs = LOF(DCvFile) \ DCVehReclen
    Get DCvFile, fpVehOne, DCVRec(1)
    Close DCvFile
    ' AddFlag = 1
    If DCVRec(1).Active <> "Y" Then
      If DCVRec(1).NextRec <= 0 Then
        fpVehOne = 0
      Else
        fpVehOne = DCVRec(1).NextRec
      End If
      GoTo startloop
    End If
  End If
End Sub
