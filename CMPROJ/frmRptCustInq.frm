Private Sub fpCmdVehicles_Click()
  Dim NumOfVRecs As Long, DCvFile As Integer, DCVehReclen As Integer
  ReDim DCVRec(1) As DCVehType
  Dim VehRec As Long
  VehRec = fpVehOne

startloop:
  If VehRec > 0 Then
    DCVehReclen = Len(DCVRec(1))
    DCvFile = FreeFile
    Open "DCVEH.DAT" For Random Access Read Write Shared As DCvFile Len = DCVehReclen
    NumOfVRecs = LOF(DCvFile) \ DCVehReclen
    Get DCvFile, VehRec, DCVRec(1)
    Close DCvFile
     ' AddFlag = 1
    If DCVRec(1).Active <> "Y" Then
      If DCVRec(1).NextRec <= 0 Then
        VehRec = 0
      Else
        VehRec = DCVRec(1).NextRec
      End If
      GoTo startloop
    End If

  End If

  If VehRec > 0 Then
    frmRptVehInq.fpVehRecNo = VehRec
    frmRptVehInq.Caption = "Vehicle Information  - " & QPTrim$(fpCustName)
    frmRptVehInq.fpCustRecNo = frmCustAddEdit.fpCustRecNo
    'frmCustVehicles.SetVehScreen
    frmRptVehInq.Show 1
  Else
    MsgBox "No Vehicles to Display.", vbOKOnly, "No Vehicles Found"
  End If

End Sub

