Attribute VB_Name = "frmCustMsgEdit"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Dim CustMsgRec As UBMessRecType, BeenDone As Boolean
Dim Recno As Long, MessageRec As Long, zz As Integer

Private Sub Form_Activate()
  If Not BeenDone Then
    BeenDone = True
    Call LoadCustMsg
  End If
End Sub

Private Sub Form_Load()
  DoEvents
'  Call LoadCustMsg
'  RecNo = Val(frmCustMsgEdit.CustRec)
End Sub

Private Sub fpCmdClear_Click()
  ReDim MsgText(0 To 5) As String
  Dim zz As Integer
  MsgText(0) = "Erase Messages?"
  MsgText(1) = "WARNING!  WARNING!  WARNING!  WARNING!"
  MsgText(2) = "THIS PROCEDURE CAN NOT BE UNDONE!"
  MsgText(3) = "CLEAR THIS ACCOUNTS MESSAGE INFORMATION?"
  MsgText(4) = ""
  MsgText(5) = "ARE YOU SURE YOU WANT TO DO THIS?"
  
  If GetOKorNot(MsgText()) Then
    For zz = 0 To 14
      frmCustMsgEdit.fpCMsg(zz) = ""
    Next
  End If
  
End Sub

Private Sub fpCmdExit_Click()
 'do save stuff
' Stop
  BeenDone = False
  Unload frmCustMsgEdit
  DoEvents
 
End Sub
Private Sub LoadCustMsg()
  Dim UBCustRecLen As Integer
  Dim UBMessRecLen As Integer
  Dim UBFile As Integer
  Dim Txt As String
  
  Recno = Val(frmCustMsgEdit.CustRec)
  ReDim UBCustRec(1) As NewUBCustRecType
  UBCustRecLen = Len(UBCustRec(1))

  ReDim UBMessRec(1) As UBMessRecType
  UBMessRecLen = Len(UBMessRec(1))
  
  UBFile = FreeFile
  Open UBCustFile For Random Shared As UBFile Len = UBCustRecLen
  Get UBFile, Recno, UBCustRec(1)
  Close UBFile
  MessageRec = UBCustRec(1).MessageRec
  If MessageRec > 0 Then
    UBFile = FreeFile
    Open UBPath + "UBMESAGE.DAT" For Random Shared As UBFile Len = UBMessRecLen
    Get UBFile, MessageRec, UBMessRec(1)
    Close UBFile
    For zz = 1 To 15
      Txt = UBMessRec(1).MessLine(zz).Msg
      Txt = RTrim(RemNulls(Txt))
      frmCustMsgEdit.fpCMsg(zz - 1).Text = Txt
    Next
  End If
End Sub

Private Sub fpCmdSave_Click()
  Dim UBCustRecLen As Integer
  Dim UBMessRecLen As Integer
  Dim UBFile As Integer
  Dim Txt As String
  
  Recno = Val(frmCustMsgEdit.CustRec)
  ReDim UBCustRec(1) As NewUBCustRecType
  UBCustRecLen = Len(UBCustRec(1))

  ReDim UBMessRec(1) As UBMessRecType
  UBMessRecLen = Len(UBMessRec(1))
  
  For zz = 1 To 15
    UBMessRec(1).MessLine(zz).Msg = frmCustMsgEdit.fpCMsg(zz - 1).Text
  Next
  UBMessRec(1).CustRec = Val(frmCustMsgEdit.CustRec)
  
  UBFile = FreeFile
  Open UBPath + "UBMESAGE.DAT" For Random Shared As UBFile Len = UBMessRecLen
  If MessageRec = 0 Then
    MessageRec = (LOF(UBFile) / UBMessRecLen) + 1
  End If
  Put UBFile, MessageRec, UBMessRec(1)
  Close UBFile

  UBFile = FreeFile
  Open UBCustFile For Random Shared As UBFile Len = UBCustRecLen
  Get UBFile, Recno&, UBCustRec(1)
  UBCustRec(1).MessageRec = MessageRec
  Put UBFile, Recno&, UBCustRec(1)
  Close UBFile

End Sub

Private Sub fpCMsg_KeyUp(Index As Integer, KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
  Case vbKeyReturn
    KeyCode = 0
    DoEvents
    SendKeys "{Tab}", True
    DoEvents
  Case vbKeyF3
    KeyCode = 0
    DoEvents
    Call fpCmdClear_Click
  Case vbKeyF10
    KeyCode = 0
    DoEvents
    Call fpCmdSave_Click
  Case vbKeyEscape
    KeyCode = 0
    DoEvents
    Call fpCmdExit_Click
  End Select
End Sub
