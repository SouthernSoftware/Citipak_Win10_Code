Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    If cmdExit.Enabled = False Then
      Cancel = True
    Else
      If MsgBox("Are You Sure You Wish To Close Program?", vbYesNo, "Close?") = vbNo Then
        Cancel = True
      Else
        ClearInUse PWcnt
      End If
    End If
  End If
End Sub

Private Sub cmdSave_Click()
  Dim RP As Integer, lenRP As Integer
  Dim RcptPrnFile As ReceiptPRNType
  RP = FreeFile
  lenRP = Len(RcptPrnFile)
  Open rcpfile$ For Random Shared As RP Len = lenRP
  
  RcptPrnFile.RcpPort = QPTrim(fptxtReceiptPrn.Text)
  If fpcboRcptDef.ListIndex = 0 Then
    RcptPrnFile.PrnDefYN = 0
  Else
    RcptPrnFile.PrnDefYN = 1
  End If
  If fpcboDrwDef.ListIndex = 0 Then
    RcptPrnFile.CtlDefYN = 0
  Else
    RcptPrnFile.CtlDefYN = 1
  End If
  RcptPrnFile.PaymDate = 0
  If fpcboRValidate.ListIndex = 0 Then
    RcptPrnFile.RValidate = 0
  Else
    RcptPrnFile.RValidate = 1
  End If
  Put RP, 1, RcptPrnFile
  Close
  cmdExit_Click
End Sub
