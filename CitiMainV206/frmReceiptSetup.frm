Private Sub getinfo()
  Dim RP As Integer, lenRP As Integer
  If TypeSysOP >= 6 Then
    rcpfile$ = "C:\CPWork\RcptPrn.dat"
  Else
    rcpfile$ = "C:\RcptPrn.dat"
  End If

  If Exist(rcpfile$) Then
    Dim RcptPrnFile As ReceiptPRNType
    RP = FreeFile
    lenRP = Len(RcptPrnFile)
    Open rcpfile$ For Random Shared As RP Len = lenRP
    Get RP, 1, RcptPrnFile
    fptxtReceiptPrn.Text = QPTrim(RcptPrnFile.RcpPort)
    If RcptPrnFile.PrnDefYN = 0 Then
      fpcboRcptDef.ListIndex = 0
    Else
      fpcboRcptDef.ListIndex = 1
    End If
    Close
    If RcptPrnFile.CtlDefYN = 0 Then
      fpcboDrwDef.ListIndex = 0
    Else
      fpcboDrwDef.ListIndex = 1
    End If
    If RcptPrnFile.RValidate = 0 Then
      fpcboRValidate.ListIndex = 0
    Else
      fpcboRValidate.ListIndex = 1
    End If
    Close

  Else
    fptxtReceiptPrn.Text = ""
  End If
End Sub
