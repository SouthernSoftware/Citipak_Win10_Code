Private Sub Form_Load()
  Call LoadMe
End Sub
Private Sub LoadMe()
  fpcmbCatType.Text = "Flat Rate"
  fpcmbCatType.AddItem "Flat Rate"
  fpcmbCatType.AddItem "Multiplier"
  fpcmbCatType.AddItem "Step Rate"
  frmBLMessageBox.Label1.Caption = "The category code can be edited but only as long as no customer data has been saved using the category. After any customer data is saved using this category then the category code field is disabled. The reason is that each and every customer who has been saved using the existing code would have to have their category code edited to match the new code number. Failure to edit the category code numbers for the affected customers would result in inaccurate business license fee calculations."
  frmBLMessageBox.Label1.FontSize = 12
  frmBLMessageBox.Label1.Top = 800
  frmBLMessageBox.Label1.Height = 2800
  frmBLMessageBox.Show vbModal
End Sub
Private Sub fpcmbCatType_Change()
  Dim x As Integer
  
  If QPTrim$(fpcmbCatType.Text) = "" Then
    fpcmbCatType = "Flat Rate"
    LabelFee.Caption = "FLAT RATE AMOUNT"
    fpcurrFee.Enabled = True
    Exit Sub
  End If
  
  If QPTrim$(fpcmbCatType.Text) = "Flat Rate" Then
    fpcmbCatType.Tag = "The amount entered in the Flat Rate Amount field will be the entire fee for 'Flat Rate'. It is recommended that once saved this field remain unchanged. The reason is that if the type changes then the base value on which this category's fees are calculated will need to be modified for every customer using this category. Otherwise the license fee calculation for this customer will not be accurate."
    LabelFee.Caption = "FLAT RATE AMOUNT"
    fpcurrFee.Enabled = True
    fpcurrFee.Tag = "The amount entered here will be charged to all businesses in this category regardless of their annual revenues."
    fptxtCatDesc.Text = "Auto Repair Shops"
    For x = 0 To 5
      fpcurrBase(x).Enabled = False
      fpcurrRecUpTo(x).Enabled = False
      fptxtPct(x).Enabled = False
      fpcurrOver(x).Enabled = False
    Next x
    cmdLine1.Enabled = False
    cmdLine2.Enabled = False
    cmdLine3.Enabled = False
    cmdLine4.Enabled = False
    cmdLine5.Enabled = False
    cmdLine6.Enabled = False
  ElseIf QPTrim$(fpcmbCatType.Text) = "Multiplier" Then
    fpcmbCatType.Tag = "For 'Multiplier' the amount entered in the Rate Per Unit field will be assessed to each profit center (a profit center for a barber shop would be a barber chair). It is recommended that once saved this field remain unchanged. The reason is that if the type changes then the base value on which this category's fees are calculated will need to be modified for every customer using this category. Otherwise the license fee calculation for this customer will not be accurate."
    LabelFee.Caption = "RATE PER UNIT"
    fpcurrFee.Enabled = True
    fpcurrFee.Tag = "The amount entered here is assessed to businesses that earn their revenue based on how many revenue producing entities the business owns. For example, if a barber shop has 3 barber chairs then their license fee would be 3 times the amount entered here."
    fptxtCatDesc.Text = "Beauty/Barber Shops"
    For x = 0 To 5
      fpcurrBase(x).Enabled = False
      fpcurrRecUpTo(x).Enabled = False
      fptxtPct(x).Enabled = False
      fpcurrOver(x).Enabled = False
    Next x
    cmdLine1.Enabled = False
    cmdLine2.Enabled = False
    cmdLine3.Enabled = False
    cmdLine4.Enabled = False
    cmdLine5.Enabled = False
    cmdLine6.Enabled = False
  ElseIf QPTrim$(fpcmbCatType.Text) = "Step Rate" Then
    fpcmbCatType.Tag = "Fees for 'Step Rate' are assessed based on the revenues generated by each business. It is recommended that once saved this field remain unchanged. The reason is that if the type changes then the base value on which this category's fees are calculated will need to be modified for every customer using this category. Otherwise the license fee calculation for this customer will not be accurate."
    LabelFee.Caption = "NOT APPLICABLE"
    fpcurrFee.Enabled = False
    fptxtCatDesc.Text = "Manufacturing - Durable Goods"
    For x = 0 To 5
      fpcurrBase(x).Enabled = True
      fpcurrRecUpTo(x).Enabled = True
      fptxtPct(x).Enabled = True
      fpcurrOver(x).Enabled = True
    Next x
    cmdLine1.Enabled = True
    cmdLine2.Enabled = True
    cmdLine3.Enabled = True
    cmdLine4.Enabled = True
    cmdLine5.Enabled = True
    cmdLine6.Enabled = True
  End If

End Sub

