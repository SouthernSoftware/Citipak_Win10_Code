Attribute VB_Name = "frmBLCustInfoTrans"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
  Dim ScrWidth As Long
  Dim Over As clsBLTextBoxOverrider
  Dim HistFlag As Boolean
  Private Temp_Class As Resize_Class

Private Sub cmdExit_Click()
  KillFile "custinfomodal.dat"
  frmBLTransEntry.Show
  DoEvents
  Unload Me
End Sub

Private Sub cmdHelp_Click()
  If InStr(cmdHelp.Text, "On") Then
    cmdHelp.Text = "F1 &Turn Help Off"
    btnHelp.AutoScan = fpAutoScanPopupOnly
    lblBalloon.Visible = True
  ElseIf InStr(cmdHelp.Text, "Off") Then
    cmdHelp.Text = "F1 &Turn Help On"
    btnHelp.AutoScan = fpAutoScanOff
    lblBalloon.Visible = False
  End If
End Sub

Private Sub cmdTransHist_Click()
  If HistFlag = False Then
    frmBLMessageBoxJr.Label1.Caption = "This customer has no transaction activity."
    frmBLMessageBoxJr.Label1.Top = 800
    frmBLMessageBoxJr.Show vbModal
    Close
    Exit Sub
  End If
  
  frmBLTransHistJr.Show vbModal
  
End Sub

Private Sub Form_Load()
  ScrWidth = Screen.Width / Screen.TwipsPerPixelX
  Set Over = New clsBLTextBoxOverrider
  Over.OverRide Me
  Set Temp_Class = New Resize_Class
  Temp_Class.InitResizeClass Me
  ScreenW = (Screen.Width / Screen.TwipsPerPixelX)
  Call LoadMe
End Sub

Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    If cmdExit.Enabled = False Then
      Cancel = True
    ElseIf MsgBox("Are You Sure You Wish To Close Program?", vbYesNo, "Close?") = vbNo Then
      Cancel = True
    Else
      ClearInUse PWcnt
      MainLog ("BusinessLicense.exe terminated via menu bar on frmBLCustInfoTrans.")
      Call Terminate
      End
    End If
  End If

End Sub

Private Sub Form_Resize()
  If Me.WindowState <> vbMinimized Then
    ''Me.Visible = False
    Temp_Class.ResizeControls Me
    Me.Visible = True
    Me.SetFocus
  End If
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%C"
      Call cmdExit_Click
      KeyCode = 0
    Case vbKeyF4:
      SendKeys "%H"
      Call cmdTransHist_Click
      KeyCode = 0
    Case Else:
  End Select
End Sub
Public Sub LoadMe()
  Dim CustFile As Integer
  Dim NumOfCustRecs As Integer
  Dim CustRec As ARCustRecType
  Dim ThisCust As Integer
  Dim CodeRec As ARNewCatCodeRecType
  Dim CodeHandle As Integer
  Dim NumOfCodeRecs As Integer
  Dim x As Integer
  Dim ThisType As String * 1
  Dim ThisCat As String
  Dim DHandle As Integer
  Dim One As Integer
  
  HistFlag = True
  lblBalloon.Visible = False
  If IsNumeric(frmBLTransEntry.fptxtAccount.Text) Then
    ThisCust = CInt(frmBLTransEntry.fptxtAccount.Text)
    If ThisCust = 0 Then
      frmBLMessageBoxJr.Label1.Caption = "Error: There was a problem loading the Customer Information screen. Please check the customer number."
      frmBLMessageBoxJr.Label1.Top = 700
      frmBLMessageBoxJr.Show vbModal
      Exit Sub
    End If
  Else
    frmBLMessageBoxJr.Label1.Caption = "Error: There was a problem loading the Customer Information screen. Please check the customer number."
    frmBLMessageBoxJr.Label1.Top = 700
    frmBLMessageBoxJr.Show vbModal
    Exit Sub
  End If
  
  OpenCustFile CustFile
  Get CustFile, ThisCust, CustRec
  Close CustFile
  
  If CustRec.LastTrans = 0 Then
    HistFlag = False
  End If
  
  ThisCustXNum = ThisCust

  fptxtBillingName.Text = QPTrim$(CustRec.BillName)
  fptxtBusName.Text = QPTrim$(CustRec.CustName)
  fptxtSearchName.Text = QPTrim$(CustRec.SortName)
  fptxtContact.Text = QPTrim$(CustRec.Contact)
  fptxtAddress1.Text = QPTrim$(CustRec.ADDRESS1)
  fptxtAddress2.Text = QPTrim$(CustRec.ADDRESS2)
  fptxtCity.Text = QPTrim$(CustRec.City)
  fptxtState.Text = QPTrim$(CustRec.State)
  fptxtZip.Text = QPTrim$(CustRec.ZipCode)
  fptxtCustNum.Text = QPTrim$(CustRec.CustNumb)
  fptxtValidThru.Text = MakeRegDate(CustRec.VALID)
  fptxtLicNum.Text = QPTrim$(CustRec.LICENSE)
  fpMaskPhone = TrimPhone(CustRec.WPHONE)
  If CustRec.Inactive = "Y" Then
    fptxtActive.Text = "Inactive"
  ElseIf CustRec.Inactive = "N" Then
    fptxtActive.Text = "Active"
  End If
  fptxtProrate.Text = CStr(CustRec.Prorate) + "%"
  If CustRec.CustLocation = "I" Then
    fptxtInOut.Text = "Inside"
  ElseIf CustRec.CustLocation = "O" Then
    fptxtInOut.Text = "Outside"
  End If
  
  fptxtCatCode(0).Text = QPTrim$(CustRec.BILLCAT1)
  fptxtCatCode(1).Text = QPTrim$(CustRec.BILLCAT2)
  fptxtCatCode(2).Text = QPTrim$(CustRec.BILLCAT3)
  fptxtCatCode(3).Text = QPTrim$(CustRec.BILLCAT4)
  fptxtCatCode(4).Text = QPTrim$(CustRec.BILLCAT5)
  
  fptxtCatDesc(0).Text = GetCatDesc(CustRec.BILLCAT1)
  fptxtCatDesc(1).Text = GetCatDesc(CustRec.BILLCAT2)
  fptxtCatDesc(2).Text = GetCatDesc(CustRec.BILLCAT3)
  fptxtCatDesc(3).Text = GetCatDesc(CustRec.BILLCAT4)
  fptxtCatDesc(4).Text = GetCatDesc(CustRec.BILLCAT5)
  
  OpenCatCodeFile CodeHandle

  NumOfCodeRecs = LOF(CodeHandle) / Len(CodeRec)
  If NumOfCodeRecs = 0 Then
    frmBLMessageBoxJr.Label1.Caption = "Error: No category codes saved."
    frmBLMessageBoxJr.Label1.Top = 700
    frmBLMessageBoxJr.Show vbModal
  End If
  
  ThisCat = fptxtCatCode(0).Text
  GoSub GetType
  fptxtCatType(0).Text = ThisType
  If ThisType = "F" Or ThisType = "M" Then
    fptxtCatRev(0).Text = CStr(CustRec.REV1)
  ElseIf ThisType = "S" Then
    fptxtCatRev(0).Text = Using$("$###,##0.00", CustRec.REV1)
  Else
    fptxtCatRev(0).Text = ""
  End If
  
  ThisCat = fptxtCatCode(1).Text
  GoSub GetType
  fptxtCatType(1).Text = ThisType
  If ThisType = "F" Or ThisType = "M" Then
    fptxtCatRev(1).Text = CStr(CustRec.REV2)
  ElseIf ThisType = "S" Then
    fptxtCatRev(1).Text = Using$("$###,##0.00", CustRec.REV2)
  Else
    fptxtCatRev(1).Text = ""
  End If
  
  ThisCat = fptxtCatCode(2).Text
  GoSub GetType
  fptxtCatType(2).Text = ThisType
  If ThisType = "F" Or ThisType = "M" Then
    fptxtCatRev(2).Text = CStr(CustRec.REV3)
  ElseIf ThisType = "S" Then
    fptxtCatRev(2).Text = Using$("$###,##0.00", CustRec.REV3)
  Else
    fptxtCatRev(2).Text = ""
  End If
  
  ThisCat = fptxtCatCode(3).Text
  GoSub GetType
  fptxtCatType(3).Text = ThisType
  If ThisType = "F" Or ThisType = "M" Then
    fptxtCatRev(3).Text = CStr(CustRec.REV4)
  ElseIf ThisType = "S" Then
    fptxtCatRev(3).Text = Using$("$###,##0.00", CustRec.REV4)
  Else
    fptxtCatRev(3).Text = ""
  End If
  
  ThisCat = fptxtCatCode(4).Text
  GoSub GetType
  fptxtCatType(4).Text = ThisType
  If ThisType = "F" Or ThisType = "M" Then
    fptxtCatRev(4).Text = CStr(CustRec.REV5)
  ElseIf ThisType = "S" Then
    fptxtCatRev(4).Text = Using$("$###,##0.00", CustRec.REV5)
  Else
    fptxtCatRev(4).Text = ""
  End If
  
  Close CodeHandle
  
  One = 1
  DHandle = FreeFile
  Open "custinfomodal.dat" For Output As DHandle Len = 2
  Print #DHandle, One
  Close DHandle
  
  Exit Sub

GetType:
  ThisType = ""
  For x = 1 To NumOfCodeRecs
    Get CodeHandle, x, CodeRec
      If QPTrim$(CodeRec.CatCode) = ThisCat Then
        ThisType = CodeRec.CodeType
        Exit For
      End If
  Next x
  
  Return
  
End Sub

Public Function TrimPhone(PhoneNumber$) As String
  Dim x As Integer
  Dim PhoneLen As Integer
  Dim thischar$
  
  PhoneLen = Len(PhoneNumber)
  For x = 1 To PhoneLen
    thischar = Mid(PhoneNumber, x, 1)
    If IsNumeric(thischar) = True Then
      TrimPhone = TrimPhone + thischar
    End If
  Next x
  
End Function

