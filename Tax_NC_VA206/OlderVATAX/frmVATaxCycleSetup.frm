Attribute VB_Name = "frmVATaxCycleSetup"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
  Dim ScrWidth As Long
  Dim Over As clsTextBoxOverRider
  'Private Temp_Class As Resize_Class
  Dim TempCycleNum() As Long
  Dim TempCycleName() As String
  Public ThisNumber As Long
  Public ThisDesc$
  Public NewNum As Long

Private Sub cmdExit_Click()
  Unload Me
End Sub

Private Sub cmdSave_Click()
  Dim TaxSetupRec As TaxMasterType
  Dim TMHandle As Integer
  Dim x As Integer, y As Long, z As Integer
  Dim ThisCycle As Integer
  Dim TaxCustRec As TaxCustType
  Dim TCHandle As Integer
  Dim NumOfTCRecs As Long
  Dim PrintLine$
  Dim PrintCnt As Integer
  
  On Error GoTo ERRORSTUFF
  
  For x = 0 To 4
    If QPTrim$(fptxtCycleName(x).Text) <> "" Then
      If fpLngIntCycleNum(x).Value = 0 Then
        Call TaxMsg(900, "Please enter a non-zero cycle number for the cycle on line " + CStr(x + 1) + ".")
        fptxtCycleName(x).SetFocus
        Exit Sub
      End If
    End If
    If fpLngIntCycleNum(x) <> 0 Then
      If QPTrim$(fptxtCycleName(x).Text) = "" Then
        Call TaxMsg(900, "Please enter a cycle description on line " + CStr(x + 1) + ".")
        fptxtCycleName(x).SetFocus
        Exit Sub
      End If
    End If
  Next x
  
  If Exist(TaxCustFile) Then
    PrintLine = ""
    PrintCnt = 0
    For x = 0 To 4
      If TempCycleNum(x + 1) <> fpLngIntCycleNum(x).Value Then
        ThisDesc$ = TempCycleName(x + 1)
        ThisNumber = TempCycleNum(x + 1)
        NewNum = fpLngIntCycleNum(x).Value
        GoSub PrintCycleUsers
        If CycleCnt > 0 Then
          frmVATaxPrintCycleUsers.Show vbModal
          If frmVATaxPrintCycleUsers.fptxtAnswer.Text = "convert" Then
            Unload frmVATaxPrintCycleUsers
            GoTo NextOne
          Else
            Unload frmVATaxPrintCycleUsers 'don't save if no conversion takes place
            Call TaxMsg(900, "Billing cycle # " + fpLngIntCycleNum(x).Text + " not saved.")
            fpLngIntCycleNum(x) = TempCycleNum(x + 1)
            fptxtCycleName(x).Text = TempCycleName(x + 1)
            If PrintCnt = 0 Then
              PrintLine = PrintLine + CStr(TempCycleNum(x + 1))
            Else
              PrintLine = PrintLine + ", " + CStr(TempCycleNum(x + 1))
            End If
          End If
        End If
      End If
NextOne:
    Next x
  End If
  
  OpenTaxSetUpFile TMHandle
  Get TMHandle, 1, TaxSetupRec
  For x = 0 To 4
    TaxSetupRec.CycleName(x + 1) = QPTrim$(fptxtCycleName(x).Text)
    TaxSetupRec.CycleNum(x + 1) = fpLngIntCycleNum(x).Value
  Next x
  Put TMHandle, 1, TaxSetupRec
  Close TMHandle
  
  If QPTrim$(PrintLine) <> "" Then
    Call Savemsg(800, "Tax billing cycles have been saved successfully. " + PrintLine + " remained unchanged.")
  Else
    Call Savemsg(900, "Tax billing cycles have been saved successfully.")
  End If
  
  Unload Me
  
  Exit Sub
  
PrintCycleUsers:
  CycleCnt = 0
  ReDim CycleUsersName(1 To 1) As String
  ReDim CycleUsersAcct(1 To 1) As Long
  OpenTaxCustFile TCHandle, NumOfTCRecs
  For y = 1 To NumOfTCRecs
    Get TCHandle, y, TaxCustRec
    If TaxCustRec.Cycle > 0 And TaxCustRec.Cycle = ThisNumber Then
      CycleCnt = CycleCnt + 1
      ReDim Preserve CycleUsersName(1 To CycleCnt) As String
      CycleUsersName(CycleCnt) = QPTrim$(TaxCustRec.CustName)
      ReDim Preserve CycleUsersAcct(1 To CycleCnt) As Long
      CycleUsersAcct(CycleCnt) = TaxCustRec.Acct
    End If
  Next y
  Close TCHandle
      
  Return
  
ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmVATaxCycleSetup", "cmdSave_Click", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close
    ClearInUse PWcnt
    Terminate
  
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%C"
      Call cmdExit_Click
      KeyCode = 0
    Case vbKeyF10:
      SendKeys "%O"
      Call cmdSave_Click
      KeyCode = 0
    Case Else:
  End Select

End Sub

Private Sub Form_Load()
  Dim TaxSetupRec As TaxMasterType
  Dim TMHandle As Integer
  Dim x As Integer
  
  ReDim TempCycleName(1 To 5) As String
  ReDim TempCycleNum(1 To 5) As Long
  NewNum = 0
  If Exist(TaxSetupName) Then
    OpenTaxSetUpFile TMHandle
    Get TMHandle, 1, TaxSetupRec
    For x = 0 To 4
      If TaxSetupRec.CycleNum(x + 1) > 0 Then
        fpLngIntCycleNum(x) = TaxSetupRec.CycleNum(x + 1)
        TempCycleNum(x + 1) = TaxSetupRec.CycleNum(x + 1)
        fptxtCycleName(x).Text = QPTrim$(TaxSetupRec.CycleName(x + 1))
        TempCycleName(x + 1) = QPTrim$(TaxSetupRec.CycleName(x + 1))
      Else
        fpLngIntCycleNum(x) = 0
        TempCycleNum(x + 1) = 0
        fptxtCycleName(x).Text = ""
        TempCycleName(x + 1) = ""
      End If
    Next x
  Else
    For x = 0 To 4
      fpLngIntCycleNum(x) = 0
      TempCycleName(x + 1) = 0
      fptxtCycleName(x).Text = ""
      TempCycleName(x + 1) = ""
    Next x
  End If
  Close TMHandle
 End Sub
 
Private Function Check4DupDescs(ThisName$, ThisRec As Integer) As Boolean
  Dim x As Integer
  
  Check4DupDescs = False
  If ThisName = "" Then Exit Function
  For x = 0 To 4
    If x <> ThisRec Then
      If QPTrim$(fptxtCycleName(x).Text) = ThisName Then
        Call TaxMsg(900, "The description entered on line " + CStr(ThisRec + 1) + " is already being used. Please select a unique description.")
        Check4DupDescs = True
        Exit Function
      End If
    End If
  Next x
  
End Function
Private Function Check4DupNums(ThisNum&, ThisRec As Integer) As Boolean
  Dim x As Integer
  
  Check4DupNums = False
  If ThisNum = 0 Then Exit Function
  For x = 0 To 4
    If x <> ThisRec Then
      If fpLngIntCycleNum(x).Value = ThisNum Then
        Call TaxMsg(900, "The cycle number entered on line " + CStr(ThisRec + 1) + " is already being used. Please select a unique cycle number.")
        Check4DupNums = True
        Exit Function
      End If
    End If
  Next x
  
End Function

Private Sub fpLngIntCycleNum_LostFocus(Index As Integer)
  If TempCycleNum(Index + 1) <> fpLngIntCycleNum(Index).Value Then
    If Check4DupNums(fpLngIntCycleNum(Index).Value, Index) = True Then
      fpLngIntCycleNum(Index).SetFocus
      Exit Sub
    End If
  End If

End Sub

Private Sub fptxtCycleName_LostFocus(Index As Integer)
  If TempCycleName(Index + 1) <> QPTrim$(fptxtCycleName(Index)) Then
    If Check4DupDescs(QPTrim$(fptxtCycleName(Index)), Index) = True Then
      fptxtCycleName(Index).SetFocus
      Exit Sub
    End If
  End If
End Sub
