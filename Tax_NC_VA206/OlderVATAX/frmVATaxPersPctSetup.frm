      frmVATaxSystemSetup.fptxtCurrYrPIntRate = CDbl(fpDblYearInt(0).Value)
    ElseIf Opt2.Value = True Then
      frmVATaxSystemSetup.fptxtCurrYrPIntRate = CDbl(fpDblYearInt(1).Value)
    ElseIf Opt3.Value = True Then
      frmVATaxSystemSetup.fptxtCurrYrPIntRate = CDbl(fpDblYearInt(2).Value)
    ElseIf Opt4.Value = True Then
      frmVATaxSystemSetup.fptxtCurrYrPIntRate = CDbl(fpDblYearInt(3).Value)
    ElseIf Opt5.Value = True Then
      frmVATaxSystemSetup.fptxtCurrYrPIntRate = CDbl(fpDblYearInt(4).Value)
    End If
  Else
    If Opt1.Value = True Then
      frmVATaxCalcInterest.fptxtCurrYrPIntRate = CDbl(fpDblYearInt(0).Value)
    ElseIf Opt2.Value = True Then
      frmVATaxCalcInterest.fptxtCurrYrPIntRate = CDbl(fpDblYearInt(1).Value)
    ElseIf Opt3.Value = True Then
      frmVATaxCalcInterest.fptxtCurrYrPIntRate = CDbl(fpDblYearInt(2).Value)
    ElseIf Opt4.Value = True Then
      frmVATaxCalcInterest.fptxtCurrYrPIntRate = CDbl(fpDblYearInt(3).Value)
    ElseIf Opt5.Value = True Then
      frmVATaxCalcInterest.fptxtCurrYrPIntRate = CDbl(fpDblYearInt(4).Value)
    End If
  End If
  
  Me.Hide
End Sub

