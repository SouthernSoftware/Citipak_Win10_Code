Private Sub cmdSave_Click()
  Dim TaxCustRec As TaxCustType
  Dim TCHandle As Integer
  Dim NumOfTaxCusts As Long
  Dim AddFlag As Boolean
  Dim ThisIdx As Long
  Dim IdxName As Boolean
  Dim IdxOpt As Boolean
  Dim IdxSrchName As Boolean
  Dim ThisListIdx As Long
  Dim ThisSSN As Boolean
  Dim ThisTown As Boolean
  Dim ThisZip$
  
  AddFlag = False
  If QPTrim$(fptxtName.Text) = "" Then
    frmVATaxMsg.Label1.Caption = "The Customer Name field is a required field. Please supply a Customer Name."
    frmVATaxMsg.Label1.Top = 800
    frmVATaxMsg.Show vbModal
    vaTabPro1.ActiveTab = 0
    fptxtName.SetFocus
    Exit Sub
  End If
  
  If QPTrim$(fptxtSearchName.Text) = "" Then
    frmVATaxMsg.Label1.Caption = "The Search Name field is a required field. Please supply a Search Name."
    frmVATaxMsg.Label1.Top = 800
    frmVATaxMsg.Show vbModal
    vaTabPro1.ActiveTab = 0
    fptxtSearchName.SetFocus
    Exit Sub
  End If
  
  If Len(QPTrim$(ReplaceString$(fptxtSocSec.Text, "-", ""))) > 0 And Len(QPTrim$(ReplaceString$(fptxtSocSec.Text, "-", ""))) <> 9 Then
    frmVATaxMsg.Label1.Caption = "The Social Security Number field requires a 9 digit number. Please supply a 9 digit Social Security Number."
    frmVATaxMsg.Label1.Top = 800
    frmVATaxMsg.Show vbModal
    vaTabPro1.ActiveTab = 0
    fptxtSocSec.SetFocus
    Exit Sub
  End If
  
  If Len(QPTrim$(ReplaceString$(fptxtOtherSSN.Text, "-", ""))) > 0 And Len(QPTrim$(ReplaceString$(fptxtOtherSSN.Text, "-", ""))) <> 9 Then
    frmVATaxMsg.Label1.Caption = "The Spouse's Social Security Number field requires a 9 digit number. Please supply a 9 digit number for the Spouse's Social Security Number or leave it blank."
    frmVATaxMsg.Label1.Top = 700
    frmVATaxMsg.Show vbModal
    vaTabPro1.ActiveTab = 0
    fptxtOtherSSN.SetFocus
    Exit Sub
  End If
  
  If QPTrim$(fptxtAdd1.Text) = "" And QPTrim$(fptxtAdd2.Text) = "" Then
    frmVATaxMsg.Label1.Caption = "An address is a requirement but both address fields are empty. Please supply an address for either Address #1 or Address #2."
    frmVATaxMsg.Label1.Top = 800
    frmVATaxMsg.Show vbModal
    vaTabPro1.ActiveTab = 0
    fptxtAdd1.SetFocus
    Exit Sub
  End If
    
  If QPTrim$(fptxtCity.Text) = "" Then
    frmVATaxMsg.Label1.Caption = "The City field is a required field. Please supply a City name."
    frmVATaxMsg.Label1.Top = 800
    frmVATaxMsg.Show vbModal
    vaTabPro1.ActiveTab = 0
    fptxtCity.SetFocus
    Exit Sub
  End If
  
  If Len(QPTrim$(fptxtState.Text)) <> 2 Then
    frmVATaxMsg.Label1.Caption = "The State field is a required field. Please supply a State two character abbreviation."
    frmVATaxMsg.Label1.Top = 800
    frmVATaxMsg.Show vbModal
    vaTabPro1.ActiveTab = 0
    fptxtState.SetFocus
    Exit Sub
  End If
    
  If QPTrim$(Mid(fptxtZip.Text, 1, 5)) = "" Then
    frmVATaxMsg.Label1.Caption = "The Zip Code field is a required field. Please supply a Zip Code number."
    frmVATaxMsg.Label1.Top = 800
    frmVATaxMsg.Show vbModal
    vaTabPro1.ActiveTab = 0
    fptxtZip.SetFocus
    Exit Sub
  End If
    
  If Len(QPTrim$(Mid(fptxtZip.Text, 1, 5))) <> 5 Then
    frmVATaxMsg.Label1.Caption = "The Zip Code field must include at least five digits. Please be sure the Zip Code has five digits."
    frmVATaxMsg.Label1.Top = 800
    frmVATaxMsg.Show vbModal
    vaTabPro1.ActiveTab = 0
    fptxtZip.SetFocus
    Exit Sub
  End If
    
  If Len(QPTrim$(Mid(fptxtZip.Text, 7, 4))) > 0 Then
    If Len(QPTrim$(Mid(fptxtZip.Text, 7, 4))) <> 4 Then
      frmVATaxMsg.Label1.Caption = "If an entry is made in the Zip Code extension field then it must include at least four digits. Please be sure the Zip Code extension has four digits or is left blank."
      frmVATaxMsg.Label1.Top = 700
      frmVATaxMsg.Show vbModal
      vaTabPro1.ActiveTab = 0
      fptxtZip.SetFocus
      Exit Sub
    End If
  End If
    
  If fpcmbActiveYN.Text = "N" Then
    If GCustNum = 0 Then
      If TaxMsgWOpts(800, "You are saving this new customer as 'Inactive'. If this is correct then press F10 to continue saving. Otherwise, press ESC to review.", "F10 Continue", "ESC Review") = "abort" Then
        Unload frmVATaxMsgWOpts
        vaTabPro1.ActiveTab = 1 '7/13/06
        fpcmbActiveYN.SetFocus
        Exit Sub
      Else
        Unload frmVATaxMsgWOpts
      End If
    End If
  End If
  
  ThisSSN = False
  If ReplaceString(TempCSSN, "-", "") <> ReplaceString(fptxtSocSec.Text, "-", "") Then
    ThisSSN = True
  End If
  
  ThisTown = False
  If TempCity <> fptxtCity.Text Then
    ThisTown = True
  End If
  
  IdxName = False
  If TempCustName <> fptxtName.Text Then
    IdxName = True
  End If
  
  IdxOpt = False
  If TempOptSrchDesc <> fptxtOptSrchData.Text Then
    IdxOpt = True
  End If
  
  IdxSrchName = False
  If TempSName <> fptxtSearchName.Text Then
    IdxSrchName = True
  End If
  
  frmVATaxSaveAnimation.Show
  DoEvents
  
  OpenTaxCustFile TCHandle, NumOfTaxCusts
  
  If GCustNum = 0 Then
    AddFlag = True
    TaxCustRec.PrePayBal = 0
    TaxCustRec.PrePayTrans = 0
    TaxCustRec.Pad1 = ""
    TaxCustRec.FirstPropRec = 0
    TaxCustRec.FirstPersRec = 0
    TaxCustRec.PIN = NumOfTaxCusts + 1
    TaxCustRec.Deleted = 0
    TaxCustRec.FileVer = 205
    TaxCustRec.LastTrans = 0
    TaxCustRec.Acct = NumOfTaxCusts + 1
    GCustNum = NumOfTaxCusts + 1
  Else
    Get TCHandle, GCustNum, TaxCustRec
  End If
  
  TaxCustRec.OPENDATE = Date2Num(fptxtOpenDate.Text)
  TaxCustRec.CustName = QPTrim$(fptxtName.Text)
  TaxCustRec.SName = QPTrim$(fptxtSearchName.Text)
  TaxCustRec.CSSN = QPTrim$(ReplaceString(fptxtSocSec.Text, "-", ""))
  TaxCustRec.HPHONE = QPTrim$(fptxtHomePhone.Text)
  TaxCustRec.OSSN = QPTrim$(ReplaceString(fptxtOtherSSN.Text, "-", ""))
  TaxCustRec.WPHONE = QPTrim$(fptxtWorkPhone.Text)
  TaxCustRec.Addr1 = QPTrim$(fptxtAdd1.Text)
  TaxCustRec.Addr2 = QPTrim$(fptxtAdd2.Text)
  TaxCustRec.City = QPTrim$(fptxtCity.Text)
  TaxCustRec.State = QPTrim$(fptxtState.Text)
  ThisZip = QPTrim$(fptxtZip.Text)
  If Len(ThisZip) = 6 And Mid(ThisZip, 6, 1) = "-" Then
    ThisZip = Mid(ThisZip, 1, 5)
  End If
  TaxCustRec.Zip = ThisZip ' QPTrim$(fptxtZip.Text)
'  TaxCustRec.Zip = QPTrim$(fptxtZip.Text)
  TaxCustRec.Active = fpcmbActiveYN.Text
  TaxCustRec.TaxExempt = fpcmbTaxExYN.Text
  TaxCustRec.Interest = fpcmbChargeIntYN.Text
  TaxCustRec.Penalty = fpcmbChargePenYN.Text
  TaxCustRec.Bankrupt = fpcmbBankruptYN.Text
  TaxCustRec.LateNotice = fpcmbLateNoticeYN.Text
  TaxCustRec.TownShip = QPTrim$(fpcmbTownship.Text)
  TaxCustRec.OptSrchDesc = QPTrim$(fptxtOptSrchData.Text)
  TaxCustRec.Employer = QPTrim$(fptxtEmployer.Text)
  TaxCustRec.DrvrsLic = QPTrim$(fptxtDrvrsLic.Text)
  TaxCustRec.ServiceAdd = QPTrim$(fptxtServiceAdd.Text)
  TaxCustRec.DeliveryPt = QPTrim$(fptxtDelPnt.Text)
  TaxCustRec.PostalRt = QPTrim$(fptxtPostRt.Text)
  fpcmbCycle.Col = 0
  TaxCustRec.Cycle = CLng(fpcmbCycle.ColText)
  fpcmbCycle.Col = 1
  TaxCustRec.CycleName = QPTrim$(fpcmbCycle.ColText)
  fpcmbCounty.Col = 0
  TaxCustRec.County4BillNum = CLng(fpcmbCounty.ColText)
  fpcmbCounty.Col = 1
  TaxCustRec.County4BillName = QPTrim$(fpcmbCounty.ColText)
  If Exist(CntyEditFile) Then 'added 7/11/07
    TaxCustRec.CountyAcctString = QPTrim$(fptxtCountyNum.Text)
  End If
  
  Put TCHandle, GCustNum, TaxCustRec
  
  Close TCHandle
  
  frmVATaxSaveAnimation.Show
  frmVATaxSaveAnimation.L1.Left = 840
  DoEvents
  
  If AddFlag = True Or IdxName = True Then
    If Exist(CustNameIdxFile) Then
      Call CreateCustNameIdx1(TaxCustRec.CustName, GCustNum)
    Else
      Call CreateCustNameIdx
    End If
  End If
  
  If AddFlag = True Or IdxSrchName = True Then
    If Exist(SrchNameIdxFile) Then
      Call CreateSrchNameIdx1(TaxCustRec.SName, GCustNum)
    Else
      Call CreateSrchNameIdx
    End If
  End If

  If AddFlag = True Or IdxOpt = True Then
    If Exist(CustOptSearch) Then
      Call CreateOptCustIdx1(TaxCustRec.OptSrchDesc, GCustNum)
    Else
      Call CreateOptCustIdx
    End If
  End If
  
  frmVATaxSaveAnimation.Show
  frmVATaxSaveAnimation.L1.Left = 2820
  DoEvents
  If Val(ReplaceString(fptxtSocSec.Text, "-", "")) <> 0 Then
    If Exist(SocSecIdxFile) Then
      Call CreateSSIdx1(fptxtSocSec.Text, GCustNum)
    Else
      Call CreateSSIdx
    End If
    frmVATaxSaveAnimation.Show
    frmVATaxSaveAnimation.L1.Left = 2820
    DoEvents
  End If
  
  Call LogSaves
  
  Unload frmVATaxSaveAnimation
  
  Call Savemsg(900, "Your data has been saved and indexed successfully.")
  
  If AddFlag = True Or AddSaveFlag = True Then 'added AddSaveFlag on 5.24.07
    If TaxMsgWOpts(800, "Have you completed the addition of this customer? Press F10 if you are done with this customer. Otherwise, press ESC to continue further editing.", "F10 Finished", "ESC More Editing") = "abort" Then
      Unload frmVATaxMsgWOpts
      vaTabPro1.ActiveTab = 0
      fptxtOpenDate.SetFocus
      AddSaveFlag = True
      Exit Sub
    Else
      Unload frmVATaxMsgWOpts
      If TaxMsgWOpts(800, "Would you like to add another new customer? Press F10 to add another new customer. Otherwise, press ESC to exit to the menu.", "F10 Add New", "ESC Exit") = "abort" Then
        Unload frmVATaxMsgWOpts
        frmVATaxCustMaintMenu.Show
        DoEvents
        Unload Me
        Exit Sub 'added 5.24.07
      Else
        Unload frmVATaxMsgWOpts
        GCustNum = 0
        GPayNum = 0
        Call LoadAdd
        Exit Sub
      End If
    End If
  End If
    
  If AddFlag = False And IdxName = True Then
    ThisListIdx = frmVATaxCustLookup.fpList1.ListIndex
    frmVATaxCustLookup.fpList1.Clear
    Call frmVATaxCustLookup.cmdSearch_Click
    frmVATaxCustLookup.fpList1.ListIndex = ThisListIdx
  ElseIf AddFlag = False And ThisSSN = True Then
    ThisListIdx = frmVATaxCustLookup.fpList1.ListIndex
    frmVATaxCustLookup.fpList1.Clear
    Call frmVATaxCustLookup.cmdSearch_Click
    frmVATaxCustLookup.fpList1.ListIndex = ThisListIdx
  ElseIf AddFlag = False And ThisTown = True Then
    ThisListIdx = frmVATaxCustLookup.fpList1.ListIndex
    frmVATaxCustLookup.fpList1.Clear
    Call frmVATaxCustLookup.cmdSearch_Click
    frmVATaxCustLookup.fpList1.ListIndex = ThisListIdx
  End If
  
  frmVATaxCustLookup.cmdExit.Enabled = True
  Unload Me
End Sub

