Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%C"
      Call cmdExit_Click
      KeyCode = 0
    Case vbKeyF10:
      SendKeys "%S"
      Call cmdSave_Click
      KeyCode = 0
    Case vbKeyF3:
      SendKeys "%D"
      Call cmdDelete_Click
      KeyCode = 0
    Case vbKeyF4:
      SendKeys "%T"
      Call cmdTHist_Click
      KeyCode = 0
    Case vbKeyF6:
      SendKeys "%P"
      Call cmdPersProp_Click
      KeyCode = 0
    Case vbKeyF7:
      SendKeys "%R"
      Call cmdRealEst_Click
      KeyCode = 0
    Case vbKeyF8:
      SendKeys "%N"
      Call cmdNextTab_Click
      KeyCode = 0
    Case vbKeyF2:
      SendKeys "%M"
      Call cmdMessage_Click
      KeyCode = 0
    Case vbKeyPageUp:
      Call cmdNextTab_Click
      KeyCode = 0
    Case vbKeyPageDown:
      Call cmdNextTab_Click
      KeyCode = 0
    Case Else:
  End Select

End Sub

