Private Sub LoadMe()
  Dim TaxCustRec As TaxCustType
  Dim TCHandle As Integer
  Dim NumOfTaxCusts As Long
  Dim x As Long, y As Long
  Dim ThisSSN$
  Dim TaxMasterRec As TaxMasterType
  Dim TMHandle As Integer
  Dim OptSearch$
  Dim TSRec As TownshipType
  Dim TSCnt As Integer
  Dim TSHandle As Integer
  
  On Error GoTo ERRORSTUFF
  
  If Exist(CntyEditFile) Then 'added 7/11/07
    fptxtCountyNum.Enabled = True
    fptxtCountyNum.ControlType = ControlTypeNormal
  End If
1:
  If Exist(TaxSetupName) Then
    OpenTaxSetUpFile TMHandle
    Get TMHandle, 1, TaxMasterRec
    Close TMHandle
  End If
2:
  fpcmbCycle.InsertRow = CStr(0) + Chr(9) + "NONE"
  fpcmbCounty.InsertRow = CStr(0) + Chr(9) + "NONE"
  For x = 1 To 5
    If TaxMasterRec.CycleNum(x) > 0 Then
      fpcmbCycle.InsertRow = CStr(TaxMasterRec.CycleNum(x)) + Chr(9) + QPTrim$(TaxMasterRec.CycleName(x))
    End If
    If TaxMasterRec.CountyNum(x) > 0 Then
      fpcmbCounty.InsertRow = CStr(TaxMasterRec.CountyNum(x)) + Chr(9) + QPTrim$(TaxMasterRec.CountyName(x))
    End If
  Next x
  
  Active = False
  
  If Len(QPTrim$(TaxMasterRec.OptSrchCust)) > 0 Then
    OptSearch$ = QPTrim$(TaxMasterRec.OptSrchCust)
    fptxtOptSrchData.ControlType = ControlTypeNormal
  Else
    OptSearch$ = "NONE SAVED"
    fptxtOptSrchData.ControlType = ControlTypeReadOnly
  End If
3:
  fptxtSearchDesc.Text = OptSearch
  OpenTaxCustFile TCHandle, NumOfTaxCusts
4:
  If GCustNum = 0 Then
    Me.HelpContextID = hlpAddANew
    cmdMessage.Visible = False
    cmdTHist.Enabled = False
    cmdDelete.Enabled = False
    fptxtAcctNum.Text = CStr(NumOfTaxCusts + 1)
    fptxtCountyNum.Text = "N/A"
    fptxtOpenDate.Text = Date
    fpcmbActiveYN.Text = "Y"
    fpcmbTaxExYN.Text = "N"
    fpcmbChargeIntYN.Text = "Y"
    fpcmbChargePenYN.Text = "Y"
    fpcmbBankruptYN.Text = "N"
    fpcmbLateNoticeYN.Text = "Y"
    fpcmbCycle.SearchText = CStr(0) + Chr(9) + "NONE"
    fpcmbCycle.Action = 0
    If fpcmbCycle.SearchIndex <> -1 Then
      fpcmbCycle.ListIndex = fpcmbCycle.SearchIndex
    Else
      fpcmbCycle.ListIndex = 0
    End If
    TempCycle = TaxCustRec.Cycle
    
    fpcmbCounty.SearchText = CStr(0) + Chr(9) + "NONE"
    fpcmbCounty.Action = 0
    If fpcmbCounty.SearchIndex <> -1 Then
      fpcmbCounty.ListIndex = fpcmbCounty.SearchIndex
    Else
      fpcmbCounty.ListIndex = 0
    End If
    fpcmbCounty.Text = fpcmbCounty.Text
    TempCounty4BillNum = TaxCustRec.County4BillNum
    fptxtThisCust.Text = ""
    GoTo AddCust
  Else
5:
    Me.HelpContextID = hlpEditAnExisting
    ThisBal = GetCustBalance(GCustNum, -1)
    frmVATaxCustLookup.cmdExit.Enabled = False
    Get TCHandle, GCustNum, TaxCustRec
    If TaxCustRec.Active = "Y" Then
      Active = True
    End If
6:
    If CustHasMsg(GCustNum) Then
      MsgAlertTimer.Enabled = True
    Else
      MsgAlertTimer.Enabled = False
      cmdMessage.ForeColor = &H80000012
    End If
    fptxtAcctNum.Text = CStr(TaxCustRec.Acct)
    fptxtOpenDate.Text = MakeRegDate(TaxCustRec.OPENDATE)
    TempOPENDATE = TaxCustRec.OPENDATE
    fptxtName.Text = QPTrim$(TaxCustRec.CustName)
    If Active = True Then
      fptxtThisCust.Text = QPTrim$(fptxtName.Text) + "  * Balance: " + QPTrim$(Using("$###,##0.00", ThisBal)) + "  * Status: Active"
    Else
      fptxtThisCust.Text = QPTrim$(fptxtName.Text) + "  * Balance: " + QPTrim$(Using("$###,##0.00", ThisBal)) + "  * Status: Inactive"
    End If
    TempCustName = QPTrim$(TaxCustRec.CustName)
    fptxtSearchName.Text = QPTrim$(TaxCustRec.SName)
    fptxtDelPnt.Text = QPTrim$(TaxCustRec.DeliveryPt)
    TempDeliveryPt = QPTrim$(TaxCustRec.DeliveryPt)
    fptxtPostRt.Text = QPTrim$(TaxCustRec.PostalRt)
    TempSName = QPTrim$(TaxCustRec.SName)
    ThisSSN = TaxCustRec.CSSN
    TempCSSN = TaxCustRec.CSSN
    If QPTrim$(TaxCustRec.CSSN) = "" Then GoTo NoSSN
    Call InsertSSNDashes(ThisSSN)
    fptxtSocSec.Text = QPTrim$(ThisSSN)
NoSSN:
    fptxtHomePhone.Text = GetPhoneNum(TaxCustRec.HPHONE)
'    TempHPHONE = GetPhoneNum(TaxCustRec.HPHONE)
'    fptxtHomePhone.Text = QPTrim$(TaxCustRec.HPHONE)
    TempHPHONE = QPTrim$(TaxCustRec.HPHONE)
    ThisSSN = TaxCustRec.OSSN
    TempOSSN = TaxCustRec.OSSN
    If QPTrim$(TaxCustRec.OSSN) = "" Then GoTo NoSpouseSSN
    Call InsertSSNDashes(ThisSSN)
    fptxtOtherSSN.Text = ThisSSN
NoSpouseSSN:
    Call InsertSSNDashes(fptxtOtherSSN.Text)
    fptxtWorkPhone.Text = GetPhoneNum(TaxCustRec.WPHONE)
'    TempWPHONE = GetPhoneNum(TaxCustRec.WPHONE)
'    fptxtWorkPhone.Text = QPTrim$(TaxCustRec.WPHONE)
    TempWPHONE = QPTrim$(TaxCustRec.WPHONE)
    fptxtAdd1.Text = QPTrim$(TaxCustRec.Addr1)
    TempAddr1 = QPTrim$(TaxCustRec.Addr1)
    fptxtAdd2.Text = QPTrim$(TaxCustRec.Addr2)
    TempAddr2 = QPTrim$(TaxCustRec.Addr2)
    fptxtServiceAdd.Text = QPTrim$(TaxCustRec.ServiceAdd)
    TempServiceAdd = QPTrim$(TaxCustRec.ServiceAdd)
    fptxtDrvrsLic.Text = QPTrim$(TaxCustRec.DrvrsLic)
    If QPTrim$(TaxCustRec.CountyAcctString) <> "" Then
      fptxtCountyNum.Text = TaxCustRec.CountyAcctString
    ElseIf TaxCustRec.CountyAcct > 0 Then
      fptxtCountyNum.Text = CStr(TaxCustRec.CountyAcct)
    Else
      fptxtCountyNum.Text = "N/A"
    End If
    TempDrvrsLic = QPTrim$(TaxCustRec.DrvrsLic)
    fptxtCity.Text = QPTrim$(TaxCustRec.City)
    TempCity = QPTrim$(TaxCustRec.City)
