    TempTownShip = QPTrim$(TaxCustRec.TownShip)
    fptxtOptSrchData.Text = QPTrim$(TaxCustRec.OptSrchDesc)
    TempOptSrchDesc = QPTrim$(TaxCustRec.OptSrchDesc)
    fptxtEmployer.Text = QPTrim$(TaxCustRec.Employer)
