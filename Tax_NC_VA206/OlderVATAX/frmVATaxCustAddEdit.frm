    Else
      fpcmbCounty.ListIndex = 0
    End If
    TempCounty4BillNum = TaxCustRec.County4BillNum
    
  End If
  
  Close TCHandle
  
AddCust:
  fpcmbActiveYN.AddItem "Y"
  fpcmbActiveYN.AddItem "N"
  fpcmbTaxExYN.AddItem "Y"
  fpcmbTaxExYN.AddItem "N"
  fpcmbChargeIntYN.AddItem "Y"
  fpcmbChargeIntYN.AddItem "N"
  fpcmbChargePenYN.AddItem "Y"
  fpcmbChargePenYN.AddItem "N"
  fpcmbBankruptYN.AddItem "Y"
  fpcmbBankruptYN.AddItem "N"
  fpcmbLateNoticeYN.AddItem "Y"
  fpcmbLateNoticeYN.AddItem "N"
  
  fpcmbTownship.Clear
  If Exist(TaxTownships) Then
    OpenTownshipFile TSHandle, TSCnt
    fpcmbTownship.AddItem "NONE"
    For x = 1 To TSCnt
      Get TSHandle, x, TSRec
      fpcmbTownship.AddItem QPTrim$(TSRec.TownShip)
      If GCustNum = 0 Or QPTrim$(TaxCustRec.TownShip) = "" Then
        fpcmbTownship.Text = "NONE"
      Else
        fpcmbTownship.Text = QPTrim$(TaxCustRec.TownShip)
      End If
    Next x
  End If
  Close TSHandle
  
  
  Exit Sub
  
ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmVATaxCustAddEdit", "LoadMe", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close
    ClearInUse PWcnt
    Terminate

End Sub

Private Sub fpcmbActiveYN_Change()
  If fpcmbActiveYN.Text = "Y" Then
    fptxtThisCust.Text = QPTrim$(fptxtName.Text) + "  * Balance: " + QPTrim$(Using("$###,##0.00", ThisBal)) + "  * Status: Active"
    Active = True
  ElseIf fpcmbActiveYN.Text = "N" Then
    fptxtThisCust.Text = QPTrim$(fptxtName.Text) + "  * Balance: " + QPTrim$(Using("$###,##0.00", ThisBal)) + "  * Status: Inactive"
    Active = False
  End If
