Private Sub cmdDelete_Click()
  Dim TaxCustRec As TaxCustType
  Dim TCHandle As Integer
  Dim NumOfTaxCusts As Long
  Dim Msg1 As Boolean
  Dim Msg2 As Boolean
  Dim Msg3 As Boolean
  Dim OpNum$
  Dim BillType$
  
  On Error GoTo ERRORSTUFF
  
  If Check4CustInPayBatch(GCustNum, OpNum, BillType) = True Then
    If BillType = "P" Then
      Call TaxMsg(700, "This customer is included in a personal payment file for operator# " + OpNum + " that has not been posted. Please either post this payment or delete this customer from the payment file.")
    ElseIf BillType = "R" Then
      Call TaxMsg(700, "This customer is included in a real payment file for operator# " + OpNum + " that has not been posted. Please either post this payment or delete this customer from the payment file.")
    End If
    Exit Sub
  End If
  
  If GCustNum = 0 Then
    frmVATaxMsg.Label1.Caption = "Cannot delete an unsaved customer."
    frmVATaxMsg.Label1.Top = 900
    frmVATaxMsg.Show vbModal
    If vaTabPro1.ActiveTab = 0 Then
      fptxtOpenDate.SetFocus
    ElseIf vaTabPro1.ActiveTab = 1 Then
      fpcmbActiveYN.SetFocus
    End If
    Exit Sub
  End If
  
  Msg1 = False
  Msg2 = False
  Msg3 = False
  
  'other traps will need to be inserted here...ie. outstanding balance
  OpenTaxCustFile TCHandle, NumOfTaxCusts
  Get TCHandle, GCustNum, TaxCustRec
  If DoesCustOwe(TaxCustRec) = True Then
    Msg1 = True
  End If
  
  If TaxCustRec.FirstPersRec > 0 Then
    Msg2 = True
  End If
  
  If TaxCustRec.FirstPropRec > 0 Then
    Msg3 = True
  End If
  
  If Msg1 = True And Msg2 = True And Msg3 = True Then
    frmVATaxMsg.Label1.Caption = "ERROR: This customer has Personal and Real Estate property attached and has an outstanding balance and therefore cannot be deleted. Please resolve these issues and try again."
    frmVATaxMsg.Label1.Top = 700
    frmVATaxMsg.Show vbModal
    Close
    Exit Sub
  ElseIf Msg1 = True And Msg2 = True And Msg3 = False Then
    frmVATaxMsg.Label1.Caption = "ERROR: This customer has Personal Property attached and has an outstanding balance and therefore cannot be deleted. Please resolve these issues and try again."
    frmVATaxMsg.Label1.Top = 700
    frmVATaxMsg.Show vbModal
    Close
    Exit Sub
  ElseIf Msg1 = True And Msg2 = False And Msg3 = False Then
    frmVATaxMsg.Label1.Caption = "ERROR: This customer has an outstanding balance and therefore cannot be deleted. Please resolve this issue and try again."
    frmVATaxMsg.Label1.Top = 700
    frmVATaxMsg.Show vbModal
    Close
    Exit Sub
  ElseIf Msg1 = True And Msg2 = False And Msg3 = True Then
    frmVATaxMsg.Label1.Caption = "ERROR: This customer has Real Estate attached and has an outstanding balance and therefore cannot be deleted. Please resolve these issues and try again."
    frmVATaxMsg.Label1.Top = 700
    frmVATaxMsg.Show vbModal
    Close
    Exit Sub
  ElseIf Msg1 = False And Msg2 = True And Msg3 = True Then
    frmVATaxMsg.Label1.Caption = "ERROR: This customer has Real Estate and Personal Property attached and therefore cannot be deleted. Please resolve these issues and try again."
    frmVATaxMsg.Label1.Top = 700
    frmVATaxMsg.Show vbModal
    Close
    Exit Sub
  ElseIf Msg1 = False And Msg2 = True And Msg3 = False Then
    frmVATaxMsg.Label1.Caption = "ERROR: This customer has Personal Property attached and therefore cannot be deleted. Please resolve these issues and try again."
    frmVATaxMsg.Label1.Top = 700
    frmVATaxMsg.Show vbModal
    Close
    Exit Sub
  ElseIf Msg1 = False And Msg2 = False And Msg3 = True Then
    frmVATaxMsg.Label1.Caption = "ERROR: This customer has Real Estate attached and therefore cannot be deleted. Please resolve these issues and try again."
    frmVATaxMsg.Label1.Top = 700
    frmVATaxMsg.Show vbModal
    Close
    Exit Sub
  End If
    
  TaxCustRec.Deleted = -1
  Put TCHandle, GCustNum, TaxCustRec
  Close TCHandle
  
  Call TaxMsg(900, "This customer has been successfully deleted. When finished with deletions please reindex on the Tax System Setup And Utilities Menu.")
  MainLog ("Customer #/name " + Using$("######", GCustNum) + "/" + QPTrim$(TaxCustRec.CustName) + " was deleted.")
  frmVATaxCustLookup.cmdExit.Enabled = True
  Unload Me
  DoEvents
  
  Exit Sub
  
ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmVATaxCustAddEdit", "cmdDelete_Click", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close
  
End Sub

Private Sub cmdExit_Click()
  If GCustNum > 0 Then
    If Check4Changes = True Then
      Exit Sub
    End If
    If AddSaveFlag = True Then
      AddSaveFlag = False
      If TaxMsgWOpts(800, "Would you like to add another new customer? Press F10 if you would like to add another new customer. Otherwise, press ESC to exit to the menu.", "F10 ADD NEW", "ESC EXIT") = "abort" Then
        Unload frmVATaxMsgWOpts
        GCustNum = 0
        ThisBal = 0
        frmVATaxCustMaintMenu.Show
        DoEvents
        Unload Me
      Else
        Unload frmVATaxMsgWOpts
        GCustNum = 0
        GPayNum = 0
        ThisBal = 0
        Call LoadAdd
        Exit Sub
      End If
    End If
  Else
    frmVATaxMsgWOpts.Label1.Caption = "You are exiting without saving any data. If you wish to continue exiting without saving then press F10. If you wish to return to the screen to save this data then press ESC and you will be returned to the screen where you can press the save button to save your data."
    frmVATaxMsgWOpts.Label1.Top = 600
    frmVATaxMsgWOpts.cmdCont.Text = "F10 OK To Exit"
    frmVATaxMsgWOpts.cmdExit.Text = "ESC Abort Exit"
    frmVATaxMsgWOpts.Show vbModal
    If frmVATaxMsgWOpts.fptxtChoice.Text = "continue" Then
      Unload frmVATaxMsgWOpts
      GoTo ExitWOSaving
    Else
      Unload frmVATaxMsgWOpts
      vaTabPro1.ActiveTab = 0
      fptxtName.SetFocus
      Exit Sub
    End If
  End If
  
  GCustNum = 0
  ThisBal = 0
ExitWOSaving:
  frmVATaxCustLookup.cmdExit.Enabled = True
  
  Unload Me
  DoEvents
End Sub

