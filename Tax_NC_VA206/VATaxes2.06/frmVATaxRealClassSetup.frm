Attribute VB_Name = "frmVATaxRealClassSetup"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
  Dim ScrWidth As Long
  Dim Over As clsTextBoxOverRider
  Private Temp_Class As Resize_Class
  Dim TempClassName() As String
  Public ThisNumber As Long
  Public ThisDesc$
  Public NewDesc As String
  Public ThisClassCnt As Long

Private Sub cmdExit_Click()
  Unload Me
End Sub

Private Sub cmdSave_Click()
  Dim TaxSetupRec As TaxMasterType
  Dim TMHandle As Integer
  Dim x As Integer, y As Long, z As Integer, p As Long
  Dim ThisCounty As Integer
  Dim TaxCustRec As TaxCustType
  Dim TCHandle As Integer
  Dim NumOfTCRecs As Long
  Dim PrintLine$
  Dim PrintCnt As Integer
  Dim RealRec As PropertyRecType
  Dim RRHandle As Integer
  Dim NumOfRRREcs As Long
  Dim ThisRealDesc$
  
  On Error GoTo ERRORSTUFF
  ReDim ClassUsersName(1 To 1) As String
  ReDim ClassUsersAcct(1 To 1) As Long
  NewDesc = ""
  If Exist(TaxCustFile) And Exist(TaxPropName) Then
    For x = 4 To 9
      If QPTrim$(TempClassName(x - 3)) <> QPTrim$(fptxtRealClass(x).Text) Then
        ThisDesc$ = QPTrim$(TempClassName(x - 3))
        If ThisDesc$ = "" Then GoTo NextOne
        GoSub PrintClassUsers
        If ThisClassCnt > 0 Then
          NewDesc = QPTrim$(fptxtRealClass(x).Text)
          frmVATaxPrintClassUsers.Show vbModal
          If frmVATaxPrintClassUsers.fptxtAnswer.Text = "convert" Then
            Unload frmVATaxPrintClassUsers
            GoTo NextOne
          Else
            Call TaxMsg(900, "Class Description " + QPTrim$(fptxtRealClass(x).Text) + " not saved.")
            Unload frmVATaxPrintClassUsers
            fptxtRealClass(x) = TempClassName(x - 3)
            If PrintCnt = 0 Then
              PrintLine = PrintLine + TempClassName(x - 3)
            Else
              PrintLine = PrintLine + ", " + TempClassName(x - 3)
            End If
          End If
        End If
      End If
NextOne:
    Next x
  End If
  
  OpenTaxSetUpFile TMHandle
  Get TMHandle, 1, TaxSetupRec
  For x = 4 To 9
    TaxSetupRec.ClassName(x - 3) = QPTrim$(fptxtRealClass(x).Text)
  Next x
  Put TMHandle, 1, TaxSetupRec
  Close TMHandle
  
  If QPTrim$(PrintLine) <> "" Then
    Call Savemsg(800, "Tax real property classifications have been saved successfully. " + PrintLine + " remained unchanged.")
  Else
    Call Savemsg(900, "Tax real property classifications have been saved successfully.")
  End If
  
  Unload Me
  Exit Sub
  
PrintClassUsers:
  OpenTaxPropFile RRHandle, NumOfRRREcs
  OpenTaxCustFile TCHandle, NumOfTCRecs
  For y = 1 To NumOfTCRecs
    Get RRHandle, y, RealRec
    ThisRealDesc = QPTrim$(RealRec.ICPDesc)
    If ThisRealDesc = "" Then GoTo NoNo
    If ThisRealDesc = ThisDesc Then
      If RealRec.CustPin > 0 Then
        Get TCHandle, RealRec.CustPin, TaxCustRec
        ThisClassCnt = ThisClassCnt + 1
        ReDim Preserve ClassUsersName(1 To ThisClassCnt) As String
        ClassUsersName(ThisClassCnt) = QPTrim$(TaxCustRec.CustName)
        ReDim Preserve ClassUsersAcct(1 To ThisClassCnt) As Long
        ClassUsersAcct(ThisClassCnt) = TaxCustRec.Acct
        ReDim Preserve ClassRealPin(1 To ThisClassCnt) As String
        ClassRealPin(ThisClassCnt) = QPTrim$(RealRec.RealPin)
      End If
    End If
NoNo:
  Next y
  Close RRHandle
  Close TCHandle
      
  Return
  
ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmVATaxCountySetup", "cmdSave_Click", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close

End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%C"
      Call cmdExit_Click
      KeyCode = 0
    Case vbKeyF10:
      SendKeys "%O"
      Call cmdSave_Click
      KeyCode = 0
    Case Else:
  End Select

End Sub

Private Sub Form_Load()
  Dim TaxSetupRec As TaxMasterType
  Dim TMHandle As Integer
  Dim x As Integer
  
  ReDim TempClassName(1 To 6) As String
  NewDesc = ""
  If Exist(TaxSetupName) Then
    OpenTaxSetUpFile TMHandle
    Get TMHandle, 1, TaxSetupRec
    For x = 4 To 9
      fptxtRealClass(x).Text = QPTrim$(TaxSetupRec.ClassName(x - 3))
      TempClassName(x - 3) = QPTrim$(TaxSetupRec.ClassName(x - 3))
    Next x
  Else
    For x = 4 To 9
      fptxtRealClass(x).Text = ""
      TempClassName(x - 3) = ""
    Next x
  End If
  Close TMHandle
  Me.HelpContextID = hlpRealPropClass
 End Sub
 
Private Function Check4DupDescs(ThisName$, ThisRec As Integer) As Boolean
  Dim x As Integer
  
  On Error GoTo ERRORSTUFF
  
  Check4DupDescs = False
  If ThisName = "" Then Exit Function
  For x = 4 To 9
    If x <> ThisRec Then
      If QPTrim$(fptxtRealClass(x).Text) = ThisName Then
        Call TaxMsg(900, "The name entered on line " + CStr(ThisRec + 1) + " is already being used. Please select a unique description.")
        Check4DupDescs = True
        Exit Function
      End If
    End If
  Next x
  Exit Function
  
ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmVATaxCountySetup", "Check4DupDescs", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close
  
End Function

Private Sub fptxtRealClass_LostFocus(Index As Integer)
  If Index < 4 Then Exit Sub
  If TempClassName(Index - 3) <> QPTrim$(fptxtRealClass(Index)) Then
    If Check4DupDescs(QPTrim$(fptxtRealClass(Index)), Index) = True Then
      fptxtRealClass(Index).SetFocus
      Exit Sub
    End If
  End If
End Sub


