Private Sub fptxtBody_KeyDown(Index As Integer, KeyCode As Integer, Shift As Integer)
 Dim x As Integer
 
 'on error goto ERRORSTUFF
 
 x = Index
  If KeyCode = vbKeyInsert Then
    If Edit = False Then
      Edit = True
      LabelInsert.Visible = True
    Else
      Edit = False
      LabelInsert.Visible = False
    End If
  End If
  If KeyCode = vbKeyReturn Or KeyCode = vbKeyDown Then
    If Index < 19 Then
     For x = Index To 18
      If fptxtBody(x + 1).Enabled Then
        fptxtBody(x + 1).SetFocus
        Exit For
      End If
     Next
    End If
    If Index = 19 Then
       cmdSave.SetFocus
    End If
  ElseIf KeyCode = vbKeyUp Then
    If Index > 0 Then
     For x = Index To 19
      If fptxtBody(x - 1).Enabled Then
        fptxtBody(x - 1).SetFocus
        Exit For
      End If
     Next
    End If
    If Index = 0 Then
      fpText5.SetFocus
    End If
  End If
  
  Exit Sub
  
ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmTaxLateNoticeLtr", "fptxtBody_Keydown", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
