Private Sub LoadMe()
  Dim LLHandle As Integer
  Dim Cnt As Integer
  Dim LLRec As TAXLateLetterType
  Dim TaxMasterRec As TaxMasterType
  Dim TMHandle As Integer
  
  OpenTaxSetUpFile TMHandle
  Get TMHandle, 1, TaxMasterRec
  Close
  TownName = QPTrim$(TaxMasterRec.Name)
  If Exist("TXLATLTR.DAT") Then
    OpenLateLtrFile LLHandle
    Get LLHandle, 1, LLRec
    fpText1.Text = RTrim(LLRec.Head1)
    fpText2.Text = RTrim(LLRec.Head2)
    fpText3.Text = RTrim(LLRec.Head3)
    fpText4.Text = RTrim(LLRec.Head4)
    fpText5.Text = RTrim(LLRec.Head5)
    For Cnt = 0 To 19
      fptxtBody(Cnt) = RTrim(LLRec.Body(Cnt + 1))
    Next
    Close
  Else
    fpText1 = TownName$
    fpText2 = ""
    fpText3 = ""
    fpText4 = QPTrim$(TaxMasterRec.City) + ", " + QPTrim$(TaxMasterRec.TaxSt) + "  " + QPTrim$(TaxMasterRec.Zip)
    fpText5 = ""
  End If
End Sub
Private Sub LateLetterSample(Grpt As Boolean)
  Dim fmt1 As String, fmt2 As String
  Dim ReportFile As String
  Dim LLHandle As Integer
  Dim LLRec As TAXLateLetterType
  Dim Cnt As Integer
  Dim TaxRpt As Integer, DeDate As String
  Dim MaxLines As Integer
  Dim FF$
  
  'on error goto ERRORSTUFF
  
  FF$ = Chr(12)
  If Exist("TXLATLTR.DAT") Then
    OpenLateLtrFile LLHandle
    Get LLHandle, 1, LLRec
    Close
  End If
    
  If Grpt Then
    MaxLines = 48
  Else
    MaxLines = 53
