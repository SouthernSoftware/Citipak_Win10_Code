Private Sub cmdExit_Click()
  Dim LLHandle As Integer
  Dim lenlate As Integer
  Dim Cnt As Integer
  Dim Chg As Integer
  Dim LLRec As TAXLateLetterType
  
  'on error goto ERRORSTUFF
  
  LLHandle = FreeFile
  Chg = 0
  If Exist("TXLATLTR.DAT") Then
    OpenLateLtrFile LLHandle
    Get LLHandle, 1, LLRec
    If Not RTrim(LLRec.Head1) = RTrim(fpText1.Text) Then Chg = Chg + 1
    If Not RTrim(LLRec.Head2) = RTrim(fpText2.Text) Then Chg = Chg + 1
    If Not RTrim(LLRec.Head3) = RTrim(fpText3.Text) Then Chg = Chg + 1
    If Not RTrim(LLRec.Head4) = RTrim(fpText4.Text) Then Chg = Chg + 1
    If Not RTrim(LLRec.Head5) = RTrim(fpText5.Text) Then Chg = Chg + 1
    For Cnt = 0 To 19
      If Not RTrim(LLRec.Body(Cnt + 1)) = RTrim(fptxtBody(Cnt)) Then Chg = Chg + 1
    Next
    Close
  Else
    If Not RTrim(fpText1.Text) = "" Then Chg = Chg + 1
    If Not RTrim(fpText2.Text) = "" Then Chg = Chg + 1
    If Not RTrim(fpText3.Text) = "" Then Chg = Chg + 1
    If Not RTrim(fpText4.Text) = "" Then Chg = Chg + 1
    If Not RTrim(fpText5.Text) = "" Then Chg = Chg + 1
    For Cnt = 0 To 19
      If Not RTrim(fptxtBody(Cnt)) = "" Then Chg = Chg + 1
    Next
  End If
  If Chg > 0 Then
    If TaxMsgWOpts(900, "Would you like to save changes before you exit?", "F10 Save", "ESC Exit") = "abort" Then
      Unload frmTaxMsgWOpts
    Else
      Call cmdSave_Click
    End If
  End If
  Unload Me
  
  Exit Sub
  
ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmTaxLatNoticeLtr", "cmdExit_Click", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    Close
    ClearInUse PWcnt
    Terminate
  
  
End Sub

