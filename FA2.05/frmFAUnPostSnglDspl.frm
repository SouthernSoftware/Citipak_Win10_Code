Attribute VB_Name = "frmFAUnPostSnglDspl"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
  Dim ScrWidth As Long
  Dim Over As clsFATextBoxOverRider
  Private Temp_Class As Resize_Class

Private Sub cmdExit_Click()
  frmFAReverseDspl.Show
  Close
  DoEvents
  Unload frmFAUnPostSnglDspl
End Sub

Private Sub cmdReverse_Click()
  If QPTrim$(fpcmbDepYN.Text) = "" Then
    MsgBox "Please enter a selection for Depreciate?."
    fpcmbDepYN.SetFocus
    Exit Sub
  End If

  If QPTrim$(fpcmbStatus.Text) = "" Then
    MsgBox "Please enter a selection for Status."
    fpcmbStatus.SetFocus
    Exit Sub
  End If
  
  Call ReverseThisData
End Sub

Private Sub ReverseThisData()
  Dim FAItemRec As FAItemRecType
  Dim FAHandle As Integer
  Dim CurrValue As Double
  Dim NmlDprAmt As Double
  Dim LifeLeft As Double
  
  On Error GoTo ERRORSTUFF
  'we have the record number for the asset to be undisposed
  'from frmFAReverseDspl so we find it in the records and
  'reassign the relevant fields
  
  OpenFAItemFile FAHandle
  Get FAHandle, GRecNum, FAItemRec
    GoSub GetLifeLeft 'refigure the asset life
    FAItemRec.LifeLeft = LifeLeft
    FAItemRec.DEPYN = fpcmbDepYN.Text
    If QPTrim$(fpcmbStatus.Text) = "Inactive" Then
      FAItemRec.ISTATUS = "I" 'do not depreciate this asset
    Else
      FAItemRec.ISTATUS = "A" 'activate for depreciation
    End If
    FAItemRec.DsplFlag = 0 'this seals it
    FAItemRec.CURRVAL = OldRound(FAItemRec.ORGCOST - FAItemRec.DEP2DATE) 'redo current value
    FAItemRec.DispDate = 0 'delete disposal date
    FAItemRec.DisposAmt = 0 'delete disposal amount
    FAItemRec.DsplMethod = "NA" 'reset disposal method
  Put FAHandle, GRecNum, FAItemRec
  MainLog ("Disposal reversal for tag number " + fptxtTagNumber.Text + " processed in frmFAUnPostSnglDspl.")
  Close FAHandle
  MsgBox "This item disposal reversal has been posted."
  'if the user wants to reverse other disposed of assets he can
  'begin the process again without exiting to the menu
  If MsgBox("Do you wish to process another item for disposal reversal?", vbYesNo) = vbYes Then
    frmFAReverseDspl.Show
    DoEvents
    Unload frmFAUnPostSnglDspl
  Else
    frmFADisposalMenu.Show
    DoEvents
    Unload frmFAUnPostSnglDspl
  End If

  Exit Sub
  
GetLifeLeft:
  'life left is determined by getting the current value and dividing that
  'amount by the depreciation amount for a normal year...then this figure
  'is rounded
  
  If FAItemRec.ILIFE = 0 Then GoTo LifeIsZero
  CurrValue = OldRound(FAItemRec.ORGCOST) - OldRound(FAItemRec.DEP2DATE) 'current value
  NmlDprAmt = OldRound(FAItemRec.ORGCOST / FAItemRec.ILIFE) 'normal yearly depreciation
  
  If CurrValue = 0 Then
    LifeLeft = 0
    GoTo LifeIsZero
  Else
    LifeLeft = CurrValue / NmlDprAmt
    LifeLeft = OldRound(LifeLeft)
    LifeLeft = OldRound(LifeLeft * 100) 'looking for uneven amounts
    Remain = Right(CStr(LifeLeft), 2) 'need remainder, if any
    'if the remainder is very small then go ahead and assign life left
    'with the nearest whole amount
    If Val(Remain) = 0 Or Val(Remain) = 98 Or Val(Remain) = 99 Then
      LifeLeft = CurrValue / NmlDprAmt
      LifeLeft = CInt(LifeLeft)
    ElseIf Val(Remain) = 50 Then 'the remainder is exactly half
      LifeLeft = OldRound(CurrValue / NmlDprAmt)
      LifeLeft = OldRound(LifeLeft + 0.5) 'round up to next whole number
    Else
      LifeLeft = OldRound(LifeLeft / 100) 'not a whole number but must
      'be rounded either up or down
      Remain = Right(CStr(LifeLeft), 2) 'read the last two digits
      'from LifeLeft
      If InStr(1, Remain, ".") Then Remain = Remain * 100 'get the
      'whole number of the remainder if it is a percentage
      If Val(Remain) < 50 Then 'round up even if this value is less than
      '50 because any amount means that the asset will have to be
      'depreciated to bring this amount to zero in the last depreciation year
        LifeLeft = CurrValue / NmlDprAmt
        LifeLeft = CInt(LifeLeft) + 1
      Else '
        LifeLeft = CurrValue / NmlDprAmt 'else let CInt round it up
        LifeLeft = CInt(LifeLeft)
      End If
    End If
  End If
LifeIsZero:
  Return
  
ERRORSTUFF:
   Select Case ErrorMessage(Err.Number, Err.Description, Err.Source, "frmFAUnPostSnglDspl", "ReverseThisData", Erl)
     Case emrExitProc:
       Resume Proc_Exit
     Case emrResume:
       Resume
     Case emrResumeNext:
       Resume Next
     Case Else
      '--- Technically, this should never happen.
       Resume Proc_Exit
   End Select
  
Proc_Exit:
  '--- Cleanup code goes here...
    ClearInUse (PWcnt)
    Terminate
    Unload Me
End Sub

Private Sub Form_Load()
  ScrWidth = Screen.Width / Screen.TwipsPerPixelX
  Set Over = New clsFATextBoxOverRider
  Over.OverRide Me
  Set Temp_Class = New Resize_Class
  Temp_Class.InitResizeClass Me
  ScreenW = (Screen.Width / Screen.TwipsPerPixelX)
  Call LoadMe
End Sub

Private Sub Form_Resize()
  If Me.WindowState <> vbMinimized Then
'    'Me.Visible = False
    Temp_Class.ResizeControls Me
    Me.Visible = True
    Me.SetFocus
  End If
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%C"
      Call cmdExit_Click
      KeyCode = 0
    Case vbKeyF10:
      SendKeys "%R"
      Call cmdReverse_Click
      KeyCode = 0
    Case Else:
  End Select
End Sub

Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    If cmdExit.Enabled = False Then
      Cancel = True
    ElseIf MsgBox("Are You Sure You Wish To Close Program?", vbYesNo, "Close?") = vbNo Then
      Cancel = True
    Else
      ClearInUse PWcnt
      MainLog ("FixedAssets.exe terminated via menu bar on frmFAUNPostSnglDspl.")
      Call Terminate
      End
    End If
  End If
End Sub

Private Sub LoadMe()
  Dim FAItemRec As FAItemRecType
  Dim FAHandle As Integer
  
  OpenFAItemFile FAHandle
  Get FAHandle, GRecNum, FAItemRec
  Close FAHandle
  
  fpcmbDepYN.Clear
  fpcmbDepYN.AddItem "Y"
  fpcmbDepYN.AddItem "N"
  
  fpcmbStatus.Clear
  fpcmbStatus.AddItem "Active"
  fpcmbStatus.AddItem "Inactive"
  
  fptxtTagNumber.Text = QPTrim$(FAItemRec.ItemTag)
  fptxtDesc.Text = QPTrim$(FAItemRec.IDESC1)
  fptxtSerialNum.Text = QPTrim$(FAItemRec.SERIALNO)
  fptxtAcquiredDate = MakeRegDate(FAItemRec.AQURDATE)
  fptxtOriginalCost = FAItemRec.ORGCOST
  fptxtCurrVal = FAItemRec.CURRVAL
  fpcmbDepYN.Text = FAItemRec.DEPYN
  If QPTrim$(FAItemRec.ISTATUS) = "I" Then
    fpcmbStatus.Text = "Inactive"
  Else
    fpcmbStatus.Text = "Active"
  End If
  If MakeRegDate(FAItemRec.DispDate) <> "12/31/1979" Then
    fptxtDisposalDate = MakeRegDate(FAItemRec.DispDate)
  Else
    fptxtDisposalDate.Text = Date
  End If
  
  If FAItemRec.DisposAmt > 0 Then
    fptxtDispPrice = FAItemRec.DisposAmt
  Else
    fptxtDispPrice = 0
  End If
End Sub

Private Sub fpcmbDepYN_KeyDown(KeyCode As Integer, Shift As Integer)
  'this prevents the user from inadvertently changing data in the combo box when
  'tabbing through the fields
  If KeyCode = vbKeySpace Then
    fpcmbDepYN.ListDown = True
  End If
  If KeyCode = vbKeyDelete Then
    fpcmbDepYN.ListIndex = -1
  End If
  If fpcmbDepYN.ListDown <> True Then
    If KeyCode = vbKeyDown Then
      SendKeys "{Tab}"
      KeyCode = 0
    Else
      If KeyCode = vbKeyUp Then
        SendKeys "+{Tab}"
        KeyCode = 0
      End If
    End If
  End If

End Sub

Private Sub fpcmbStatus_Change()
  'this field defaults to Active
  If QPTrim$(fpcmbStatus.Text) = "" Then fpcmbStatus.Text = "Active"
End Sub

