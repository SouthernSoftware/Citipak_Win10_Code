Attribute VB_Name = "frmFASystemSetup"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
  Dim ScrWidth As Long
  Dim Over As clsFATextBoxOverRider
  Private Temp_Class As Resize_Class

Private Sub cmdExit_Click()
  frmFAMainMenu.Show
  DoEvents
  Unload frmFASystemSetup
End Sub

Private Sub cmdSave_Click()
  Dim FASetup As FASetupRecType
  Dim SetUpHandle As Integer
  
  FASetup.TownName = QPTrim$(fptxtTownName)
  FASetup.Pct1St = Val(fptxtFirstYearPct)
  FASetup.PRate1St = QPTrim$(fpcmbProrate.Text)

  OpenFASetUpFile SetUpHandle
  Put SetUpHandle, 1, FASetup
  Close SetUpHandle
  MsgBox "Your information has been saved"
  frmFAMainMenu.Show
  DoEvents
  Unload frmFASystemSetup
  
End Sub

Private Sub Form_Load()
  ScrWidth = Screen.Width / Screen.TwipsPerPixelX
  Set Over = New clsFATextBoxOverRider
  Over.OverRide Me
  Set Temp_Class = New Resize_Class
  Temp_Class.InitResizeClass Me
  ScreenW = (Screen.Width / Screen.TwipsPerPixelX)
  fpcmbProrate.Text = "Y"
  fpcmbProrate.AddItem "Y"
  fpcmbProrate.AddItem "N"
  Call LoadMe
End Sub

Private Sub Form_Resize()
  If Me.WindowState <> vbMinimized Then
    Me.Visible = False
    Temp_Class.ResizeControls Me
    Me.Visible = True
    Me.SetFocus
  End If
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%C"
      KeyCode = 0
    Case vbKeyF10:
      SendKeys "%S"
      KeyCode = 0
    Case Else:
  End Select
End Sub

Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  If ((UnloadMode = vbFormControlMenu)) Then
    If cmdExit.Enabled = False Then
      Cancel = True
    ElseIf MsgBox("Are You Sure You Wish To Close Program?", vbYesNo, "Close?") = vbNo Then
      Cancel = True
    Else
      Call UnloadAllFormsAndOpn
'      ClearInUse PWcnt
      MainLog ("FixedAssets.exe terminated via menu bar on frmPayrollMainMenu.")
      End
    End If
  End If
End Sub

Private Sub LoadMe()

  Dim FASetup As FASetupRecType
  Dim SetUpHandle As Integer

  If Exist("FASETUP.DAT") Then
    OpenFASetUpFile SetUpHandle
    Get SetUpHandle, 1, FASetup
    Close SetUpHandle
    fptxtTownName = QPTrim$(FASetup.TownName)
    fptxtFirstYearPct = FASetup.Pct1St
    fpcmbProrate.Text = QPTrim$(FASetup.PRate1St)
  End If

End Sub

Private Sub fpcmbProrate_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeySpace Then
    fpcmbProrate.ListDown = True
  End If
  If KeyCode = vbKeyDelete Then
    fpcmbProrate.ListIndex = -1
  End If
  If fpcmbProrate.ListDown <> True Then
    If KeyCode = vbKeyDown Then
      SendKeys "{Tab}"
      KeyCode = 0
    Else
      If KeyCode = vbKeyUp Then
        SendKeys "+{Tab}"
        KeyCode = 0
      End If
    End If
  End If

End Sub

Private Sub fpcmbProrate_LostFocus()
  fpcmbProrate.Action = ActionClearSearchBuffer

End Sub
Private Sub Form_KeyPress(KeyAscii As Integer)
'  this causes all characters to be capitalized
   KeyAscii = Asc(UCase$(Chr$(KeyAscii)))

End Sub

