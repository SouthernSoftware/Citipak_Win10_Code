TYPE DistType
    DACN    AS STRING * 16
    DACNM   AS STRING * 20
    DACREC  AS INTEGER
    DAMT    AS DOUBLE
END TYPE

TYPE APInvType
    DELFLAG  AS INTEGER
    VENDOR   AS STRING * 10
    VendName AS STRING * 20
    PONUM    AS STRING * 20
    INVNUM   AS STRING * 25
    INVAMT   AS DOUBLE
    PAYCODE  AS STRING * 1
    INVDATE  AS INTEGER
    DUEDATE  AS INTEGER
    DISTDATE AS INTEGER
    Dist(1 TO 36) AS DistType
END TYPE

