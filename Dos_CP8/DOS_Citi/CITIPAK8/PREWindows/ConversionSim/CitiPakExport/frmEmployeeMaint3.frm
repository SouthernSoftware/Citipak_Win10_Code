Option Explicit
Dim Over As clsTextBoxOverRider
Private Temp_Class As Resize_Class
Dim newEmpFlag As Boolean
Dim thisRecordNum As Integer
'the following variables are used in comparing the loaded
'times earned/used with any changes made so balances
'can be updated
Dim tempVacEarned As Double, tempVacUsed As Double
