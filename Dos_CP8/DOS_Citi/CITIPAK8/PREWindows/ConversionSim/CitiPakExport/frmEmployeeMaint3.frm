Private Sub fptxtCTEarned_LostFocus()
   'this routine checks changes in Comp Time Earned so
   'the Comp Time Balance can be automatically updated
   If Val(fptxtCTEarned.Text) <> tempCTEarned Then
      fptxtCTBalance.Text = Val(fptxtCTEarned.Text) - Val(fptxtCTUsed.Text)
      tempCTEarned = Val(fptxtCTEarned.Text) 'updates changes
   End If
