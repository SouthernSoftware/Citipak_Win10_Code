    Call SaveEmpInfo(newEmpFlag, thisRecordNum, frmEmployeeMaint, frmEmployeeMaint2, Me)
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  Select Case KeyCode
    Case vbKeyDown, vbKeyReturn:
      SendKeys "{Tab}"
      KeyCode = 0
    Case vbKeyEscape:
      SendKeys "%X"
      KeyCode = 0
    Case vbKeyF10:
      SendKeys "%S"
      KeyCode = 0
    Case Else:
  End Select

End Sub

Private Sub Form_Load()
  Dim cnt As Integer
  Dim ScrWidth As Long
  ScrWidth = Screen.Width / Screen.TwipsPerPixelX
  Set Over = New clsTextBoxOverRider
  Over.OverRide Me
  Set Temp_Class = New Resize_Class
  Temp_Class.InitResizeClass Me
  Select Case ScrWidth
  Case 1280
    For cnt = 1 To 6
      vaspreadDEC.ColWidth(cnt) = vaspreadDEC.ColWidth(cnt) + 8
      vaspreadWD.ColWidth(cnt) = vaspreadWD.ColWidth(cnt) + 10
    Next
  Case 1024
    For cnt = 1 To 6
      vaspreadDEC.ColWidth(cnt) = vaspreadDEC.ColWidth(cnt) + 1.45
      vaspreadWD.ColWidth(cnt) = vaspreadWD.ColWidth(cnt) + 2.15
    Next
  Case 800
    For cnt = 1 To 6
      vaspreadDEC.ColWidth(cnt) = vaspreadDEC.ColWidth(cnt) + 0.85
      vaspreadWD.ColWidth(cnt) = vaspreadWD.ColWidth(cnt) + 1.55
    Next
  Case Else
  End Select
  
  LoadEmpMnt3File
End Sub

Private Sub Form_Resize()
  If Me.WindowState <> vbMinimized Then
    Me.Visible = False
    Temp_Class.ResizeControls Me
    Me.Visible = True
    Me.SetFocus
  End If

End Sub

Private Sub LoadEmpMnt3File()
'   On Error Resume .
   Dim EmpData2FileHandle As Integer, x As Integer, y As Integer
   Dim EmpData2FileRec As EmpData2Type
   Dim ErnCodeFileHandle As Integer, ECCnt As Integer
   Dim ErnCodeRecLen As ErnCodeRecType
   Dim LeaveHandle As Integer
   Dim LeaveRec As LeaveRecType
   Dim LeaveNum As Integer
   
   OpenLeaveFileName LeaveHandle
   LeaveNum = LOF(LeaveHandle) \ Len(LeaveRec)
   Close LeaveHandle
   'find out how many leave tables exist and load
   'the leave table combo box with that many options
   For x = 1 To LeaveNum
     fpcomboLeaveT.AddItem (x)
   Next x
   
   fpcomboEXOnESC.AddItem ("Y")
   fpcomboEXOnESC.AddItem ("N")
   
   OpenErnCodeFile ErnCodeFileHandle
   ' fields in the default earnings code spreadsheet are always
   ' loaded when this form is loaded
   For x = 1 To 3
     Get ErnCodeFileHandle, x, ErnCodeRecLen
     vaspreadDEC.Col = 1
     vaspreadDEC.Row = x
     vaspreadDEC.Text = QPTrim$(ErnCodeRecLen.ERNCODE1)
   Next x
   Close ErnCodeFileHandle
   
   If frmEmployeeMaintMenu.Selection = neoOn Then
      newEmpFlag = True
   Else
       thisRecordNum = RecNum
       newEmpFlag = False
   End If
   
   If newEmpFlag = False Then
      OpenEmpData2File EmpData2FileHandle
      Get EmpData2FileHandle, thisRecordNum, EmpData2FileRec
      Close EmpData2FileHandle
      
      vaspreadDEC.Col = 2
      vaspreadDEC.Row = 1
      vaspreadDEC.Text = EmpData2FileRec.EMPEACT1
      vaspreadDEC.Col = 3
      vaspreadDEC.Row = 1
      vaspreadDEC.Text = EmpData2FileRec.EMPEAMT1
     
      vaspreadDEC.Col = 2
      vaspreadDEC.Row = 2
      vaspreadDEC.Text = EmpData2FileRec.EMPEACT2
      vaspreadDEC.Col = 3
      vaspreadDEC.Row = 2
      vaspreadDEC.Text = EmpData2FileRec.EMPEAMT2
      
      vaspreadDEC.Col = 2
      vaspreadDEC.Row = 3
      vaspreadDEC.Text = EmpData2FileRec.EMPEACT3
      vaspreadDEC.Col = 3
      vaspreadDEC.Row = 3
      vaspreadDEC.Text = EmpData2FileRec.EMPEAMT3
'      EmpData2FileRec.EMPHP = ""
      For x = 1 To 8
         vaspreadWD.Col = 1
         vaspreadWD.Row = x
         If Len(QPTrim(EmpData2FileRec.EDist(x).DAcct)) <= 0 Then
            Exit For
         End If
         vaspreadWD.Text = QPTrim$(EmpData2FileRec.EDist(x).DAcct)
         vaspreadWD.Col = 2
         vaspreadWD.Row = x
         vaspreadWD.Text = EmpData2FileRec.EDist(x).DAmt
      Next x
      fptxtVacEarned.Text = Val(EmpData2FileRec.EMPVACE)
      tempVacEarned = EmpData2FileRec.EMPVACE
      fptxtVacUsed.Text = Val(EmpData2FileRec.EMPVUSED)
      tempVacUsed = Val(EmpData2FileRec.EMPVBAL)
      fptxtVacBal.Text = Val(EmpData2FileRec.EMPVBAL)
      fpTxtSLEarned.Text = Val(EmpData2FileRec.EMPSLE)       'num of allowance
      tempSLEarned = EmpData2FileRec.EMPSLE
      fptxtSLUsed.Text = Val(EmpData2FileRec.EMPSLUSE)
      tempSLUsed = EmpData2FileRec.EMPSLUSE
      fptxtSLBalance.Text = Val(EmpData2FileRec.EMPSLBAL)
      fptxtCTEarned.Text = Val(EmpData2FileRec.EMPCTE)
      tempCTEarned = EmpData2FileRec.EMPCTE
      fptxtCTUsed.Text = Val(EmpData2FileRec.EMPCTUSE)
      tempCTUsed = EmpData2FileRec.EMPCTUSE
      fptxtCTBalance.Text = Val(EmpData2FileRec.EMPCTBAL)
      fpTxtPersEarned.Text = Val(EmpData2FileRec.PERERN)
      tempPTEarned = EmpData2FileRec.PERERN
      fptxtPersUsed.Text = Val(EmpData2FileRec.PERUSED)
      tempPTUsed = EmpData2FileRec.PERUSED
      fptxtPersBalance.Text = Val(EmpData2FileRec.PERBAL)
      fptxtHolEarned.Text = Val(EmpData2FileRec.HOLERN)
      tempHolEarned = EmpData2FileRec.HOLERN
      fptxtHolUsed.Text = Val(EmpData2FileRec.HOLUSED)
      tempHolUsed = EmpData2FileRec.HOLUSED
      fptxtHolBalance.Text = Val(EmpData2FileRec.HOLBAL)
      fpcomboLeaveT.Text = Val(EmpData2FileRec.LeaveTbl)
      fpcomboEXOnESC.Text = QPTrim$(EmpData2FileRec.ExcludeESC)
      
    End If
End Sub
